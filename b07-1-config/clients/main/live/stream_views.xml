	<!-- D1c Cam 1 -->
	<bean id="d1c1_roi" class="uk.ac.gda.remoting.client.GdaRmiProxyFactoryBean">
		<property name="serviceUrl" value="${gda.rmi.prefix}d1c1Roi" />
		<property name="serviceInterface" value="gda.device.detector.nxdetector.roi.RemoteRectangularROIsProvider" />
		<property name="refreshStubOnConnectFailure" value="true" />
	</bean>
	<bean id="d1c_cam1" class="uk.ac.gda.client.live.stream.view.CameraConfiguration">
		<property name="displayName" value="D1C Cam 1" />
		<property name="url" value="http://bl07c-di-serv-01.diamond.ac.uk:8081/D1C.DCAM1.mjpg.mjpg" />
		<property name="arrayPv" value="BL07C-DI-PHDGN-01:DCAM1:ARR" />
		<property name="roiProvider" ref="d1c1_roi" />
	</bean>

