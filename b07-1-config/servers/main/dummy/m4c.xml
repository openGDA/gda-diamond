<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="m4c_x_motor" class="gda.device.motor.DummyMotor">
		<property name="local" value="true" />
	</bean>

	<bean id="m4c_y_motor" class="gda.device.motor.DummyMotor">
		<property name="local" value="true" />
	</bean>

	<bean id="m4c_pitch_motor" class="gda.device.motor.DummyMotor">
		<property name="local" value="true" />
	</bean>

	<bean id="m4c_roll_motor" class="gda.device.motor.DummyMotor">
		<property name="local" value="true" />
	</bean>

	<bean id="m4c_yaw_motor" class="gda.device.motor.DummyMotor">
		<property name="local" value="true" />
	</bean>

	<bean id="m4c_current1" class="gda.device.scannable.DummyScannable" />
	<bean id="m4c_current2" class="gda.device.scannable.DummyScannable" />

</beans>
