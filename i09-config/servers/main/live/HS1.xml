<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="hs1temp1" class="gda.spring.EpicsMonitorFactoryBean">
		<property name="deviceName" value="HS1.TEMP1" />
	</bean>
	<bean id="hs1temp2" class="gda.spring.EpicsMonitorFactoryBean">
		<property name="deviceName" value="HS1.TEMP2" />
	</bean>
	<bean id="hs1temp3" class="gda.spring.EpicsMonitorFactoryBean">
		<property name="deviceName" value="HS1.TEMP3" />
	</bean>
	<bean id="hs1temp4" class="gda.spring.EpicsMonitorFactoryBean">
		<property name="deviceName" value="HS1.TEMP4" />
	</bean>

	<bean id="hs11wfs1" class="gda.spring.EpicsMonitorFactoryBean">
		<property name="deviceName" value="HS1.WFS1" />
	</bean>

	<bean id="hs1inboard_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="HS1.INBOARD" />
	</bean>
	<bean id="hs1outboard_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="HS1.OUTBOARD" />
	</bean>
	<bean id="hs1top_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="HS1.TOP" />
	</bean>
	<bean id="hs1bottom_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="HS1.BOTTOM" />
	</bean>

	<bean id="hs1xcentre_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="HS1.XCENTRE" />
	</bean>
	<bean id="hs1xgap_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="HS1.XGAP" />
	</bean>
	<bean id="hs1ycentre_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="HS1.YCENTRE" />
	</bean>
	<bean id="hs1ygap_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="HS1.YGAP" />
	</bean>
</beans>
