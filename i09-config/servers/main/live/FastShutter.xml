<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="fsj1" class="gda.device.enumpositioner.EpicsSimpleBinary">
		<property name="deviceName" value="FSJ1" />
	</bean>
	<bean id="fsi1" class="gda.device.enumpositioner.EpicsSimpleBinary">
		<property name="deviceName" value="FSI1" />
	</bean>
	<bean id="fsj1inlim" class="gda.device.enumpositioner.EpicsSimpleBinary">
		<property name="deviceName" value="FSJ1.INLIM" />
		<property name="readonly" value="true"/>
	</bean>
	<bean id="fsi1inlim" class="gda.device.enumpositioner.EpicsSimpleBinary">
		<property name="deviceName" value="FSI1.INLIM" />
		<property name="readonly" value="true"/>
	</bean>

</beans>
