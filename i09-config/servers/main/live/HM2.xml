<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="hm2y_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="HM2.Y" />
	</bean>
	<bean id="hm2upstreamx_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="HM2.UPSTREAMX" />
	</bean>
	<bean id="hm2downstreamx_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="HM2.DOWNSTREAMX" />
	</bean>
	<bean id="hm2pitch_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="HM2.PITCH" />
	</bean>
	<bean id="hm2x_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="HM2.X" />
	</bean>
</beans>
