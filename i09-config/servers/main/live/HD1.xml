<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="hd1y_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="HD1.Y" />
	</bean>
	<bean id="hd1aposn" class="gda.spring.EpicsPositionerFactoryBean">
		<property name="deviceName" value="HD1.APOSN" />
	</bean>
	<bean id="hd1temp1" class="gda.spring.EpicsMonitorFactoryBean">
		<property name="deviceName" value="HD1.TEMP1" />
	</bean>
	<!-- <bean id="hd1cam" class="gda.device.diagnosis.AravisCamera"> -->
	<!-- <property name="deviceName" value="HD1.CAM_aravisCamera"/> -->
	<!-- </bean> -->

</beans>
