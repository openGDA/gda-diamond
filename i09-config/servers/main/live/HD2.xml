<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="hd2iamp2" class="gda.device.currentamplifier.EpicsCurrAmpSingle">
		<property name="deviceName" value="HD2.IAMP2" />
		<property name="poll" value="true"/>
	</bean>
	<bean id="hd2y_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="HD2.Y" />
	</bean>
	<bean id="hd2aposn" class="gda.spring.EpicsPositionerFactoryBean">
		<property name="deviceName" value="HD2.APOSN" />
	</bean>
	<!-- <bean id="hd2cam" class="gda.device.diagnosis.AravisCamera"> -->
	<!-- <property name="deviceName" value="HD2.CAM_aravisCamera"/> -->
	<!-- </bean> -->
</beans>
