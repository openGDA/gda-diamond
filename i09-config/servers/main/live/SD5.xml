<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<bean id="sd5i" class="gda.device.currentamplifier.EpicsCurrAmpSingle">
		<property name="deviceName" value="SD5.IAMP15" />
		<property name="poll" value="true"/>
	</bean>
	<bean id="sd5y_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="SD5.Y" />
	</bean>

	<bean id="sd5aposn" class="gda.spring.EpicsPositionerFactoryBean">
		<property name="deviceName" value="SD5.APOSN" />
	</bean>
</beans>
