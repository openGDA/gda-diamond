<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="sm_xp_motor" class="gda.device.motor.DummyMotor"/>

	<bean id="sm_yp_motor" class="gda.device.motor.DummyMotor"/>

	<bean id="sm_zp_motor" class="gda.device.motor.DummyMotor"/>

	<bean id="sm_polar_motor" class="gda.device.motor.DummyMotor">
		<property name="unitString" value="deg"/>
	</bean>
	<bean id="sm_azimuth_motor" class="gda.device.motor.DummyMotor">
		<property name="unitString" value="deg"/>
	</bean>

</beans>
