<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="ccmc_x_motor" class="gda.device.motor.DummyMotor"/>

	<bean id="ccmc_y_motor" class="gda.device.motor.DummyMotor"/>

	<bean id="ccmc_z_motor" class="gda.device.motor.DummyMotor"/>

	<bean id="roty_motor" class="gda.device.motor.DummyMotor"/>

	<bean id="ccmc_named_position" class="gda.device.enumpositioner.DummyEnumPositioner">
		<property name="positions">
			<list>
				<value>Out of Beam</value>
				<value>Xtal_2000</value>
				<value>Xtal_2250</value>
				<value>Xtal_2500</value>
			</list>
		</property>
	</bean>
</beans>