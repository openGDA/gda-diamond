    			<entry key="1e4" value="10^4 low noise" />
    			<entry key="1e5" value="10^5 low noise" />
    			<entry key="1e6" value="10^6 low noise" />
    			<entry key="1e7" value="10^7 low noise" />
    			<entry key="1e8" value="10^8 low noise" />
