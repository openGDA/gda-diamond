	<bean id="sm2_xp_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL07C-EA-SM-02:XP" />
	</bean>

	<bean id="sm2_yp_motor" class="gda.device.motor.EpicsMotor">
