	</bean>

	<bean id="sm2_xpc_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL07C-EA-SM-02:XPC" />
	</bean>

	<bean id="sm2_ypc_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL07C-EA-SM-02:YPC" />
