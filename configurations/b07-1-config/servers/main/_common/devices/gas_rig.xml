				<entry key="P-5" value="38.8" />
				<entry key="NO" value="30" />
				<entry key="NF3" value="71" />
				<entry key="NH3" value="17" />
				<entry key="Cl2" value="71" />
				<entry key="H2S" value="34" />
				<entry key="SO2" value="64" />
				<entry key="C3H6" value="42" />
			</map>
		</constructor-arg>
	</bean>
</beans>