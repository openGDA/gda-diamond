<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- Ring -->
	<import resource="ring.xml"/>

	<!-- Slits -->
	<import resource="s1c.xml"/>
	<import resource="s2c.xml"/>
	<import resource="s3c.xml"/>
	<import resource="s4c.xml"/>

	<!-- Mirrors -->
	<import resource="m1b.xml"/>
	<import resource="m1c.xml"/>
	<import resource="m3c.xml"/>
	<import resource="m4c.xml"/>
	<import resource="m5c.xml"/>

	<!-- Diagnostics -->
	<import resource="d1c.xml"/>
	<import resource="d2c.xml"/>
	<import resource="d3c.xml"/>
	<import resource="d4c.xml"/>
	<import resource="d5c.xml"/>
	<import resource="d6c.xml"/>
	<import resource="d7c.xml"/>
	<import resource="d9c.xml"/>

	<!-- Monocromator -->
	<import resource="pgm.xml"/>

	<!-- Dummy devices -->
	<import resource="dummy.xml"/>

</beans>
