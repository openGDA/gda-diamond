<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="fast_shutter_con" class="gda.device.enumpositioner.DummyEnumPositioner">
		<property name="positions">
			<list>
				<value>Open</value>
				<value>Close</value>
				<value>Reset</value>
			</list>
		</property>
	</bean>
	
	<bean id="fast_shutter_sta" class="gda.device.enumpositioner.DummyEnumPositioner">
		<property name="positions">
			<list>
				<value>Fault</value>
				<value>Open</value>
				<value>Opening</value>
				<value>Closed</value>
				<value>Closing</value>
			</list>
		</property>
	</bean>
	
	<bean id="expt_shutter_con" class="gda.device.enumpositioner.DummyEnumPositioner">
		<property name="positions">
			<list>
				<value>Open</value>
				<value>Close</value>
				<value>Reset</value>
			</list>
		</property>
	</bean>
	
	<bean id="expt_shutter_sta" class="gda.device.enumpositioner.DummyEnumPositioner">
		<property name="positions">
			<list>
				<value>Fault</value>
				<value>Open</value>
				<value>Opening</value>
				<value>Closed</value>
				<value>Closing</value>
			</list>
		</property>
	</bean>

</beans>