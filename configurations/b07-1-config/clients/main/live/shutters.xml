<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="fast_shutter_con" class="gda.device.enumpositioner.EpicsSimpleMbbinary">
		<property name="recordName" value="BL07C-EA-SHTR-21:CON" />
		<property name="readOnly" value="false"/>
	</bean>
	
	<bean id="fast_shutter_sta" class="gda.device.enumpositioner.EpicsSimpleMbbinary">
		<property name="recordName" value="BL07C-EA-SHTR-21:STA" />
	</bean>
	
	<bean id="expt_shutter_con" class="gda.device.enumpositioner.EpicsSimpleMbbinary">
		<property name="recordName" value="BL07C-PS-SHTR-01:CON" />
		<property name="readOnly" value="false"/>
	</bean>
	
	<bean id="expt_shutter_sta" class="gda.device.enumpositioner.EpicsSimpleMbbinary">
		<property name="recordName" value="BL07C-PS-SHTR-01:STA" />
	</bean>

</beans>