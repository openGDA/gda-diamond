#! /bin/sh

# run on i05-control

cd /dls_sw/i05/software/gda-devel/config
chmod o+w .
sudo su - gda2
cd /dls_sw/i05/software/gda-devel/config
cp i05ssh.key lala
mv -f lala i05ssh.key
^D
chmod o-w .
chmod a+x /dls_sw/i05/software/gda/workspace_git/gda-core.git/uk.ac.gda.core/bin/gda
setfacl -m g:i05_staff:rwx -m u:i05user:rwx xml/user_permissions.xml xml/beamlinestaff.xml
