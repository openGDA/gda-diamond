<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="ll_pressure" class="gda.device.monitor.DummyEpicsMonitor">
		<property name="unit" value="mbar" />
		<property name="value">
			<value type="double">1.4e-10</value>
		</property>
	</bean>

	<bean id="lc_pressure" class="gda.device.monitor.DummyEpicsMonitor">
		<property name="unit" value="mbar" />
		<property name="value">
			<value type="double">1.4e-10</value>
		</property>
	</bean>

	<bean id="uc_pressure" class="gda.device.monitor.DummyEpicsMonitor">
		<property name="unit" value="mbar" />
		<property name="value">
			<value type="double">3.2e-10</value>
		</property>
	</bean>

	<bean id="ic_pressure" class="gda.device.monitor.DummyEpicsMonitor">
		<property name="unit" value="mbar" />
		<property name="value">
			<value type="double">7.5e-10</value>
		</property>
	</bean>

</beans>
