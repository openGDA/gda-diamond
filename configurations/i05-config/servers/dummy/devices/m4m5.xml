<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="m4m5_x_motor" class="gda.device.motor.DummyMotor"/>

	<bean id="m4m5_y_motor" class="gda.device.motor.DummyMotor"/>

	<bean id="m4m5_z_motor" class="gda.device.motor.DummyMotor"/>

	<bean id="m4m5_pitch_motor" class="gda.device.motor.DummyMotor"/>

	<bean id="m4m5_yaw_motor" class="gda.device.motor.DummyMotor"/>

	<bean id="m4m5_roll_motor" class="gda.device.motor.DummyMotor"/>

	<bean id="m4m5_mirror" class="gda.device.enumpositioner.DummyEnumPositioner">
		<property name="positions">
			<list>
				<value>None</value>
				<value>M4</value>
				<value>M5</value>
				<value>Reference</value>
			</list>
		</property>
	</bean>

</beans>
