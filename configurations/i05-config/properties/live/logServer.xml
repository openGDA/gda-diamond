<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration>

<!-- Add debug="true" into the configuration here to log logback configuration -->
<!-- scan="true" scanPeriod="10 seconds" enable dynamic updating of logging config -->
<configuration scan="true" scanPeriod="10 seconds">

	<!-- The logServer.xml files define which messages are sent to the logging system appenders. -->
	<!-- Levels: ALL TRACE DEBUG INFO WARN ERROR FATAL OFF -->

	<!-- Load the common logging config -->
	<!-- include file is relative to the application directory -->
	<include file="${gda.config}/properties/_common/logServer.xml" />

	<!-- Only add stuff here which only applies to live -->

</configuration>
