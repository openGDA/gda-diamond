<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="ring_current" class="gda.device.monitor.DummyEpicsMonitor">
		<property name="unit" value="mA"/>
		<property name="value">
			<value type="double">300</value>
		</property>
	</bean>
	
	<bean id="ring_energy" class="gda.device.monitor.DummyEpicsMonitor">
		<property name="unit" value="GeV"/>
		<property name="value">
			<value type="double">3.0</value>
		</property>
	</bean>

</beans>
