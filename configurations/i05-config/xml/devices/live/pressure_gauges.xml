<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="lc_pressure" class="gda.device.monitor.EpicsMonitor">
		<property name="pvName" value="BL05I-VA-HCG-02:P" />
		<property name="unit" value="mbar" />
		<property name="poll" value="true" />
		<property name="sensitivity" value="2.0" />
	</bean>

	<bean id="uc_pressure" class="gda.device.monitor.EpicsMonitor">
		<property name="pvName" value="BL05I-VA-HCG-03:P" />
		<property name="unit" value="mbar" />
		<property name="poll" value="true" />
		<property name="sensitivity" value="2.0" />
	</bean>

	<bean id="ic_pressure" class="gda.device.monitor.EpicsMonitor">
		<property name="pvName" value="BL05I-VA-HCG-04:P" />
		<property name="unit" value="mbar" />
		<property name="poll" value="true" />
		<property name="sensitivity" value="2.0" />
	</bean>

</beans>
