<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="salong" class="uk.ac.gda.arpes.scannable.CombinedManipulator">
		<property name="scannables">
			<list>
				<ref bean="sax" />
				<ref bean="say" />
			</list>
		</property>
		<property name="calculator">
			<bean class="uk.ac.gda.beamline.i05.scannable.salongCalculator" />
		</property>
	</bean>

	<bean id="saperp" class="uk.ac.gda.arpes.scannable.CombinedManipulator">
		<property name="scannables">
			<list>
				<ref bean="sax" />
				<ref bean="say" />
			</list>
		</property>
		<property name="calculator">
			<bean class="uk.ac.gda.beamline.i05.scannable.saperpCalculator" />
		</property>
	</bean>

</beans>