<?xml version='1.0' encoding='UTF-8'?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- MAC stage 1 Motors -->
	<bean id="mac1a_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL11I-EA-MAC-01:A" />
	</bean>
	<bean id="mac12a_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL11I-EA-MAC-01:2A" />
	</bean>
	<!-- MAC stage 2 Motors -->
	<bean id="mac2a_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL11I-EA-MAC-02:A" />
	</bean>
	<bean id="mac22a_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL11I-EA-MAC-02:2A" />
	</bean>
	<!-- MAC stage 31 Motors -->
	<bean id="mac3a_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL11I-EA-MAC-03:A" />
	</bean>
	<bean id="mac32a_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL11I-EA-MAC-03:2A" />
	</bean>
	<!-- MAC stage 4 Motors -->
	<bean id="mac4a_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL11I-EA-MAC-04:A" />
	</bean>
	<bean id="mac42a_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL11I-EA-MAC-04:2A" />
	</bean>
	<!-- MAC stage 5 Motors -->
	<bean id="mac5a_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL11I-EA-MAC-05:A" />
	</bean>
	<bean id="mac52a_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL11I-EA-MAC-05:2A" />
	</bean>
</beans>
