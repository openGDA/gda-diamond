<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- GIS Motors (individual Motions) -->
	<bean id="gis_y_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL11I-MO-TABLE-03:Y"/>
	</bean>
	<bean id="gis_chi_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL11I-MO-TABLE-03:ROLL"/>
	</bean>
	<bean id="gis_psi_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL11I-MO-TABLE-03:YAW"/>
	</bean>
</beans>
