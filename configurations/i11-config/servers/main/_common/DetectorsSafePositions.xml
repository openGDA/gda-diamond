<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	
	<bean id="macsafeposition" class="gda.hrpd.pmac.SafePosition">
		<property name="position" value="40.0"/>
		<property name="tolerance" value="1.0"/>
	</bean>

	<bean id="psdsafeposition" class="gda.hrpd.pmac.SafePosition">
		<property name="position" value="70.0"/>
		<property name="tolerance" value="1.0"/>
	</bean>
</beans>
