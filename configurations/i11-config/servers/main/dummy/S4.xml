<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="s4bpm" class="gda.device.monitor.DummyQbpm"/>

	<!-- S4 Motors (individual Motions) -->
	<bean id="s4xminus_motor" class="gda.device.motor.DummyMotor"/>
	<bean id="s4xplus_motor" class="gda.device.motor.DummyMotor"/>
	<bean id="s4yminus_motor" class="gda.device.motor.DummyMotor"/>
	<bean id="s4yplus_motor" class="gda.device.motor.DummyMotor"/>
	<!-- S4 Motors (Compound Motions) -->
	<bean id="s4xpos_motor" class="gda.device.motor.DummyMotor"/>
	<bean id="s4xgap_motor" class="gda.device.motor.DummyMotor"/>
	<bean id="s4ypos_motor" class="gda.device.motor.DummyMotor"/>
	<bean id="s4ygap_motor" class="gda.device.motor.DummyMotor"/>
</beans>
