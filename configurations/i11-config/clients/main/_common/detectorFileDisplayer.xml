<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="detectorFileDisplayer" class="uk.ac.gda.beamline.i11.views.DetectorFileDisplayer">
		<property name="viewID" value="uk.ac.gda.beamline.i11.views.DetectorFilePlotView"/>
		<property name="viewName" value="DetectorPlot"/>
		<property name="newPlot" value="true"/>
		<property name="plotType" value="#{ T(org.eclipse.dawnsci.plotting.api.PlotType).XY}"/>
	</bean>

</beans>
