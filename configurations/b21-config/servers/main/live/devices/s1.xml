<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="s1_xcentre_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S1.XCENTRE" />
		<property name="local" value="true" />
	</bean>

	<bean id="s1_xgap_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S1.XSIZE" />
		<property name="local" value="true" />
	</bean>

	<bean id="s1_ycentre_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S1.YCENTRE" />
		<property name="local" value="true" />
	</bean>

	<bean id="s1_ygap_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S1.YSIZE" />
		<property name="local" value="true" />
	</bean>

	<bean id="s1_xplus_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S1.XPLUS" />
		<property name="local" value="true" />
	</bean>

	<bean id="s1_xminus_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S1.XMINUS" />
		<property name="local" value="true" />
	</bean>

	<bean id="s1_yplus_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S1.YPLUS" />
		<property name="local" value="true" />
	</bean>

	<bean id="s1_yminus_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S1.YMINUS" />
		<property name="local" value="true" />
	</bean>

</beans>
