<?xml version='1.0' encoding='UTF-8'?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="s5_xcentre_motor" class="gda.device.motor.EpicsMotor">
		<property name="local" value="true" />
		<property name="pvName" value="BL21B-AL-SLITS-05:X:CENTRE" />
	</bean>

	<bean id="s5_xgap_motor" class="gda.device.motor.EpicsMotor">
		<property name="local" value="true" />
		<property name="pvName" value="BL21B-AL-SLITS-05:X:SIZE" />
	</bean>

	<bean id="s5_ycentre_motor" class="gda.device.motor.EpicsMotor">
		<property name="local" value="true" />
		<property name="pvName" value="BL21B-AL-SLITS-05:Y:CENTRE" />
	</bean>

	<bean id="s5_ygap_motor" class="gda.device.motor.EpicsMotor">
		<property name="local" value="true" />
		<property name="pvName" value="BL21B-AL-SLITS-05:Y:SIZE" />
	</bean>

	<bean id="s5_xplus_motor" class="gda.device.motor.EpicsMotor">
		<property name="local" value="true" />
		<property name="pvName" value="BL21B-AL-SLITS-05:X:PLUS" />
	</bean>

	<bean id="s5_xminus_motor" class="gda.device.motor.EpicsMotor">
		<property name="local" value="true" />
		<property name="pvName" value="BL21B-AL-SLITS-05:X:MINUS" />
	</bean>

	<bean id="s5_yplus_motor" class="gda.device.motor.EpicsMotor">
		<property name="local" value="true" />
		<property name="pvName" value="BL21B-AL-SLITS-05:Y:PLUS" />
	</bean>

	<bean id="s5_yminus_motor" class="gda.device.motor.EpicsMotor">
		<property name="local" value="true" />
		<property name="pvName" value="BL21B-AL-SLITS-05:Y:MINUS" />
	</bean>

</beans>
