<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="s5_xcentre_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S5.XCENTRE" />
		<property name="local" value="true" />
	</bean>

	<bean id="s5_xgap_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S5.XSIZE" />
		<property name="local" value="true" />
	</bean>

	<bean id="s5_ycentre_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S5.YCENTRE" />
		<property name="local" value="true" />
	</bean>

	<bean id="s5_ygap_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S5.YSIZE" />
		<property name="local" value="true" />
	</bean>

	<bean id="s5_xplus_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S5.XPLUS" />
		<property name="local" value="true" />
	</bean>

	<bean id="s5_xminus_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S5.XMINUS" />
		<property name="local" value="true" />
	</bean>

	<bean id="s5_yplus_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S5.YPLUS" />
		<property name="local" value="true" />
	</bean>

	<bean id="s5_yminus_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S5.YMINUS" />
		<property name="local" value="true" />
	</bean>

</beans>
