<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">


	<bean id="t5_y_motor" class="gda.device.motor.EpicsMotor" >
		<property name="pvName" value="BL21B-MO-TABLE-05:Y" />
		<property name="local" value="true" />
	</bean>

	<bean id="t5_yaw_motor" class="gda.device.motor.EpicsMotor" >
		<property name="pvName" value="BL21B-MO-TABLE-05:YAW1" />
		<property name="local" value="true" />
	</bean>

</beans>
