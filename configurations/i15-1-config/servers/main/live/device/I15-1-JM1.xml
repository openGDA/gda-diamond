<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="m1Y_motor"
			class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL15J-OP-MIRR-01:Y" />
	</bean>

	<bean id="m1Pitch_motor"
			class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL15J-OP-MIRR-01:PITCH" />
	</bean>

	<bean id="m1Roll_motor"
			class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL15J-OP-MIRR-01:ROLL" />
	</bean>

	<bean id="m1X_motor"
			class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL15J-OP-MIRR-01:X" />
	</bean>

	<bean id="m1Yaw_motor"
			class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL15J-OP-MIRR-01:YAW" />
	</bean>

	<bean id="m1ibY_motor"
			class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL15J-OP-MIRR-01:J1" />
	</bean>

	<bean id="m1obY_motor"
			class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL15J-OP-MIRR-01:J2" />
	</bean>

	<bean id="m1dsY_motor"
			class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL15J-OP-MIRR-01:J3" />
	</bean>

	<bean id="m1usX_motor"
			class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL15J-OP-MIRR-01:X1" />
	</bean>

	<bean id="m1dsX_motor"
			class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL15J-OP-MIRR-01:X2" />
	</bean>
</beans>
