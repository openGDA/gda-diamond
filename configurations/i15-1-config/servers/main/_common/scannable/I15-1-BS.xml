<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="bs2X" class="gda.device.scannable.ScannableMotor">
		<property name="initialUserUnits" value="um"/>
		<property name="motor" ref="bs2X_motor" />
	</bean>

	<bean id="bs2Y" class="gda.device.scannable.ScannableMotor">
		<property name="initialUserUnits" value="um"/>
		<property name="motor" ref="bs2Y_motor" />
	</bean>

	<bean id="bs2" class="gda.device.scannable.scannablegroup.ScannableGroup">
		<property name="groupMembers">
			<list>
				<ref bean="bs2X" />
				<ref bean="bs2Y" />
			</list>
		</property>
	</bean>
</beans>