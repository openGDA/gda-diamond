<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="gantry_gripper" class="gda.device.enumpositioner.DummyEnumPositioner">
		<property name="positions">
			<list>
				<value>Open</value>
				<value>Close</value>
			</list>
		</property>
	</bean>

	<bean id="gantry_gripper_angle_motor" class="gda.device.motor.DummyMotor" >
		<property name="speed" value="25" />
	</bean>
	<bean id="gantry_x_motor" class="gda.device.motor.DummyMotor">
		<property name="speed" value="10" />
	</bean>
	<bean id="gantry_y_motor" class="gda.device.motor.DummyMotor">
		<property name="speed" value="10" />
	</bean>
</beans>