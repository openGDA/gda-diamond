	</bean>
	<bean id="xtal_minus1_pitch_motor" class="gda.device.motor.DummyMotor">
		<property name="speed" value="200" />
	</bean>
	<bean id="xtal_central_y_motor" class="gda.device.motor.DummyMotor">
		<property name="speed" value="200" />
	</bean>
	<bean id="xtal_central_rot_motor" class="gda.device.motor.DummyMotor">
		<property name="speed" value="200" />
	</bean>
	<bean id="xtal_central_pitch_motor" class="gda.device.motor.DummyMotor">
		<property name="speed" value="200" />
	</bean>
	<bean id="xtal_plus1_x_motor" class="gda.device.motor.DummyMotor">
		<property name="speed" value="200" />
	</bean>
	<bean id="xtal_plus1_y_motor" class="gda.device.motor.DummyMotor">
		<property name="speed" value="200" />
	</bean>
	<bean id="xtal_plus1_rot_motor" class="gda.device.motor.DummyMotor">
		<property name="speed" value="200" />
	</bean>
	<bean id="xtal_plus1_pitch_motor" class="gda.device.motor.DummyMotor">
		<property name="speed" value="200" />
	</bean>
	<bean id="det_x_motor" class="gda.device.motor.DummyMotor">
		<property name="speed" value="200" />
	</bean>
	<bean id="det_y_motor" class="gda.device.motor.DummyMotor">
		<property name="speed" value="200" />
	</bean>
	<bean id="det_rot_motor" class="gda.device.motor.DummyMotor">
		<property name="speed" value="200" />
	</bean>
	<bean id="xtal_x_motor" class="gda.device.motor.DummyMotor">
		<property name="speed" value="200" />
	</bean>
	<bean id="spec_rot_motor" class="gda.device.motor.DummyMotor">
		<property name="speed" value="200" />
	</bean>
	<bean id="det_slit_motor" class="gda.device.motor.DummyMotor">
		<property name="speed" value="200" />
	</bean>

	<bean id="xes_slit_motor" class="gda.device.motor.DummyMotor">
		<property name="local" value="false" />
		<property name="speed" value="200" />
	</bean>

	<bean id="radius" class="gda.device.scannable.DummyPersistentScannable">
		<property name="outputFormat">
			<util:list>
				<value>1.0</value>
			</util:list>
		</property>
	</bean>
	
	<bean id="cut1" class="gda.device.enumpositioner.DummyEnumPositioner">
		<property name="positions">
			<list>
				<value>1</value>
				<value>2</value>
				<value>3</value>
			</list>
		</property>
	</bean>
	<bean id="cut2" class="gda.device.enumpositioner.DummyEnumPositioner">
		<property name="positions">
			<list>
				<value>1</value>
				<value>2</value>
				<value>3</value>
			</list>
		</property>
	</bean>
	<bean id="cut3" class="gda.device.enumpositioner.DummyEnumPositioner">
		<property name="positions">
			<list>
				<value>1</value>
				<value>2</value>
				<value>3</value>
			</list>
		</property>
