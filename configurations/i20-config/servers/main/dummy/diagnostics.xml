<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">


	<!--diagnostics -->

	<bean id="d1_current" class="gda.device.currentamplifier.EpicsCurrAmpSingle">
		<property name="deviceName" value="D1.D1I" />
	</bean>
	<bean id="d1_screen" class="gda.device.enumpositioner.EpicsPositioner">
		<property name="deviceName" value="D1P" />
	</bean>
	<bean id="d1_motor" class="gda.device.motor.DummyMotor" />
	<bean id="d1_gain" class="gda.device.enumpositioner.EpicsSimplePositioner">
		<property name="pvName" value="BL20I-DI-IAMP-01:GAIN" />
		<property name="values">
			<map>
				<entry key="3" value="10^3 low noise" />
				<entry key="4" value="10^4 low noise" />
				<entry key="5" value="10^5 low noise" />
				<entry key="6" value="10^6 low noise" />
				<entry key="7" value="10^7 low noise" />
				<entry key="8" value="10^8 low noise" />
				<entry key="9" value="10^9 low noise" />
			</map>
		</property>
	</bean>


	<bean id="d3_screen" class="gda.device.enumpositioner.EpicsPositioner">
		<property name="deviceName" value="D3P" />
	</bean>
	<bean id="d3_motor" class="gda.device.motor.DummyMotor" />
	<bean id="d3_updrain" class="gda.device.monitor.EpicsMonitor">
