<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

<!--	sample -->

	<bean id="table1_x_motor" class="gda.device.motor.DummyMotor"/>
	<bean id="table1_y_motor" class="gda.device.motor.DummyMotor"/>

	<bean id="table2_x_motor" class="gda.device.motor.DummyMotor"/>
	<bean id="table2_y_motor" class="gda.device.motor.DummyMotor"/>

	<bean id="sample_x_motor" class="gda.device.motor.DummyMotor"/>
	<bean id="sample_y_motor" class="gda.device.motor.DummyMotor"/>
	<bean id="sample_z_motor" class="gda.device.motor.DummyMotor"/>
	<bean id="sample_pitch_motor" class="gda.device.motor.DummyMotor"/>
	<bean id="sample_roll_motor" class="gda.device.motor.DummyMotor"/>
	<bean id="sample_rot_motor" class="gda.device.motor.DummyMotor"/>
	<bean id="sample_fine_rot_motor" class="gda.device.motor.DummyMotor"/>

	<bean id="filterwheel" class="gda.device.enumpositioner.DummyEnumPositioner">
		<property name="positions">
			<list>
				<value>Empty</value>
				<value>Mn 5um</value>
				<value>Fe 5um</value>
				<value>Co 5um</value>
				<value>Ni 5um</value>
				<value>Cu 5um</value>
				<value>Zn 5um</value>
				<value>Zr 20um</value>
				<value>Pb 10um</value>
				<value>Rh (empty)</value>
				<value>Pd 25um</value>
				<value>Ag (empty)</value>
				<value>Cd 25um</value>
				<value>W 4um</value>
				<value>Pt 5um</value>
				<value>User</value>
			</list>
		</property>
	</bean>

</beans>
