<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
       
    	<bean id="ionc1_power_supply" class="gda.device.scannable.PowerSupplyScannable">
		<property name="readback" value="BL20I-EA-ISEG-01:V1_ACTUAL" />
		<property name="vA" value="BL20I-EA-ISEG-01:SET_V1_SET.A" />
		<property name="vB" value="BL20I-EA-ISEG-01:SET_V1_SET.B" />
		<property name="vC" value="BL20I-EA-ISEG-01:SET_V1_SET.C" />
		<property name="vD" value="BL20I-EA-ISEG-01:SET_V1_SET.D" />
		<property name="send" value="BL20I-EA-ISEG-01:SET_V1_SET:SEND.PROC" />
		<property name="start_ramp" value="BL20I-EA-ISEG-01:V1_START" />
	</bean>

	<bean id="ionc2_power_supply" class="gda.device.scannable.PowerSupplyScannable">
		<property name="readback" value="BL20I-EA-ISEG-01:V2_ACTUAL" />
		<property name="vA" value="BL20I-EA-ISEG-01:SET_V2_SET.A" />
		<property name="vB" value="BL20I-EA-ISEG-01:SET_V2_SET.B" />
		<property name="vC" value="BL20I-EA-ISEG-01:SET_V2_SET.C" />
		<property name="vD" value="BL20I-EA-ISEG-01:SET_V2_SET.D" />
		<property name="send" value="BL20I-EA-ISEG-01:SET_V2_SET:SEND.PROC" />
		<property name="start_ramp" value="BL20I-EA-ISEG-01:V2_START" />
	</bean>

	<bean id="ionc3_power_supply" class="gda.device.scannable.PowerSupplyScannable">
		<property name="readback" value="BL20I-EA-ISEG-02:V1_ACTUAL" />
		<property name="vA" value="BL20I-EA-ISEG-02:SET_V1_SET.A" />
		<property name="vB" value="BL20I-EA-ISEG-02:SET_V1_SET.B" />
		<property name="vC" value="BL20I-EA-ISEG-02:SET_V1_SET.C" />
		<property name="vD" value="BL20I-EA-ISEG-02:SET_V1_SET.D" />
		<property name="send" value="BL20I-EA-ISEG-02:SET_V1_SET:SEND.PROC" />
		<property name="start_ramp" value="BL20I-EA-ISEG-02:V1_START" />
	</bean>


	<bean id="ionc_purge_pressure" class="gda.device.scannable.GasFillFieldScannable">
		<property name="pvName" value="BL20I-EA-GIR-01:PURGE:P" />
	</bean>
	<bean id="ionc_purge_period" class="gda.device.scannable.GasFillFieldScannable">
		<property name="pvName" value="BL20I-EA-GIR-01:PURGE:PERIOD" />
	</bean>
	<bean id="ionc_purge_timeout" class="gda.device.scannable.GasFillFieldScannable">
		<property name="pvName" value="BL20I-EA-GIR-01:PURGE:TIMEOUT" />
	</bean>
	<bean id="ionc_gas_fill1_pressure" class="gda.device.scannable.GasFillFieldScannable">
		<property name="pvName" value="BL20I-EA-GIR-01:FILL1:P" />
	</bean>
	<bean id="ionc_gas_fill1_period" class="gda.device.scannable.GasFillFieldScannable">
		<property name="pvName" value="BL20I-EA-GIR-01:FILL1:PERIOD" />
	</bean>
	<bean id="ionc_gas_fill1_timeout" class="gda.device.scannable.GasFillFieldScannable">
		<property name="pvName" value="BL20I-EA-GIR-01:FILL1:TIMEOUT" />
	</bean>
	<bean id="ionc_gas_fill2_pressure" class="gda.device.scannable.GasFillFieldScannable">
		<property name="pvName" value="BL20I-EA-GIR-01:FILL2:P" />
	</bean>
	<bean id="ionc_gas_fill2_period" class="gda.device.scannable.GasFillFieldScannable">
		<property name="pvName" value="BL20I-EA-GIR-01:FILL2:PERIOD" />
	</bean>
	<bean id="ionc_gas_fill2_timeout" class="gda.device.scannable.GasFillFieldScannable">
		<property name="pvName" value="BL20I-EA-GIR-01:FILL2:TIMEOUT" />
	</bean>
	<bean id="ionc_gas_fill_start" class="gda.device.scannable.GasFillFieldScannable">
		<property name="pvName" value="BL20I-EA-GIR-01:START" />
	</bean>
	<bean id="ionc_gas_select" class="gda.device.scannable.GasFillFieldScannable">
		<property name="pvName" value="BL20I-EA-GIR-01:FILL1:GAS" />
	</bean>
	<bean id="ionc_control_select" class="gda.device.scannable.GasFillFieldScannable">
		<property name="pvName" value="BL20I-EA-GIR-01:CON" />
	</bean>
	<bean id="ionc_ion_chamber_select" class="gda.device.scannable.GasFillFieldScannable">
		<property name="pvName" value="BL20I-EA-GIR-01:IONC" />
	</bean>
	<bean id="ionc_gas_injection_status" class="gda.device.scannable.GasFillFieldScannable">
		<property name="pvName" value="BL20I-EA-GIR-01:STS" />
	</bean>
	<bean id="ionc_base_pressure" class="gda.device.scannable.GasFillFieldScannable">
		<property name="pvName" value="BL20I-EA-GIR-01:PCTRL1:P:RD" />
	</bean>


	<bean id="ionc1_gas_injector" class="gda.device.scannable.GasInjectionScannable">
		<property name="purge_pressure" ref="ionc_purge_pressure" />
		<property name="purge_period" ref="ionc_purge_period" />
		<property name="purge_timeout" ref="ionc_purge_timeout" />
		<property name="gas_fill1_pressure" ref="ionc_gas_fill1_pressure" />
		<property name="gas_fill1_period" ref="ionc_gas_fill1_period" />
		<property name="gas_fill1_timeout" ref="ionc_gas_fill1_timeout" />
		<property name="gas_fill2_pressure" ref="ionc_gas_fill2_pressure" />
		<property name="gas_fill2_period" ref="ionc_gas_fill2_period" />
		<property name="gas_fill2_timeout" ref="ionc_gas_fill2_timeout" />
		<property name="gas_fill_start" ref="ionc_gas_fill_start" />
		<property name="gas_select" ref="ionc_gas_select" />
		<property name="control_select" ref="ionc_control_select" />
		<property name="ion_chamber_select" ref="ionc_ion_chamber_select" />
		<property name="gas_injection_status" ref="ionc_gas_injection_status" />
		<property name="power_supply" ref="ionc1_power_supply" />
		<property name="ion_chamber" value="0" />
		<property name="base_pressure" ref="ionc_base_pressure" />
	</bean>
	
	<bean id="ionc2_gas_injector" class="gda.device.scannable.GasInjectionScannable">
		<property name="purge_pressure" ref="ionc_purge_pressure" />
		<property name="purge_period" ref="ionc_purge_period" />
		<property name="purge_timeout" ref="ionc_purge_timeout" />
		<property name="gas_fill1_pressure" ref="ionc_gas_fill1_pressure" />
		<property name="gas_fill1_period" ref="ionc_gas_fill1_period" />
		<property name="gas_fill1_timeout" ref="ionc_gas_fill1_timeout" />
		<property name="gas_fill2_pressure" ref="ionc_gas_fill2_pressure" />
		<property name="gas_fill2_period" ref="ionc_gas_fill2_period" />
		<property name="gas_fill2_timeout" ref="ionc_gas_fill2_timeout" />
		<property name="gas_fill_start" ref="ionc_gas_fill_start" />
		<property name="gas_select" ref="ionc_gas_select" />
		<property name="control_select" ref="ionc_control_select" />
		<property name="ion_chamber_select" ref="ionc_ion_chamber_select" />
		<property name="gas_injection_status" ref="ionc_gas_injection_status" />
		<property name="power_supply" ref="ionc2_power_supply" />
		<property name="ion_chamber" value="1" />
		<property name="base_pressure" ref="ionc_base_pressure" />
	</bean>
	
	<bean id="ionc3_gas_injector" class="gda.device.scannable.GasInjectionScannable">
		<property name="purge_pressure" ref="ionc_purge_pressure" />
		<property name="purge_period" ref="ionc_purge_period" />
		<property name="purge_timeout" ref="ionc_purge_timeout" />
		<property name="gas_fill1_pressure" ref="ionc_gas_fill1_pressure" />
		<property name="gas_fill1_period" ref="ionc_gas_fill1_period" />
		<property name="gas_fill1_timeout" ref="ionc_gas_fill1_timeout" />
		<property name="gas_fill2_pressure" ref="ionc_gas_fill2_pressure" />
		<property name="gas_fill2_period" ref="ionc_gas_fill2_period" />
		<property name="gas_fill2_timeout" ref="ionc_gas_fill2_timeout" />
		<property name="gas_fill_start" ref="ionc_gas_fill_start" />
		<property name="gas_select" ref="ionc_gas_select" />
		<property name="control_select" ref="ionc_control_select" />
		<property name="ion_chamber_select" ref="ionc_ion_chamber_select" />
		<property name="gas_injection_status" ref="ionc_gas_injection_status" />
		<property name="power_supply" ref="ionc3_power_supply" />
		<property name="ion_chamber" value="2" />
		<property name="base_pressure" ref="ionc_base_pressure" />
	</bean>

	<bean id="ionc1_pressure" class="gda.device.scannable.GasFillFieldScannable">
		<property name="pvName" value="BL20I-EA-GIR-01:P2" />
	</bean>
	<bean id="ionc2_pressure" class="gda.device.scannable.GasFillFieldScannable">
		<property name="pvName" value="BL20I-EA-GIR-01:P3" />
	</bean>
	<bean id="ionc3_pressure" class="gda.device.scannable.GasFillFieldScannable">
		<property name="pvName" value="BL20I-EA-GIR-01:P4" />
	</bean>
           
</beans>