	<bean id="datawriterconfig" class="gda.data.scan.datawriter.AsciiDataWriterConfiguration">
		<property name="commentMarker" value="#"></property>
		<property name="controllerName" value="ExafsScriptObserver"></property>
		<property name="header">
			<list>
				<bean id="machine1" class="gda.data.scan.datawriter.AsciiMetadataConfig">
					<property name="label" value="DIAMOND LIGHT SOURCE" />
				</bean>
				<bean id="inst1" class="gda.data.scan.datawriter.AsciiMetadataConfig">
					<property name="label" value="Instrument: I20-XAS Date: %s" />
					<property name="labelValues">
						<list>
							<bean id="date1" class="gda.device.scannable.TimeScannable" />
						</list>
					</property>
				</bean>
				<bean id="ringenergy" class="gda.data.scan.datawriter.AsciiMetadataConfig">
					<property name="label" value="Ring energy: %5.5f GeV" />
					<property name="labelValues">
						<list>
							<ref bean="ringEnergy" />
						</list>
					</property>
				</bean>
				<bean id="ringcurrent" class="gda.data.scan.datawriter.AsciiMetadataConfig">
					<property name="label" value="Initial ring current: %5.5f mA" />
					<property name="labelValues">
						<list>
							<ref bean="ringCurrent" />
						</list>
					</property>
				</bean>
				<bean id="fillingmode" class="gda.data.scan.datawriter.AsciiMetadataConfig">
					<property name="label" value="Filling mode: %s" />
