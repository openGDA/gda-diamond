						</list>
					</property>
				</bean>
				<bean id="stanford3" class="gda.data.scan.datawriter.AsciiMetadataConfig">
					<property name="label" value="Iref Stanford sensitivity: %s %s Offsets: %s %s" />
					<property name="labelValues">
						<list>
							<ref bean="iref_stanford_sensitivity" />
							<ref bean="iref_stanford_sensitivity_units" />
							<ref bean="iref_stanford_offset" />
							<ref bean="iref_stanford_offset_units" />
						</list>
					</property>
				</bean>
				
			</list>
		</property>
		<property name="footer">
			<list>
				<bean id="inst2" class="gda.data.scan.datawriter.AsciiMetadataConfig">
					<property name="label" value="Instrument: I20-XAS Date: %s" />
					<property name="labelValues">
