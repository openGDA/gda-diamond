<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="table1_x_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL20I-MO-TABLE-01:X" />
		<property name="local" value="true" />
	</bean>
	
	<bean id="table1_y_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL20I-MO-TABLE-01:Y" />
		<property name="local" value="true" />
	</bean>
	
	<bean id="table2_x_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL20I-MO-TABLE-02:X" />
		<property name="local" value="false" />
	</bean>
	
	<bean id="table2_y_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL20I-MO-TABLE-02:Y" />
		<property name="local" value="false" />
	</bean>

	<bean id="sample_x_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL20I-MO-TABLE-03:X" />
		<property name="local" value="true" />
	</bean>

	<bean id="sample_y_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL20I-MO-TABLE-03:Y" />
		<property name="local" value="true" />
	</bean>

	<bean id="sample_z_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL20I-MO-TABLE-03:Z" />
		<property name="local" value="true" />
	</bean>

	<bean id="sample_pitch_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL20I-MO-TABLE-03:PITCH" />
		<property name="local" value="true" />
	</bean>

	<bean id="sample_roll_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL20I-MO-TABLE-03:ROLL" />
		<property name="local" value="true" />
	</bean>

	<bean id="sample_rot_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL20I-MO-TABLE-03:ROT" />
		<property name="local" value="true" />
	</bean>

	<bean id="sample_fine_rot_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL20I-MO-TABLE-03:ROTF" />
		<property name="local" value="true" />
	</bean>

	<bean id="filterwheel" class="gda.device.enumpositioner.EpicsEditablePositionerCallback">
		<property name="recordName" value="BL20I-OP-ATTN-06:MP" />
	</bean>

</beans>