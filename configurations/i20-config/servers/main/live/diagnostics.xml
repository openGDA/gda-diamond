		<property name="pvBase" value="BL20I-DI-PHDGN-06" />
		<property name="controlValues">
			<map>
				<entry key="in" value="Open" />
				<entry key="out" value="Close" />
			</map>
		</property>
		<property name="statusValues">
			<map>
				<entry key="Open" value="in" />
				<entry key="Closed" value="out" />
