<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<description>
	While the beamline is still being commissioned, we can add dummy objects which new UI parts
	would address in order to allow beamline staff to play with these parts in live mode without
	moving real hardware which they may still be carefully aligning etc.
	</description>
	
	<!--           energy workflow           -->
	<bean id="dummy_fe_shutter"  class="gda.device.enumpositioner.DummyEnumPositioner">
		<property name="positions">
			<list>
				<value>Open</value>
				<value>Close</value>
			</list>
		</property>
	</bean>
	<bean id="dummy_s1_xcentre" class="gda.device.scannable.DummyScannableMotor" />
	<bean id="dummy_s1_xsize" class="gda.device.scannable.DummyScannableMotor" />
	<bean id="dummy_s1_ycentre" class="gda.device.scannable.DummyScannableMotor" />
	<bean id="dummy_s1_ysize" class="gda.device.scannable.DummyScannableMotor" />
	<bean id="dummy_m1_x" class="gda.device.scannable.DummyScannableMotor" />
	<bean id="dummy_m1_y" class="gda.device.scannable.DummyScannableMotor" />
	<bean id="dummy_m1_roll" class="gda.device.scannable.DummyScannableMotor" />
	<bean id="dummy_m1_pitch" class="gda.device.scannable.DummyScannableMotor" />
	<bean id="dummy_m1_yaw" class="gda.device.scannable.DummyScannableMotor" />
	<bean id="dummy_m2_x" class="gda.device.scannable.DummyScannableMotor" />
	<bean id="dummy_m2_y" class="gda.device.scannable.DummyScannableMotor" />
	<bean id="dummy_m2_roll" class="gda.device.scannable.DummyScannableMotor" />
	<bean id="dummy_m2_pitch" class="gda.device.scannable.DummyScannableMotor" />
	<bean id="dummy_m2_yaw" class="gda.device.scannable.DummyScannableMotor" />
	<bean id="dummy_m2_bendus" class="gda.device.scannable.DummyScannableMotor" />
	<bean id="dummy_m2_bendds" class="gda.device.scannable.DummyScannableMotor" />
	<bean id="dummy_s2_xcentre" class="gda.device.scannable.DummyScannableMotor" />
	<bean id="dummy_s2_xsize" class="gda.device.scannable.DummyScannableMotor" />
	<bean id="dummy_s2_ycentre" class="gda.device.scannable.DummyScannableMotor" />
	<bean id="dummy_s2_ysize" class="gda.device.scannable.DummyScannableMotor" />
	<bean id="dummy_dcm1_y"  class="gda.device.enumpositioner.DummyEnumPositioner">
		<property name="positions">
			<list>
				<value>In</value>
				<value>Out</value>
			</list>
		</property>
	</bean>
	<bean id="dummy_dcm1_bragg" class="gda.device.scannable.DummyScannableMotor" />
	<bean id="dummy_dcm1_z" class="gda.device.scannable.DummyScannableMotor" />
	<bean id="dummy_dcm1_roll" class="gda.device.scannable.DummyScannableMotor" />
	<bean id="dummy_dcm1_pitch" class="gda.device.scannable.DummyScannableMotor" />
	<!--           energy workflow           -->
	
</beans>
