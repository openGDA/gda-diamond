<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:gda="http://www.diamond.ac.uk/schema/gda/gda"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
			http://www.diamond.ac.uk/schema/gda/gda http://www.diamond.ac.uk/schema/gda/gda/gda.xsd">

	<!-- Dummy motors until the motors are commissioned in EPICS -->
	<gda:motor id="tr6_y"
		liveClass="#dummy"
		live-minPosition="0"
		live-maxPosition="50.0"
		live-position="9.1"
		live-unitString="mm"
		dummy-minPosition="0"
		dummy-maxPosition="50.0"
		dummy-position="9.1"
		dummy-unitString="mm" />

	<gda:motor id="tr6_rot"
		liveClass="#dummy"
		live-minPosition="0"
		live-maxPosition="360"
		live-position="65"
		live-unitString="deg"
		live-speed="10000"
		dummy-minPosition="0"
		dummy-maxPosition="360"
		dummy-position="65"
		dummy-unitString="deg"
		dummy-speed="10000"/>

	<bean id="tr6" class="gda.device.scannable.scannablegroup.ScannableGroup">
		<property name="groupMembers">
			<list>
				<ref bean="tr6_y" />
				<ref bean="tr6_rot" />
			</list>
		</property>
	</bean>
</beans>
