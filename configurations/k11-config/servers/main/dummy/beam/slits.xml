<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="slit_x_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="7" />
		<property name="maxPosition" value="50" />
		<property name="position" value="10" />
		<property name="unitString" value="um" />
		<property name="local" value="true" />
	</bean>
	
	<bean id="slit_y_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="1" />
		<property name="maxPosition" value="50" />
		<property name="position" value="10" />
		<property name="unitString" value="um" />
		<property name="local" value="true" />
	</bean>
	
</beans>
