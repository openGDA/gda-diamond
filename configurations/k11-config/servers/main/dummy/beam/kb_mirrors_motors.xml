<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="kb_x_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-60" />
		<property name="maxPosition" value="60.0" />
		<property name="position" value="0" />
		<property name="unitString" value="mm" />
		<property name="speed" value="10000" />
	</bean>

	<bean id="kb_y_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-60" />
		<property name="maxPosition" value="60.0" />
		<property name="position" value="0" />
		<property name="unitString" value="mm" />
		<property name="speed" value="10000" />
	</bean>
	
</beans>
