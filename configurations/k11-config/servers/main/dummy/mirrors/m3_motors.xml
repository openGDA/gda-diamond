<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- COMBINED MOTION -->
	
	<bean id="m3_x_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-20" />
		<property name="maxPosition" value="15" />
		<property name="position" value="11.0005" />
		<property name="unitString" value="mm" />
	</bean>

	<bean id="m3_y_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-15" />
		<property name="maxPosition" value="15" />
		<property name="position" value="10.9997" />
		<property name="unitString" value="mm" />
	</bean>

	<bean id="m3_roll_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-4" />
		<property name="maxPosition" value="4" />
		<property name="position" value="0.0" />
		<property name="unitString" value="mrad" />
	</bean>

	<bean id="m3_pitch_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-25" />
		<property name="maxPosition" value="25" />
		<property name="position" value="-1.8025" />
		<property name="unitString" value="mrad" />
	</bean>

	<bean id="m3_yaw_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-0.75" />
		<property name="maxPosition" value="0.75" />
		<property name="position" value="-0.0002" />
		<property name="unitString" value="mrad" />
	</bean>

	<!-- RAW MOTION -->

	<bean id="m3_x1_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-21.3515" />
		<property name="maxPosition" value="18.6485" />
		<property name="position" value="12.0878" />
		<property name="unitString" value="mm" />
	</bean>

	<bean id="m3_x2_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-21.0588" />
		<property name="maxPosition" value="18.9412" />
		<property name="position" value="9.91325" />
		<property name="unitString" value="mm" />
	</bean>

	<bean id="m3_y1_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-15" />
		<property name="maxPosition" value="15" />
		<property name="position" value="11.0" />
		<property name="unitString" value="mm" />
	</bean>

	<bean id="m3_y2_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-15" />
		<property name="maxPosition" value="15" />
		<property name="position" value="10.9992" />
		<property name="unitString" value="mm" />
	</bean>

	<bean id="m3_y3_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-15.0" />
		<property name="maxPosition" value="15.0" />
		<property name="position" value="10.9998" />
		<property name="unitString" value="mm" />
	</bean>

</beans>
