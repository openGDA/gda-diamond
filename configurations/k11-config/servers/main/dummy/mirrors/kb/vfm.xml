<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="vfm_pitch_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-12.5" />
		<property name="maxPosition" value="12.5" />
		<property name="position" value="9.45" />
		<property name="unitString" value="mrad" />
	</bean>

	<bean id="vfm_bendus_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-12.5" />
		<property name="maxPosition" value="12.5" />
		<property name="position" value="9.45" />
		<property name="unitString" value="mm" />
	</bean>

	<bean id="vfm_bendds_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-12.5" />
		<property name="maxPosition" value="12.5" />
		<property name="position" value="9.45" />
		<property name="unitString" value="mm" />
	</bean>

</beans>