<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- COMBINED MOTION -->

	<bean id="m4_x_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL11K-OP-MR-04:X" />
	</bean>

	<bean id="m4_y_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL11K-OP-MR-04:Y" />
	</bean>

	<bean id="m4_roll_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL11K-OP-MR-04:ROLL" />
	</bean>

	<bean id="m4_pitch_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL11K-OP-MR-04:PITCH" />
	</bean>

	<bean id="m4_yaw_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL11K-OP-MR-04:YAW" />
	</bean>


	<!-- RAW MOTION -->

	<bean id="m4_x1_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL11K-OP-MR-04:X1" />
	</bean>

	<bean id="m4_x2_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL11K-OP-MR-04:X2" />
	</bean>

	<bean id="m4_y1_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL11K-OP-MR-04:Y1" />
	</bean>

	<bean id="m4_y2_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL11K-OP-MR-04:Y2" />
	</bean>

	<bean id="m4_y3_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL11K-OP-MR-04:Y3" />
	</bean>

	<bean id="m4_bendus_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL11K-OP-MR-04:BENDUS" />
	</bean>

	<bean id="m4_bendds_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL11K-OP-MR-04:BENDDS" />
	</bean>

</beans>
