<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="kb_cs_x_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="#{configProperties.KBM_PV_PREFIX}01:CS:X" />
	</bean>
	
	<bean id="kb_cs_y_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="#{configProperties.KBM_PV_PREFIX}01:CS:Y" />
	</bean>

</beans>