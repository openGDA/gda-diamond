<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="vfm_pitch_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL11K-OP-KBM-01:VFM:PITCH" />
	</bean>

	<bean id="vfm_bendus_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL11K-OP-KBM-01:VFM:BENDUS" />
	</bean>

	<bean id="vfm_bendds_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL11K-OP-KBM-01:VFM:BENDDS" />
	</bean>

</beans>