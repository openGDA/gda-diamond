<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="s4_xsize_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL11K-AL-SLITS-04:XSIZE" />
	</bean>

	<bean id="s4_xcentre_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL11K-AL-SLITS-04:XCENTRE" />
	</bean>

	<bean id="s4_ysize_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL11K-AL-SLITS-04:YSIZE" />
	</bean>

	<bean id="s4_ycentre_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL11K-AL-SLITS-04:YCENTRE" />
	</bean>
</beans>
