<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="tr6_y_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="0" />
		<property name="maxPosition" value="50.0" />
		<property name="position" value="9.1" />
		<property name="unitString" value="mm" />
		<property name="local" value="true" />
	</bean>
	
	<bean id="tr6_rot_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="0" />
		<property name="maxPosition" value="360" />
		<property name="position" value="65" />
		<property name="unitString" value="deg" />
		<property name="local" value="true" />
	</bean>

</beans>
