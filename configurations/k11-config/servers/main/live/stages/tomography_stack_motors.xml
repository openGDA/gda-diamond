<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="tomo_y_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="0" />
		<property name="maxPosition" value="50.0" />
		<property name="position" value="12.3" />
		<property name="unitString" value="mm" />
	</bean>
	
	<bean id="tomo_rot_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="0" />
		<property name="maxPosition" value="360.0" />
		<property name="position" value="30.0" />
		<property name="unitString" value="deg" />
	</bean>
	
	<bean id="tomo_centring_x_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-12.5" />
		<property name="maxPosition" value="12.5" />
		<property name="position" value="9.45" />
		<property name="unitString" value="mm" />
	</bean>
	
	<bean id="tomo_centring_z_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-15" />
		<property name="maxPosition" value="15" />
		<property name="position" value="-3.8" />
		<property name="unitString" value="mm" />
	</bean>

</beans>
