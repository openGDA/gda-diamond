<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="bs_x_axis" class="java.lang.String" factory-method="valueOf">
		<constructor-arg value="simx" />
	</bean>
	<bean id="bs_y_axis" class="java.lang.String" factory-method="valueOf">
		<constructor-arg value="simy" />
	</bean>
	<bean id="bs_mono_mri" class="java.lang.String" factory-method="valueOf">
		<constructor-arg value="#{workstation}-ML-SCAN-02" />
	</bean>
	<bean id="bs_pink_mri" class="java.lang.String" factory-method="valueOf">
		<constructor-arg value="#{workstation}-ML-SCAN-01" />
	</bean>
</beans>
