<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<!-- Stage axis names defined as String beans for convenience and to avoid repetition of the same literal value -->
	<bean id="x_axis_name" class="java.lang.String" factory-method="valueOf"><constructor-arg value="kb_x"/></bean>
	<bean id="y_axis_name" class="java.lang.String" factory-method="valueOf"><constructor-arg value="kb_y"/></bean>

</beans>
