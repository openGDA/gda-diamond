<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="eh_shutter_config" class="uk.ac.diamond.daq.beamline.k11.view.ShutterWidgetConfiguration">
		<property name="shutterName" value="eh_shutter" />
		<property name="shutterDisplayName" value="EH Shutter" />
		<property name="enablingScannableNamesAndMessages">
			<map>
				<entry key="eh1_searched_and_locked" value="Hutch not searched" />
			</map>
		</property>
	</bean>

</beans>
