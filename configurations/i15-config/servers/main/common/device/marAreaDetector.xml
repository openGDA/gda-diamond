<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="marAD_config_base" class="gda.spring.V17ADBaseFactoryBean">
		<property name="prefix" value="BL15I-EA-MAR-01:CAM:" />
	</bean>

	<bean id="marAD_summary_arr" class="gda.spring.V17NDArrayFactoryBean">
		<property name="prefix" value="BL15I-EA-MAR-01:ARR:" />
	</bean>

	<bean id="marAD_summary_stat" class="gda.spring.V17NDStatsFactoryBean">
		<property name="prefix" value="BL15I-EA-MAR-01:STAT1:" />
	</bean>

	<bean id="marAD_summary_roi" class="gda.spring.V17NDROIFactoryBean">
		<property name="prefix" value="BL15I-EA-MAR-01:ROI1:" />
	</bean>

	<bean id="marAD_summary_mjpg" class="gda.spring.V17FfmpegStreamFactoryBean">
		<property name="prefix" value="BL15I-EA-MAR-01:MJPG2:" />
	</bean>

	<bean id="marAD_summary_proc" class="gda.spring.V17NDProcessFactoryBean">
		<property name="prefix" value="BL15I-EA-MAR-01:PROC1:" />
	</bean>
