<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="pil3AD_config_base"
			class="gda.spring.V17ADBaseFactoryBean">
		<property name="prefix" value="BL15I-EA-PILAT-03:CAM:" />
	</bean>

	<bean id="pil3AD_summary_arr"
			class="gda.spring.V17NDArrayFactoryBean">
		<property name="prefix" value="BL15I-EA-PILAT-03:ARR:" />
	</bean>

	<bean id="pil3AD_summary_stat"
			class="gda.spring.V17NDStatsFactoryBean">
		<property name="prefix" value="BL15I-EA-PILAT-03:STAT:" />
	</bean>

	<bean id="pil3AD_summary_roi"
			class="gda.spring.V17NDROIFactoryBean">
		<property name="prefix" value="BL15I-EA-PILAT-03:ROI:" />
	</bean>

	<bean id="pil3AD_summary_mjpg"
			class="gda.spring.V17FfmpegStreamFactoryBean">
		<property name="prefix" value="BL15I-EA-PILAT-03:MJPG:" />
	</bean>

	<bean id="pil3AD_summary_proc"
			class="gda.spring.V17NDProcessFactoryBean">
		<property name="prefix" value="BL15I-EA-PILAT-03:PROC:" />
	</bean>
</beans>
