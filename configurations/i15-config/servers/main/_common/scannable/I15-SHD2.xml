<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="sh2dx" class="gda.device.scannable.ScannableMotor">
		<property name="motor" ref="sh2dx_motor" />
	</bean>

	<bean id="sh2dy" class="gda.device.scannable.ScannableMotor">
		<property name="motor" ref="sh2dy_motor" />
	</bean>

	<bean id="sh2dz" class="gda.device.scannable.ScannableMotor">
		<property name="motor" ref="sh2dz_motor" />
	</bean>

	<bean id="SHD2" class="gda.device.scannable.scannablegroup.ScannableGroup">
		<property name="groupMembers">
			<list>
				<ref bean="sh2dx" />
				<ref bean="sh2dy" />
				<ref bean="sh2dz" />
			</list>
		</property>
	</bean>
</beans>