<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="DXMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL15I-MO-DIFF-01:SAMPLE:X" />
	</bean>

	<bean id="DYMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL15I-MO-DIFF-01:SAMPLE:Y" />
	</bean>

	<bean id="DZMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL15I-MO-DIFF-01:SAMPLE:Z" />
	</bean>

	<bean id="DkphiMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL15I-MO-DIFF-01:SAMPLE:KPHI" />
	</bean>

	<bean id="Dr1kappaMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL15I-MO-DIFF-01:SAMPLE:KAPPA" />
	</bean>

	<beans profile='dktheta_available'>
		<bean id="Dr1kthetaMotor" class="gda.device.motor.EpicsMotor">
			<property name="pvName" value="BL15I-MO-DIFF-01:SAMPLE:KTHETA" />
		</bean>

		<bean id="Dr1muMotor" class="gda.device.motor.EpicsMotor">
			<property name="pvName" value="BL15I-MO-DIFF-01:SAMPLE:MU" />
		</bean>
	</beans>

	<beans profile='!dktheta_available'>
		<bean id="Dr1kthetaMotor" class="gda.device.motor.DummyMotor"/>
		<bean id="Dr1muMotor" class="gda.device.motor.DummyMotor"/>
	</beans>

</beans>
