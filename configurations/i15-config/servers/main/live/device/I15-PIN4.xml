<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="Pin4XMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL15I-AL-APTR-04:X" />
		<property name="local" value="true"/>
	</bean>

	<bean id="Pin4YMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL15I-AL-APTR-04:Y" />
		<property name="local" value="true"/>
	</bean>

	<bean id="Pin4ZMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL15I-AL-APTR-04:Z" />
		<property name="local" value="true"/>
	</bean>

	<bean id="Pin4PitchMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL15I-AL-APTR-04:PITCH" />
		<property name="local" value="true"/>
	</bean>

	<bean id="Pin4YawMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL15I-AL-APTR-04:YAW" />
		<property name="local" value="true"/>
	</bean>
</beans>
