<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="bs3x_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="bs3.x" />
		<property name="local" value="true"/>
	</bean>

	<bean id="bs3y_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="bs3.y" />
		<property name="local" value="true"/>
	</bean>

	<bean id="bs3z_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="bs3.z" />
		<property name="local" value="true"/>
	</bean>
</beans>
