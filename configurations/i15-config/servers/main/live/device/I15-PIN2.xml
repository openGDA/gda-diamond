<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="Pin2XMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL15I-AL-APTR-02:X" />
	</bean>

	<bean id="Pin2YMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL15I-AL-APTR-02:Y" />
	</bean>

	<bean id="Pin2ZMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL15I-AL-APTR-02:Z" />
	</bean>

	<bean id="Pin2PitchMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL15I-AL-APTR-02:PITCH" />
	</bean>

	<bean id="Pin2YawMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL15I-AL-APTR-02:YAW" />
	</bean>
</beans>
