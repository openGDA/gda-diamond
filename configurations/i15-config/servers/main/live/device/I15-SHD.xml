<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="shdx_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="SHEAD.X" />
		<property name="local" value="true"/>
	</bean>

	<bean id="shdy_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="SHEAD.Y" />
		<property name="local" value="true"/>
	</bean>

	<bean id="shdz_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="SHEAD.Z" />
		<property name="local" value="true"/>
	</bean>
</beans>
