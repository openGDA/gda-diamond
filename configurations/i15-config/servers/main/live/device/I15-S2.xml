<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="S2XGapMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL15I-AL-SLITS-02:X:SIZE" />
		<property name="local" value="true"/>
	</bean>	

	<bean id="S2XPosMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL15I-AL-SLITS-02:X:CENTER" />
		<property name="local" value="true"/>
	</bean>	

	<bean id="S2YGapMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL15I-AL-SLITS-02:Y:SIZE" />
		<property name="local" value="true"/>
	</bean>	

	<bean id="S2YPosMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL15I-AL-SLITS-02:Y:CENTER" />
		<property name="local" value="true"/>
	</bean>	
</beans>
