<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="HFMxMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL15I-OP-HFM-01:X" />
	</bean>

	<bean id="HFMyMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL15I-OP-HFM-01:Y" />
	</bean>

	<bean id="HFMpitchMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL15I-OP-HFM-01:PITCH" />
	</bean>

	<bean id="HFMyawMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL15I-OP-HFM-01:YAW" />
	</bean>

	<bean id="HFMrollMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL15I-OP-HFM-01:ROLL" />
	</bean>

	<bean id="HFMcurveMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL15I-OP-HFM-01:CURVE" />
	</bean>

	<bean id="HFMellipticityMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL15I-OP-HFM-01:ELLIP" />
	</bean>
</beans>
