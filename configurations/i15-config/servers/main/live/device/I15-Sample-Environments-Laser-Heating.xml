	</bean>

	<bean id="armds_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL15I-MO-LHEAT-01:ARM:DS" />
		<property name="local" value="true" />
	</bean>

	<bean id="focus_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL15I-MO-LHEAT-01:LASFOC:US" />
