	</bean>

	<bean id="laserxus_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL15I-MO-LHEAT-01:LASX:US" />
		<property name="local" value="true" />
	</bean>

	<bean id="laserxds_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL15I-MO-LHEAT-01:LASX:DS" />
		<property name="local" value="true" />
	</bean>

	<bean id="laseryus_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL15I-MO-LHEAT-01:LASY:US" />
