<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="sh2dx_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="SHEAD2.X" />
		<property name="local" value="true"/>
	</bean>

	<bean id="sh2dy_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="SHEAD2.Y" />
		<property name="local" value="true"/>
	</bean>

	<bean id="sh2dz_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="SHEAD2.Z" />
		<property name="local" value="true"/>
	</bean>
</beans>
