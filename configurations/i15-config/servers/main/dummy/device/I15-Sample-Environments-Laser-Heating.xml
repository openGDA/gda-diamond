<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- Fiber Laser -->

	<bean id="laserpowerus"
			class="gda.device.scannable.DummyScannable">
		<property name="name" value="laserpowerus"/>
	</bean>

	<bean id="laserpowerds"
			class="gda.device.scannable.DummyScannable">
		<property name="name" value="laserpowerds"/>
	</bean>

	<!-- Huber motion -->

	<!-- Smaract Motion -->

	<!-- Note, this is also on the Newport motors EDM -->
	<bean id="laseryus_motor"
			class="gda.device.motor.DummyMotor"/>

	<!-- Note, this is also on the Newport motors EDM -->
	<bean id="laseryds_motor"
			class="gda.device.motor.DummyMotor"/>

	<!-- Horiba Spectrum Analyser -->

	<!-- See I15-214 & http://confluence.diamond.ac.uk/x/rgGwAg
	 -->
	<bean id="laserstartacqproc"
			class="gda.device.scannable.DummyScannable">
		<property name="name" value="laserstartacqproc"/>
	</bean>

	<!-- Newport Motors -->

</beans>