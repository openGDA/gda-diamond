<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:util="http://www.springframework.org/schema/util"
	xmlns:corba="http://www.diamond.ac.uk/schema/gda/corba"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
		http://www.springframework.org/schema/util
		http://www.springframework.org/schema/util/spring-util-2.0.xsd
		http://www.diamond.ac.uk/schema/gda/corba
		http://www.diamond.ac.uk/schema/gda/corba/gda-corba-1.0.xsd">

	<bean
		class="org.springframework.beans.factory.config.PropertyPlaceholderConfigurer">
		<property name="locations">
			<list value-type="java.lang.String">
				<value>file:${gda.config}/properties/dummy/java.properties</value>
				<value>file:${gda.config}/properties/common/java.properties</value>
			</list>
		</property>
	</bean>

	<bean id="terminallog_path_provider" class="gda.data.SimpleObservablePathProvider">
		<property name="local" value="true" />
	</bean>

	<import resource="device/all.xml"/>
	<import resource="transient/all.xml"/>
	<import resource="../common/server.xml"/>

	<import resource="metadata.xml"/>

	<!-- Dummy FileRegistrar? -->

	<bean id="Ring" class="gda.spring.FindableEpicsDeviceFactoryBean">
		<property name="recordPvs">
			<map>
				<entry key="Top" value="" />
				<entry key="Current" value="SR-DI-DCCT-01:SIGNAL" />
				<entry key="FastCurrent" value="SR21C-DI-DCCT-01:SIGNAL" />
				<entry key="TimeUntilRefill" value="SR-CS-FILL-01:COUNTDOWN" />
				<entry key="BeamMode" value="CS-CS-MSTAT-01:MODE" />
			</map>
		</property>
		<property name="dummyMode" value="true"/>
	</bean>

	<!-- id="prop" not available in dummy -->	

	<!-- id="ODCCD" not available in dummy -->	
	<!-- id="Mar345Detector" not available in dummy -->	

	<!-- corba:export not needed in dummy -->

	<!-- SingletonRegistrationPostProcessor not needed in dummy-->
</beans>