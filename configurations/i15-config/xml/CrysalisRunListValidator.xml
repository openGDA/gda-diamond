<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<configuration>
<!-- domegaindeg = dktheta + 90 -->
<min_domegaindeg>70</min_domegaindeg>
<max_domegaindeg>111.1</max_domegaindeg>
<!-- ddetectorindeg = ddelta - 2theta -->
<min_ddetectorindeg>-10.10</min_ddetectorindeg>
<max_ddetectorindeg>31.0</max_ddetectorindeg>
<!-- dkappaindeg = dkappa -->
<min_dkappaindeg>-10.5</min_dkappaindeg>
<max_dkappaindeg>0.1</max_dkappaindeg>
<!-- dphiindeg = dkphi for now ignore as we only perform phi scans-->
<min_dphiindeg>-180.1</min_dphiindeg>
<max_dphiindeg>180.1</max_dphiindeg>
<!-- these depends on the scantype  - for now jsut use phi scans -->
<min_dscanstartindeg>-126.</min_dscanstartindeg>
<max_dscanstartindeg>-116.</max_dscanstartindeg>
<min_dscanendindeg>-126.</min_dscanendindeg>
<max_dscanendindeg>-116.</max_dscanendindeg>
<min_dscanwidthindeg>-1.0</min_dscanwidthindeg>
<max_dscanwidthindeg>1.0</max_dscanwidthindeg>
<!-- speedratio ? -->
<min_dscanspeedratio>-1.0</min_dscanspeedratio>
<max_dscanspeedratio>1.0</max_dscanspeedratio>
<min_dexposuretimeinsec>0.01</min_dexposuretimeinsec>
<max_dexposuretimeinsec>60.0</max_dexposuretimeinsec>
</configuration>
