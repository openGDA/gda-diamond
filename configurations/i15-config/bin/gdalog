#!/bin/bash

LOGPANEL=$(ps -ef | grep LogPanel | grep -v grep)
if [ "X$LOGPANEL" == "X" ] ; then
  . $(dirname "$0")/GDA_common gda_log_output

  echo Starting GDA Log Panel...
  echo  $GDA_CORE_SCRIPT logpanel
  echo  $GDA_CORE_SCRIPT logpanel >> $LOGFILE
  nohup $GDA_CORE_SCRIPT logpanel >> $LOGFILE 2>&1 &

else
  echo Raising GDA Log Panel...
  wmctrl -R "GDA Log Panel"
fi