<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

	<!-- S5 Motors (individual Motions) -->
	<bean id="user_motor1" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL11I-MO-USER-01:MTR1"/>
	</bean>
	<bean id="user_motor3" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL11I-MO-USER-01:MTR2"/>
	</bean>
	<bean id="user_motor4" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL11I-MO-USER-01:MTR4"/>
	</bean>
</beans>
