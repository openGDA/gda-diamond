<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<!-- Large sample table TL Motors (individual Motions) -->
	<bean id="tlx_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="TL.X"/>
	</bean>
	<bean id="tly_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="TL.Y"/>
	</bean>
	<bean id="tlz_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="TL.Z"/>
	</bean>
</beans>
