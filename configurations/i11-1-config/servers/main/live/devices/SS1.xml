<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

	<bean id="ss1x_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL11J-MO-SSLID-01:X"/>
	</bean>
	<bean id="ss1y_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL11J-MO-SSLID-01:Y"/>
	</bean>


	<bean id="ss1_1_temp" class="gda.device.monitor.EpicsMonitor">
		<property name="pvName" value="BL11J-EA-SCL-01:1:TEMP" />
		<property name="local" value="true" />
		<property name="unit" value="C" />
	</bean>
	<bean id="ss1_1_humidity" class="gda.device.monitor.EpicsMonitor">
		<property name="pvName" value="BL11J-EA-SCL-01:1:HUMIDITY" />
		<property name="local" value="true" />
		<property name="unit" value="%" />
	</bean>
	<bean id="ss1_2_temp" class="gda.device.monitor.EpicsMonitor">
		<property name="pvName" value="BL11J-EA-SCL-01:2:TEMP" />
		<property name="local" value="true" />
		<property name="unit" value="C" />
	</bean>
	<bean id="ss1_2_humidity" class="gda.device.monitor.EpicsMonitor">
		<property name="pvName" value="BL11J-EA-SCL-01:2:HUMIDITY" />
		<property name="local" value="true" />
		<property name="unit" value="%" />
	</bean>
	<bean id="ss1_3_temp" class="gda.device.monitor.EpicsMonitor">
		<property name="pvName" value="BL11J-EA-SCL-01:3:TEMP" />
		<property name="local" value="true" />
		<property name="unit" value="C" />
	</bean>
	<bean id="ss1_3_humidity" class="gda.device.monitor.EpicsMonitor">
		<property name="pvName" value="BL11J-EA-SCL-01:3:HUMIDITY" />
		<property name="local" value="true" />
		<property name="unit" value="%" />
	</bean>

</beans>
