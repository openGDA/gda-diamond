<?xml version='1.0' encoding='UTF-8'?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="dcm_bragg_motor" class="gda.device.motor.EpicsMotor" >
		<property name="pvName" value="BL11I-MO-DCM-01:BRAGG" />
	</bean>
	<bean id="dcm_gap_motor" class="gda.device.motor.EpicsMotor" >
		<property name="pvName" value="BL11I-MO-DCM-01:PERP" />
	</bean>
	<bean id="dcm_pitch_motor" class="gda.device.motor.EpicsMotor" >
		<property name="pvName" value="BL11I-MO-DCM-01:PITCH" />
	</bean>
	<bean id="dcm_roll_motor" class="gda.device.motor.EpicsMotor" >
		<property name="pvName" value="BL11I-MO-DCM-01:ROLL" />
	</bean>
	<bean id="dcm_energy_motor" class="gda.device.motor.EpicsMotor" >
		<property name="pvName" value="BL11I-MO-DCM-01:ENERGY" />
	</bean>
	<bean id="dcm_offset_motor" class="gda.device.motor.EpicsMotor" >
		<property name="pvName" value="BL11I-MO-DCM-01:OFFSET" />
	</bean>
	

</beans>
