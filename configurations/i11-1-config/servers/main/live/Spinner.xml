<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

	<bean id="spin" class="gda.device.spin.Spin">
		<property name="enablePV" value="BL11J-EA-MCL-02:DISABLE"/>
		<property name="speedPV" value="BL11J-EA-MCL-02:SPEED"/>
	</bean>
	<bean class="uk.ac.gda.remoting.server.GdaRmiServiceExporter">
		<property name="serviceName" value="gda/spin" />
		<property name="service" ref="spin" />
		<property name="serviceInterface" value="gda.device.ISpin" />
	</bean>

</beans>
