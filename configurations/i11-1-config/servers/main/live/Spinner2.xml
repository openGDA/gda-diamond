<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="spin2" class="gda.device.spin.Spin">
		<property name="enableName" value="SPIN.ENABLE"/>
		<property name="speedName" value="SPIN.SPEED"/>
	</bean>
	<bean class="uk.ac.gda.remoting.server.GdaRmiServiceExporter">
		<property name="serviceName" value="gda/spin2" />
		<property name="service" ref="spin2" />
		<property name="serviceInterface" value="gda.device.ISpin" />
	</bean>

</beans>
