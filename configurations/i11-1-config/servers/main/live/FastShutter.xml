<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

	<bean id="fastshutter" class="gda.device.enumpositioner.EpicsSimpleMbbinary">
		<property name="deviceName" value="ENV.SHTR"/>
	</bean>
	<bean id="fastshutter1" class="gda.device.enumpositioner.EpicsSimpleMbbinary">
		<property name="recordName" value="BL11J-MO-FSHTR-01:SHUTTER"/>
		<property name="readOnly" value="false"/>
	</bean>
	<bean id="fastshutter2" class="gda.device.enumpositioner.EpicsSimpleMbbinary">
		<property name="recordName" value="BL11J-MO-FSHTR-02:SHUTTER"/>
		<property name="readOnly" value="false"/>
	</bean>

</beans>
