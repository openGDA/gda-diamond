<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="enabler_controller" class="gda.device.spin.EnablerController">
		<property name="deviceName" value="SPIN.ENABLE"/>
		<property name="local" value="true"/>
	</bean>
	<bean id="speed_controller" class="gda.device.spin.SpeedController">
		<property name="deviceName" value="SPIN.SPEED"/>
		<property name="local" value="true"/>
	</bean>

</beans>
