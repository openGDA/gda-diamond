<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

	<bean id="bpm2posn" class="gda.spring.EpicsPositionerFactoryBean">
		<property name="deviceName" value="BPM2.POSN"/>
	</bean>
	<bean id="bpm2_amp" class="gda.device.enumpositioner.EpicsCurrAmpQuadController">
		<property name="deviceName" value="BPM2.AMP"/>
		<property name="poll" value="true"/>
	</bean>
	<bean id="bpm2_bpm" class="gda.device.monitor.EpicsBpmController">
		<property name="deviceName" value="BPM2.BPM"/>
	</bean>
</beans>
