<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

	<!-- MAC stage 1 Motors -->
	<bean id="mac1a_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="MAC1.A"/>
	</bean>
	<bean id="mac12a_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="MAC1.2A"/>
	</bean>
	<bean id="mac1trans_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="MAC1.TRANS"/>
	</bean>
	<!-- MAC stage 2 Motors -->
	<bean id="mac2a_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="MAC2.A"/>
	</bean>
	<bean id="mac22a_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="MAC2.2A"/>
	</bean>
	<bean id="mac2trans_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="MAC2.TRANS"/>
	</bean>
	<!-- MAC stage 31 Motors -->
	<bean id="mac3a_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="MAC3.A"/>
	</bean>
	<bean id="mac32a_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="MAC3.2A"/>
	</bean>
	<bean id="mac3trans_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="MAC3.TRANS"/>
	</bean>
	<!-- MAC stage 4 Motors -->
	<bean id="mac4a_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="MAC4.A"/>
	</bean>
	<bean id="mac42a_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="MAC4.2A"/>
	</bean>
	<bean id="mac4trans_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="MAC4.TRANS"/>
	</bean>
	<!-- MAC stage 5 Motors -->
	<bean id="mac5a_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="MAC5.A"/>
	</bean>
	<bean id="mac52a_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="MAC5.2A"/>
	</bean>
	<bean id="mac5trans_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="MAC5.TRANS"/>
	</bean>
</beans>
