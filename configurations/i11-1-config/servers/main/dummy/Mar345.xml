	<bean id="tmar_z_mtr" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="TMAR.Z"/>
	</bean>
	<bean id="bmar_slitx_mtr" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="BMAR.SLITX"/>
	</bean>
	<bean id="bmar_slity_mtr" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="BMAR.SLITY"/>
