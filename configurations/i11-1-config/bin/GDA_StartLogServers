#!/bin/bash
. "/$SOFTWAREFOLDER/$BEAMLINE/software/gda/config/bin/gda_environment.sh"

if [ -f "/etc/profile.d/modules.sh" ]; then
	. /etc/profile.d/modules.sh	
	module load java/gda844
else
	echo "cannot scd ..ource /etc/profile.d/modules.sh"	
	exit 1
fi

echo "starting GDA Log Server"
echo `dirname $0`/gda_command logserver --restart $@
`dirname $0`/gda_command logserver --restart $@