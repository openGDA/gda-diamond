		<property name="unitString" value="mm"/>
	</bean>

	<bean id="MotorZ_S4" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-1.0000000" />
		<property name="maxPosition" value="175.010000" />
		<property name="speed" value="1" />
		<property name="unitString" value="mm"/>
	</bean>

	<bean id="MotorXSize_S4" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-0.30283" />
		<property name="maxPosition" value="3.6982" />
		<property name="speed" value="1" />
		<property name="unitString" value="mm"/>
	</bean>

	<bean id="MotorYSize_S4" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-10.0602" />
		<property name="maxPosition" value="3.9408" />
