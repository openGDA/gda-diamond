<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- Exit Slits S4 Motors -->
	<bean id="MotorXGap_S4" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="3" />
		<property name="maxPosition" value="15.1214" />
		<property name="speed" value="1" />
		<property name="unitString" value="mm"/>
	</bean>

	<bean id="MotorYGap_S4" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-16.9570" />
		<property name="maxPosition" value="-1.4570" />
