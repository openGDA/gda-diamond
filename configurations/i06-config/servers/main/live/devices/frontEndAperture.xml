<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- Front End: Custom Aperture Epics Motors -->
	<bean id="MotorX_FrontEndAperture" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="FEAPX" />
		<property name="local" value="true"/>
	</bean>

	<bean id="MotorY_FrontEndAperture" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="FEAPY" />
		<property name="local" value="true"/>
	</bean>

</beans>
