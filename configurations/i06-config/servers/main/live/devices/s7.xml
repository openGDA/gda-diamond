<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- Branchline Cleanup Slits S7 Motors -->
	<bean id="MotorXPos_S7" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S7.XPOS" />
		<property name="local" value="true"/>
	</bean>

	<bean id="MotorXNeg_S7" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S7.XNEG" />
		<property name="local" value="true"/>
	</bean>

	<bean id="MotorYPos_S7" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S7.YPOS" />
		<property name="local" value="true"/>
	</bean>

	<bean id="MotorYNeg_S7" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S7.YNEG" />
		<property name="local" value="true"/>
	</bean>

	<bean id="MotorXSize_S7" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S7.XSIZE" />
		<property name="local" value="true"/>
	</bean>

	<bean id="MotorXCentre_S7" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S7.XCENTRE" />
		<property name="local" value="true"/>
	</bean>

	<bean id="MotorYSize_S7" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S7.YSIZE" />
		<property name="local" value="true"/>
	</bean>

	<bean id="MotorYCentre_S7" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S7.YCENTRE" />
		<property name="local" value="true"/>
	</bean>

</beans>
