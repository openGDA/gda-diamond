<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- PEEM line PEEM Manipulating Stage Motors -->
	<bean id="MotorPEEMStage_X" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="PEEM.X" />
		<property name="local" value="true" />
	</bean>

	<bean id="MotorPEEMStage_Y" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="PEEM.Y" />
		<property name="local" value="true" />
	</bean>
	<bean id="MotorPEEMStage_phi" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="PEEM.Phi" />
		<property name="local" value="true" />
	</bean>

</beans>
