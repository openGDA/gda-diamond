<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- D7 Fluorescent Screen Pneumatic Valve -->
	<bean id="d7valve" class="gda.device.enumpositioner.EpicsPneumaticCallback">
		<property name="deviceName" value="D7" />
		<property name="local" value="true"/>
	</bean>
	<!-- Fluorescent Screen and Diode D7 Motors -->
	<bean id="MotorD_D7" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="D7.X" />
		<property name="local" value="true"/>
	</bean>

	<!-- Fluorescent Screen and Diode D7a Motors -->
	<bean id="MotorD_D7a" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="D7a.X" />
		<property name="local" value="true"/>
	</bean>

	
</beans>
