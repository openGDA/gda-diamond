<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- X-Ray Beam Position Monitor in the Ring -->
	<bean id="xbpm2x" class="gda.device.monitor.EpicsMonitor">
		<property name="pvName" value="FE06I-DI-PBPM-02:BEAMX" />
		<property name="local" value="true"/>
	</bean>

	<bean id="xbpm2y" class="gda.device.monitor.EpicsMonitor">
		<property name="pvName" value="FE06I-DI-PBPM-02:BEAMY" />
		<property name="local" value="true"/>
	</bean>

	<!-- FE Photon Beam Position Monitor XPBM1 -->
	<bean id="pm2a" class="gda.device.monitor.EpicsMonitor">
		<property name="deviceName" value="PBPM2.BLADEA" />
		<property name="local" value="true"/>
	</bean>

	<bean id="pm2b" class="gda.device.monitor.EpicsMonitor">
		<property name="deviceName" value="PBPM2.BLADEB" />
		<property name="local" value="true"/>
	</bean>

	<bean id="pm2c" class="gda.device.monitor.EpicsMonitor">
		<property name="deviceName" value="PBPM2.BLADEC" />
		<property name="local" value="true"/>
	</bean>

	<bean id="pm2d" class="gda.device.monitor.EpicsMonitor">
		<property name="deviceName" value="PBPM2.BLADED" />
		<property name="local" value="true"/>
	</bean>

	<bean id="pm2x" class="gda.device.monitor.EpicsMonitor">
		<property name="deviceName" value="PBPM2.X" />
		<property name="local" value="true"/>
	</bean>

	<bean id="pm2y" class="gda.device.monitor.EpicsMonitor">
		<property name="deviceName" value="PBPM2.Y" />
		<property name="local" value="true"/>
	</bean>

	<bean id="pm2xabs" class="gda.device.monitor.EpicsMonitor">
		<property name="deviceName" value="PBPM2.XABS" />
		<property name="local" value="true"/>
	</bean>

	<bean id="pm2yabs" class="gda.device.monitor.EpicsMonitor">
		<property name="deviceName" value="PBPM2.YABS" />
		<property name="local" value="true"/>
	</bean>


</beans>
