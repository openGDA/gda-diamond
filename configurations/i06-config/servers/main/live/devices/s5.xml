<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- Cleanup Slits S5 Motors -->
	<bean id="MotorXPos_S5" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL06I-AL-SLITS-05:X:POS" />
	</bean>

	<bean id="MotorXNeg_S5" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL06I-AL-SLITS-05:X:NEG" />
	</bean>

	<bean id="MotorYPos_S5" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL06I-AL-SLITS-05:Y:POS" />
	</bean>

	<bean id="MotorYNeg_S5" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL06I-AL-SLITS-05:Y:NEG" />
	</bean>

	<bean id="MotorXSize_S5" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL06I-AL-SLITS-05:X:SIZE" />
	</bean>

	<bean id="MotorXCentre_S5" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL06I-AL-SLITS-05:X:CENTER" />
	</bean>

	<bean id="MotorYSize_S5" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL06I-AL-SLITS-05:Y:SIZE" />
	</bean>

	<bean id="MotorYCentre_S5" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL06I-AL-SLITS-05:Y:CENTER" />
	</bean>

</beans>