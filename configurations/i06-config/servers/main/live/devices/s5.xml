<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- Cleanup Slits S5 Motors -->
	<bean id="MotorXPos_S5" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S5.XPOS" />
		<property name="local" value="true"/>
	</bean>

	<bean id="MotorXNeg_S5" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S5.XNEG" />
		<property name="local" value="true"/>
	</bean>

	<bean id="MotorYPos_S5" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S5.YPOS" />
		<property name="local" value="true"/>
	</bean>

	<bean id="MotorYNeg_S5" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S5.YNEG" />
		<property name="local" value="true"/>
	</bean>

	<bean id="MotorXSize_S5" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S5.XSIZE" />
		<property name="local" value="true"/>
	</bean>

	<bean id="MotorXCentre_S5" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S5.XCENTRE" />
		<property name="local" value="true"/>
	</bean>

	<bean id="MotorYSize_S5" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S5.YSIZE" />
		<property name="local" value="true"/>
	</bean>

	<bean id="MotorYCentre_S5" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S5.YCENTRE" />
		<property name="local" value="true"/>
	</bean>

</beans>
