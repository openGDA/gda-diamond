<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="esz_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-0.50000" />
		<property name="maxPosition" value="13.19000" />
		<property name="speed" value="1" />
		<property name="unitString" value="mm"/>
	</bean>
	<bean id="caz_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-0.50000" />
		<property name="maxPosition" value="13.19000" />
		<property name="speed" value="1" />
		<property name="unitString" value="mm"/>
	</bean>
	<bean id="catilt_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-0.50000" />
		<property name="maxPosition" value="13.19000" />
		<property name="speed" value="1" />
		<property name="unitString" value="mm"/>
	</bean>
	
	<bean id="ES" class="gda.device.enumpositioner.DummyEnumPositioner">
		<property name="positions">
			<list>
				<value>1000</value>
				<value>125</value>
				<value>60</value>
				<value>25</value>
				<value>12.5</value>
			</list>			
		</property>
	</bean>
	<bean id="CA" class="gda.device.enumpositioner.DummyEnumPositioner">
		<property name="positions">
			<list>
				<value>out</value>
				<value>1000</value>
				<value>70</value>
				<value>60</value>
				<value>50</value>
				<value>30</value>
				<value>20</value>
				<value>10</value>
			</list>			
		</property>
