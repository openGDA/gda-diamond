<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="sample_x_coarse_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-1.5" />
		<property name="maxPosition" value="27.5" />
		<property name="position" value="11.3213" />
		<property name="timeToVelocity" value="0.01" />
	 	<property name="unitString" value="mm"/>
	 	<property name="homeable" value="true"/>
	</bean>

	<bean id="sample_y_coarse_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="0.0" />
		<property name="maxPosition" value="2.5" />
		<property name="position" value="2.19317" />
		<property name="timeToVelocity" value="0.01" />
	 	<property name="unitString" value="mm"/>
	 	<property name="homeable" value="true"/>
	</bean>

	<bean id="sample_x_fine_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-1000" />
		<property name="maxPosition" value="1000" />
		<property name="position" value="0.021969" />
		<property name="timeToVelocity" value="0.01" />
	 	<property name="unitString" value="mm"/>
	</bean>

	<bean id="sample_y_fine_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-1000" />
		<property name="maxPosition" value="1000" />
		<property name="position" value="-0.01456" />
		<property name="timeToVelocity" value="0.01" />
	 	<property name="unitString" value="mm"/>
	</bean>

</beans>
