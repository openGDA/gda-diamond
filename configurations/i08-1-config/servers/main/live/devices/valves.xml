	</bean>
	
	<bean id="gv12" class="gda.device.enumpositioner.EpicsPneumaticCallback">
		<property name="controlPv" value="BL08I-VA-VALVE-12:CON" />
		<property name="statusPv" value="BL08I-VA-VALVE-12:STA" />
