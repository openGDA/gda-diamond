<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="sample_x_coarse_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL08J-EA-TABLE-01:XC" />
	</bean>

	<bean id="sample_y_coarse_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL08J-EA-TABLE-01:YC" />
	</bean>

	<bean id="sample_x_fine_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL08J-EA-TABLE-01:XF" />
	</bean>

	<bean id="sample_y_fine_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL08J-EA-TABLE-01:YF" />
	</bean>

</beans>
