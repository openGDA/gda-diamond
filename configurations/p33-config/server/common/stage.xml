<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:gda="http://www.diamond.ac.uk/schema/gda/gda"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
						http://www.diamond.ac.uk/schema/gda/gda http://www.diamond.ac.uk/schema/gda/gda/gda-gda-1.2.xsd">

	<gda:motor id="stage_x"
		live-pvName="BL33P-MO-STEP-02:AXIS8"
		dummy-minPosition="-20"
		dummy-maxPosition="20"
		dummy-position="2.339"
		dummy-unitString="mm"/>

	<gda:motor id="stage_y"
		live-pvName="BL33P-MO-STEP-02:AXIS7"
		dummy-minPosition="-20"
		dummy-maxPosition="20"
		dummy-position="2.339"
		dummy-unitString="mm"/>
	
	<gda:motor id="samdy"
		live-pvName="BL33P-MO-PPMAC-01:LIN2" /> <!-- Kollmorgen linear -->
		
	<gda:motor id="camy"
		live-pvName="BL33P-MO-PPMAC-01:LIN1" /> <!-- Newport 100mm -->
	
	<gda:motor id="samdx"
		live-pvName="BL33P-MO-PPMAC-01:LIN3" /> <!-- Newport 150mm 1 -->
	
	<gda:motor id="lasery"
		live-pvName="BL33P-MO-PPMAC-01:LIN4" /> <!-- Newport 150mm 2 -->
	
	<gda:motor id="omega"
		live-pvName="BL33P-MO-PPMAC-01:OMEGA" /> <!-- Thorlab rotation -->

</beans>
