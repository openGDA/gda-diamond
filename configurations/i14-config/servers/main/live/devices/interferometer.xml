<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">


	<bean id="stage1_x_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL14I-MO-STAGE-01:XC" />
		<property name="local" value="true" />
	</bean>

	<bean id="stage1_y_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL14I-MO-STAGE-01:YC" />
		<property name="local" value="true" />
	</bean>

	<bean id="stage1_z_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL14I-MO-STAGE-01:ZC" />
		<property name="local" value="true" />
	</bean>

	<bean id="intf_x_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL14I-OP-INTF-01:X" />
		<property name="local" value="true" />
	</bean>

	<bean id="intf_y_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL14I-OP-INTF-01:Y" />
		<property name="local" value="true" />
	</bean>

</beans>