<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="sjj_x_centre_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL14I-AL-SJJ-01:X:CENTRE" />
	</bean>
	<bean id="sjj_x_size_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL14I-AL-SJJ-01:X:SIZE" />
	</bean>
	<bean id="sjj_y_centre_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL14I-AL-SJJ-01:Y:CENTRE" />
	</bean>
	<bean id="sjj_y_size_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL14I-AL-SJJ-01:Y:SIZE" />
	</bean>

</beans>
