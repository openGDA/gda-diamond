<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<!-- Coarse motors -->
	<bean id="SampleX_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL14I-MO-STAGE-01:X" />
	</bean>

	<bean id="SampleY_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL14I-MO-STAGE-01:Y" />
	</bean>

</beans>