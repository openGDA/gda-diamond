<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<!-- GDA should only move the coarse motors: see I14-676 -->
	<bean id="SampleX_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL14I-MO-STAGE-01:XC" />
	</bean>

	<bean id="SampleY_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL14I-MO-STAGE-01:YC" />
	</bean>

	<bean id="SampleZ_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL14I-MO-STAGE-01:YC" />
	</bean>

</beans>