<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- S4 coordinate system motors -->
	<bean id="s4_x_size_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL14I-AL-SLITS-04:X:SIZE" />
	</bean>

	<bean id="s4_y_size_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL14I-AL-SLITS-04:Y:SIZE" />
	</bean>

</beans>