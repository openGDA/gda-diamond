<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="SampleX_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL14I-MO-STAGE-01:X" />
	</bean>

	<bean id="SampleY_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL14I-MO-STAGE-01:Y" />
	</bean>

	<bean id="SampleZ_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL14I-MO-STAGE-01:Z" />
	</bean>

	<bean id="stage1_rotation_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL14I-MO-STAGE-01:ROT" />
	</bean>
	
	<bean id="xas_stage_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL14I-MO-XAS-01:STAGE" />
	</bean>

</beans>