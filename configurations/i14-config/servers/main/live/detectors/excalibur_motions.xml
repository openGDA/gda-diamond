<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">


	<bean id="beam_stop_x_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL14I-EA-EXBS-01:X" />
	</bean>

	<bean id="beam_stop_y_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL14I-EA-EXBS-01:Y" />
	</bean>

	<bean id="excalibur_y_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL14I-EA-EXCBR-01:Y" />
	</bean>

	<bean id="excalibur_z_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL14I-EA-EXCBR-01:Z" />
	</bean>

 </beans>