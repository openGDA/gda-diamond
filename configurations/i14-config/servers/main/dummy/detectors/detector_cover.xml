<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<!-- Detector cover: -->
	<bean id="detector_cover_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-16.00" />
		<property name="maxPosition" value="19.00" />
		<property name="position" value="12.00" />
		<property name="unitString" value="mm" />
	</bean>

</beans>