<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="mebrick1Motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-1000" />
		<property name="maxPosition" value="1000" />
		<property name="position" value="200" />
		<property name="speed" value="90" />
		<property name="local" value="true" />
	</bean>

	<bean id="mebrick3Motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-1000" />
		<property name="maxPosition" value="1000" />
		<property name="position" value="200" />
		<property name="speed" value="90" />
		<property name="local" value="true" />
	</bean>

</beans>