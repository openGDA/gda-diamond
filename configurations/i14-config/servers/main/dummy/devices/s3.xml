<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- S3 (white beam slits) coordinate system motors -->
	<bean id="s3_x_centre_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-1000.0" />
		<property name="maxPosition" value="1000.0" />
		<property name="position" value="1.03" />
		<property name="unitString" value="mm" />
	</bean>

	<bean id="s3_x_size_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-1000.0" />
		<property name="maxPosition" value="1000.0" />
		<property name="position" value="2.45" />
		<property name="unitString" value="mm" />
	</bean>

	<bean id="s3_y_centre_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-1000.0" />
		<property name="maxPosition" value="1000.0" />
		<property name="position" value="1.1023" />
		<property name="unitString" value="mm" />
	</bean>

	<bean id="s3_y_size_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-1000.0" />
		<property name="maxPosition" value="1000.0" />
		<property name="position" value="1.1081" />
		<property name="unitString" value="mm" />
	</bean>

</beans>