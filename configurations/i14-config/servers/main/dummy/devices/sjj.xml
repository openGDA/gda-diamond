<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="sjj_x_centre_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-1000.0" />
		<property name="maxPosition" value="1000.0" />
		<property name="position" value="-0.07" />
		<property name="unitString" value="mm" />
	</bean>
	<bean id="sjj_x_size_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-1000.0" />
		<property name="maxPosition" value="1000.0" />
		<property name="position" value="1.4985" />
		<property name="unitString" value="mm" />
	</bean>
	<bean id="sjj_y_centre_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-1000.0" />
		<property name="maxPosition" value="1000.0" />
		<property name="position" value="0.0093" />
		<property name="unitString" value="mm" />
	</bean>
	<bean id="sjj_y_size_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-1000.0" />
		<property name="maxPosition" value="1000.0" />
		<property name="position" value="1.2013" />
		<property name="unitString" value="mm" />
	</bean>

</beans>
