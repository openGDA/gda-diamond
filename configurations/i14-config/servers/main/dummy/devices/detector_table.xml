<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">


	<bean id="detector_table_x_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-1400" />
		<property name="maxPosition" value="1405" />
		<property name="position" value="0.32" />
		<property name="unitString" value="mm" />
		<property name="local" value="true" />
	</bean>

	<bean id="detector_table_femto" class="gda.device.monitor.DummyEpicsMonitorDouble">
		<property name="unit" value="uA" />
		<property name="value">
			<value type="double">0.865</value>
		</property>
	</bean>

	<bean id="detector_table_locum_c1" class="gda.device.monitor.DummyEpicsMonitorDouble">
		<property name="unit" value="V" />
		<property name="value">
			<value type="double">0.345</value>
		</property>
	</bean>

	<bean id="detector_table_locum_c2" class="gda.device.monitor.DummyEpicsMonitorDouble">
		<property name="unit" value="V" />
		<property name="value">
			<value type="double">1.93</value>
		</property>
	</bean>

	<bean id="detector_table_locum_c3" class="gda.device.monitor.DummyEpicsMonitorDouble">
		<property name="unit" value="V" />
		<property name="value">
			<value type="double">0.02</value>
		</property>
	</bean>

	<bean id="detector_table_locum_c4" class="gda.device.monitor.DummyEpicsMonitorDouble">
		<property name="unit" value="V" />
		<property name="value">
			<value type="double">2.4</value>
		</property>
	</bean>

</beans>