<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="sample_beamstop_x_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="10.0768" />
		<property name="maxPosition" value="140.077" />
		<property name="position" value="40.0" />
		<property name="unitString" value="mm" />
	</bean>

	<bean id="sample_beamstop_y_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-12.0287" />
		<property name="maxPosition" value="0.271332" />
		<property name="position" value="-3.8" />
		<property name="unitString" value="mm" />
	</bean>

</beans>
