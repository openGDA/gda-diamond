<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="SampleX_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-1024.0" />
		<property name="maxPosition" value="1024.0" />
		<property name="position" value="2.6921" />
		<property name="unitString" value="mm" />
	</bean>

	<bean id="SampleY_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-1024.0" />
		<property name="maxPosition" value="1024.0" />
		<property name="position" value="2.00064" />
		<property name="unitString" value="mm" />
	</bean>

	<bean id="SampleZ_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-1024.0" />
		<property name="maxPosition" value="1024.0" />
		<property name="position" value="12.9134" />
		<property name="unitString" value="mm" />
	</bean>

	<bean id="stage1_rotation_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-180.0" />
		<property name="maxPosition" value="270.0" />
		<property name="position" value="2.5" />
		<property name="unitString" value="deg" />
	</bean>

</beans>