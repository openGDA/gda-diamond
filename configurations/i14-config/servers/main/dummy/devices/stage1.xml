<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">


	<bean id="stage1_x_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-18.3" />
		<property name="maxPosition" value="19.2" />
		<property name="position" value="0.15" />
		<property name="unitString" value="mm" />
	</bean>

	<bean id="stage1_y_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-7.6" />
		<property name="maxPosition" value="5.35" />
		<property name="position" value="-0.0002" />
		<property name="unitString" value="mm" />
	</bean>

	<bean id="stage1_z_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-6.3" />
		<property name="maxPosition" value="27.5" />
		<property name="position" value="27.4" />
		<property name="unitString" value="mm" />
	</bean>

	<bean id="stage1_xf_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-10.0" />
		<property name="maxPosition" value="10.0" />
		<property name="position" value="1.73" />
		<property name="unitString" value="mm" />
	</bean>

	<bean id="stage1_yf_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-10.0" />
		<property name="maxPosition" value="10.0" />
		<property name="position" value="-0.98" />
		<property name="unitString" value="mm" />
	</bean>

	<bean id="stage1_rotation_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-180.0" />
		<property name="maxPosition" value="270.0" />
		<property name="position" value="2.5" />
		<property name="unitString" value="deg" />
	</bean>

</beans>