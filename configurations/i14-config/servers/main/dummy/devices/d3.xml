<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<!-- D3 -->
	<bean id="d3_x_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-1000.0" />
		<property name="maxPosition" value="-1000.0" />
		<property name="position" value="0.3" />
		<property name="unitString" value="mm" />
		<property name="local" value="true" />
	</bean>
	<bean id="d3_diode1" class="gda.device.monitor.DummyEpicsMonitorDouble">
		<property name="unit" value="uA" />
		<property name="value">
			<value type="double">0.012148</value>
		</property>
	</bean>

 </beans>