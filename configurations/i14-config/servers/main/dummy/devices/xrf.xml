<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="xrf_x_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-3.5" />
		<property name="maxPosition" value="17.25" />
		<property name="position" value="-2.1" />
		<property name="unitString" value="mm" />
	</bean>

	<bean id="xrf_y_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-2.0" />
		<property name="maxPosition" value="4.3" />
		<property name="position" value="1.52" />
		<property name="unitString" value="mm" />
	</bean>

</beans>
