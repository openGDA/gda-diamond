<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	    http://www.springframework.org/schema/util
		http://www.springframework.org/schema/util/spring-util-2.5.xsd">


	<!-- D1 -->
	<bean id="d1_x_motor" class="gda.device.motor.DummyMotor">
		<property name="local" value="true" />
	</bean>
	<bean id="d1_x_position" class="gda.device.enumpositioner.DummyEnumPositioner">
		<property name="positions">
			<util:list list-class="java.util.ArrayList">
				<value>In</value>
				<value>Out</value>
			</util:list>
		</property>
	</bean>

	<!-- D3 -->
	<bean id="d3_x_motor" class="gda.device.motor.DummyMotor">
		<property name="local" value="true" />
	</bean>
	<bean id="d3_diode1" class="gda.device.monitor.DummyEpicsMonitorDouble">
		<property name="unit" value="V" />
		<property name="value">
			<value type="double">0.235</value>
		</property>
	</bean>

	<!-- D4 -->
	<bean id="d4_x_motor" class="gda.device.motor.DummyMotor">
		<property name="local" value="true" />
	</bean>
	<bean id="d4_y_motor" class="gda.device.motor.DummyMotor">
		<property name="local" value="true" />
	</bean>
	<bean id="d4_diode1" class="gda.device.monitor.DummyEpicsMonitorDouble">
		<property name="unit" value="V" />
		<property name="value">
			<value type="double">0.865</value>
		</property>
	</bean>
	<bean id="d4_diode2" class="gda.device.monitor.DummyEpicsMonitorDouble">
		<property name="unit" value="V" />
		<property name="value">
			<value type="double">0.121</value>
		</property>
	</bean>

</beans>
