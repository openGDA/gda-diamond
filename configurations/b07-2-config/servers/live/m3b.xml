<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="m3b_x_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL07B-OP-VFM-01:X" />
	</bean>

	<bean id="m3b_y_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL07B-OP-VFM-01:Y" />
	</bean>

	<bean id="m3b_pitch_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL07B-OP-VFM-01:PITCH" />
	</bean>

	<bean id="m3b_roll_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL07B-OP-VFM-01:ROLL" />
	</bean>

	<bean id="m3b_yaw_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL07B-OP-VFM-01:YAW" />
	</bean>

</beans>