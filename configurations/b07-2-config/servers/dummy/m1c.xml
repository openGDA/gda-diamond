<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="m1c_mask_temp" class="gda.device.monitor.DummyEpicsMonitorDouble">
		<property name="unit" value="°C"/>
		<property name="value">
			<value type="double">25.3</value>
		</property>
		<property name="lowerLimit" value="18" />
		<property name="upperLimit" value="56" />
		<property name="increment" value="0.35" />
	</bean>

	<bean id="m1c_mirror_temp" class="gda.device.monitor.DummyEpicsMonitorDouble">
		<property name="unit" value="°C"/>
		<property name="value">
			<value type="double">25.3</value>
		</property>
		<property name="lowerLimit" value="18" />
		<property name="upperLimit" value="56" />
		<property name="increment" value="0.35" />
	</bean>

</beans>
