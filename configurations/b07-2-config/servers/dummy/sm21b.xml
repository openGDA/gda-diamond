<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="sm21b_x_motor" class="gda.device.motor.DummyMotor" />

	<bean id="sm21b_z_motor" class="gda.device.motor.DummyMotor" />

	<bean id="sm21b_y_motor" class="gda.device.motor.DummyMotor" />

	<bean id="sm21b_roty_motor" class="gda.device.motor.DummyMotor" />

	<bean id="sm21b_y_positioner" class="gda.device.enumpositioner.DummyEnumPositioner">
		<property name="positions">
			<list>
				<value>Out of Beam</value>
				<value>In Beam</value>
			</list>
		</property>
	</bean>
</beans>
