<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<!-- Motors -->
	<bean id="m4_pitch" class="uk.ac.gda.remoting.client.GdaRmiProxy" />

	<!-- Detectors -->
	<bean id="d6_positioner" class="uk.ac.gda.remoting.client.GdaRmiProxy" />
	<bean id="d6_camera_control" class="uk.ac.gda.remoting.client.GdaRmiProxy" />

	<!-- Shutters -->
	<bean id="s2_shutter" class="uk.ac.gda.remoting.client.GdaRmiProxy" />
	<bean id="s2_ss_x" class="uk.ac.gda.remoting.client.GdaRmiProxy" />
	<bean id="s2_ss_y" class="uk.ac.gda.remoting.client.GdaRmiProxy" />

	<!-- Energy focus parameters -->
	<bean id="energyFocusFunction" class="uk.ac.gda.remoting.client.GdaRmiProxy" />

</beans>
