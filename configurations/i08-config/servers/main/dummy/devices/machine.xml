<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
           http://www.springframework.org/schema/util
           http://www.springframework.org/schema/util/spring-util-2.0.xsd">


<!--machine and front end -->

	<bean id="absorber" class="gda.device.monitor.DummyEpicsMonitor">
		<property name="value" value="Open" />
		<property name="outputFormat" value="%s" />
	</bean>

	<bean id="topup" class="gda.device.monitor.DummyEpicsMonitor">
		<property name="value">
			<bean class="java.lang.Double">
				<constructor-arg value="545.353" />
			</bean>
		</property>
	</bean>

</beans>