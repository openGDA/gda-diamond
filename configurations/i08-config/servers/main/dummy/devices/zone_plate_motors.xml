<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="zone_plate_x_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-0.75" />
		<property name="maxPosition" value="0.8" />
		<property name="position" value="0.01458" />
	 	<property name="unitString" value="mm"/>
	 	<property name="speed" value="100"/>
	</bean>

	<bean id="zone_plate_y_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-3.1" />
		<property name="maxPosition" value="3.9" />
		<property name="position" value="-0.979" />
	 	<property name="unitString" value="mm"/>
	 	<property name="speed" value="100"/>
	</bean>

	<bean id="zone_plate_z_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-30.0" />
		<property name="maxPosition" value="30.0" />
		<property name="position" value="1.0849" />
	 	<property name="unitString" value="mm"/>
	 	<property name="speed" value="100"/>
	</bean>

</beans>