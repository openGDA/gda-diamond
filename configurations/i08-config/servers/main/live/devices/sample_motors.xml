<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<description>
		Motors for T1 endstation
	</description>

	<bean id="SampleX_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL08I-EA-TABLE-01:X" />
	</bean>

	<bean id="SampleY_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL08I-EA-TABLE-01:Y" />
	</bean>

	<bean id="SampleX_coarse_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL08I-EA-TABLE-01:XC" />
	</bean>

	<bean id="SampleY_coarse_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL08I-EA-TABLE-01:YC" />
	</bean>

	<bean id="SampleZ_coarse_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL08I-EA-TABLE-01:ZC" />
	</bean>

	<bean id="SampleX_fine_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL08I-EA-TABLE-01:XF" />
	</bean>

	<bean id="SampleY_fine_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL08I-EA-TABLE-01:YF" />
	</bean>

</beans>
