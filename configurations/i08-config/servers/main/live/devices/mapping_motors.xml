<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<description>
		Motors for T1 endstation
	</description>

	<bean id="T1X_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL08I-EA-TABLE-01:X" />
		<property name="local" value="true" />
	</bean>

	<bean id="T1Y_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL08I-EA-TABLE-01:Y" />
		<property name="local" value="true" />
	</bean>

	<bean id="T1ZC_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL08I-EA-TABLE-01:ZC" />
		<property name="local" value="true" />
	</bean>

</beans>
