<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- Periscope Top Motors -->
	<bean id="wp1_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-1000000" />
		<property name="maxPosition" value="1000000" />
		<property name="speed" value="20.0" />
		<property name="unitString" value="deg"/>
	</bean>

	<bean id="wp2_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-1000000" />
		<property name="maxPosition" value="1000000" />
		<property name="speed" value="20.0" />
		<property name="unitString" value="deg"/>
	</bean>

	<bean id="wp3_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="0.000" />
		<property name="maxPosition" value="149.000" />
		<property name="speed" value="0.8"/>
		<property name="unitString" value="mm"/>
	</bean>
</beans>
