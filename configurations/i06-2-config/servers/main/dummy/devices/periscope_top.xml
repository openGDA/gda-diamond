<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- Periscope Top Motors -->
	<bean id="periscope_top_X" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-5.00000" />
		<property name="maxPosition" value="5.00000" />
		<property name="speed" value="0.04" />
		<property name="unitString" value="deg"/>
	</bean>

	<bean id="periscope_top_Y" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-5.000000" />
		<property name="maxPosition" value="5.000000" />
		<property name="speed" value="0.04" />
		<property name="unitString" value="deg"/>
	</bean>

</beans>
