<?xml version="1.0" encoding="utf-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean class="gda.jython.ScriptProject">
        <property name="path" value="/dls_sw/p38/scripts/"/>
        <property name="name" value="Scripts: User"/>
        <property name="type" value="USER"/>
    </bean>
    <bean class="gda.jython.ScriptProject">
        <property name="path" value="${gda.config}/scripts"/>
        <property name="name" value="Scripts: Config"/>
        <property name="type" value="CONFIG"/>
    </bean>
    <bean class="gda.jython.ScriptProject">
        <property name="path" value="${gda.config}/../i22-config/scripts"/>
        <property name="name" value="Scripts: I22 Config"/>
        <property name="type" value="CONFIG"/>
    </bean>
    <bean class="gda.jython.ScriptProject">
        <property name="path" value="/dls_sw/i22/scripts/"/>
        <property name="name" value="Scripts: I22 User"/>
        <property name="type" value="CONFIG"/>
    </bean>
</beans>
