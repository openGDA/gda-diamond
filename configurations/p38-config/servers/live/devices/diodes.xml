<?xml version="1.0" encoding="utf-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="diode4" class="gda.device.currentamplifier.CalibratedAmplifier">
		<property name="useNameAsExtraName" value="true"/>
		<property name="hasUnits" value="false"/>
		<property name="pvName" value="BL38P-EA-XBPM-01:Cur4:MeanValue_RBV"/>
		<property name="settletime" value="500"/>
		<property name="upperThreshold" value="8.5"/>
	</bean>

	<bean id="diode3" class="gda.device.currentamplifier.CalibratedAmplifier">
