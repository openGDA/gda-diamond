<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
           
    <!-- FrontEnd Aperture -->
	
	<bean id="feapx_motor" class="gda.device.motor.DummyMotor" >
		<property name="minPosition" value="-5" />
		<property name="maxPosition" value="5" />
		<property name="speed" value="0.4" />
		<property name="unitString" value="mm"/>
		<property name="local" value="true" />
	</bean>
	
	<bean id="feapy_motor" class="gda.device.motor.DummyMotor" >
		<property name="minPosition" value="-5" />
		<property name="maxPosition" value="5" />
		<property name="speed" value="0.06" />
		<property name="unitString" value="mm"/>
		<property name="local" value="true" />
	</bean></beans>