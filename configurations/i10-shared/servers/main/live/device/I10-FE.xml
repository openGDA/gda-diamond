<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

    <!-- FrontEnd Aperture -->

	<bean id="feapx_motor" class="gda.device.motor.EpicsMotor" >
		<property name="deviceName" value="FEAPX" />
		<property name="epicsConfiguration" ref="epicsConfig" />
		<property name="local" value="true"/>
		<property name="accessControlName" value="branchena" />
	</bean>

	<bean id="feapy_motor" class="gda.device.motor.EpicsMotor" >
		<property name="deviceName" value="FEAPY" />
		<property name="epicsConfiguration" ref="epicsConfig" />
		<property name="local" value="true"/>
		<property name="accessControlName" value="branchena" />
	</bean>
</beans>