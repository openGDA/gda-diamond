<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<!-- M3M5 -->

	<bean id="m3m5x_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="M3M5.X" />
		<property name="epicsConfiguration" ref="epicsConfig" />
		<property name="local" value="true"/>
		<property name="accessControlName" value="branchena" />
	</bean>
	
	<bean id="m3m5y_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="M3M5.Y" />
		<property name="epicsConfiguration" ref="epicsConfig" />
		<property name="local" value="true"/>
		<property name="accessControlName" value="branchena" />
	</bean>
	
	<bean id="m3m5z_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="M3M5.Z" />
		<property name="epicsConfiguration" ref="epicsConfig" />
		<property name="local" value="true"/>
		<property name="accessControlName" value="branchena" />
	</bean>
	
	<bean id="m3m5yaw_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="M3M5.YAW" />
		<property name="epicsConfiguration" ref="epicsConfig" />
		<property name="local" value="true"/>
		<property name="accessControlName" value="branchena" />
	</bean>
	
	<bean id="m3m5pitch_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="M3M5.PITCH" />
		<property name="epicsConfiguration" ref="epicsConfig" />
		<property name="local" value="true"/>
		<property name="accessControlName" value="branchena" />
	</bean>
	
	<bean id="m3m5roll_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="M3M5.ROLL" />
		<property name="epicsConfiguration" ref="epicsConfig" />
		<property name="local" value="true"/>
		<property name="accessControlName" value="branchena" />
	</bean>
</beans>