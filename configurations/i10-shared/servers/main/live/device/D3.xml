<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<!-- D3 (D5) --> 

	<bean id="d3d5_scrn_motor" class="gda.device.motor.DummyMotor" />

	<bean id="d3d5_det_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="D3.DETX" />
		<property name="epicsConfiguration" ref="epicsConfig" />
		<property name="local" value="true"/>
		<property name="accessControlName" value="branchena" />
	</bean>

    <bean id="d3_pos" class="gda.device.enumpositioner.EpicsPositioner">
        <property name="deviceName" value="D3.POS"/>
    </bean>
</beans>