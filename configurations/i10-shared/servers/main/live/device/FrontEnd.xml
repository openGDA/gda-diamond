<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- FrontEnd Aperture -->

	<bean id="feapx_motor" class="gda.device.motor.EpicsMotor" >
		<property name="pvName" value="FE10I-AL-APTR-02:X" />
		<property name="local" value="true"/>
	</bean>

	<bean id="feapy_motor" class="gda.device.motor.EpicsMotor" >
		<property name="pvName" value="FE10I-AL-APTR-02:Y" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="frontend" class ="gda.device.enumpositioner.EpicsPneumaticCallback">
		<property name="controlPv" value="FE10I-CS-BEAM-01:BLCON"/>
		<property name="statusPv" value="FE10I-CS-BEAM-01:STA"/>
		<property name="local" value="true"/>
		<property name="readOnly" value="true"/>
	</bean>

	<bean id="ab0" class ="gda.device.enumpositioner.EpicsPneumaticCallback">
		<property name="controlPv" value="FE10I-RS-ABSB-02:BLCON"/>
		<property name="statusPv" value="FE10I-RS-ABSB-02:STA"/>
		<property name="local" value="true"/>
	</bean></beans>