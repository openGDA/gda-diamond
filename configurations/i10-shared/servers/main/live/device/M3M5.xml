<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<!-- M3M5 -->

	<bean id="m3m5x_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL10I-OP-SWTCH-01:X" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="m3m5y_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL10I-OP-SWTCH-01:YAW" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="m3m5z_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL10I-OP-SWTCH-01:Z" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="m3m5yaw_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL10I-OP-SWTCH-01:YAW" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="m3m5pitch_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL10I-OP-SWTCH-01:PITCH" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="m3m5roll_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL10I-OP-SWTCH-01:ROLL" />
		<property name="local" value="true"/>
	</bean>
</beans>