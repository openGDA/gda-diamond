<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="table_x_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL18I-MO-TABLE-03:X"></property>
	</bean>

	<bean id="table_y_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL18I-MO-TABLE-03:Y"></property>
	</bean>

	<bean id="table_z_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL18I-MO-TABLE-03:Z"></property>
	</bean>

	<bean id="table_pitch_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL18I-MO-TABLE-03:PITCH"></property>
	</bean>

</beans>
