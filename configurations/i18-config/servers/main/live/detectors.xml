<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="daserver" class="gda.device.detector.DAServer">
		<property name="local" value="True" />
		<property name="host" value="i18-xspress0" />
		<property name="port" value="1972" />
		<property name="startupCommands">
			<list>
				<value>~config</value>
			</list>
		</property>
		<property name="dataPort" value="20030" />
		<property name="replyTimeOut" value="5000" />
	</bean>

	<bean id="xspress3_daserver" class="gda.device.detector.DAServer">
		<property name="local" value="True" />
		<property name="host" value="bl18i-ea-serv-01" />
		<property name="port" value="1972" />
		<property name="startupCommands">
			<list>
				<value>~config</value>
			</list>
		</property>
		<property name="dataPort" value="20030" />
		<property name="replyTimeOut" value="5000" />
	</bean>

	<bean id="photonccd" class="gda.device.detector.IPPserver">
		<property name="outputFolderRoot" value="x:/data/2012/cm5712-5/xrd/" />
		<property name="fileNameRoot" value="i18-" />
		<property name="fileFormat" value="tif" />
		<property name="collectionTime" value="1" />
		<property name="host" value="172.23.118.71" />
		<property name="port" value="23" />
	</bean>

	<bean id="I0_keithley" class="gda.device.enumpositioner.EpicsSimplePositioner">
		<property name="pvName" value="BL18I-EA-IAMP-02:Gain" />
		<property name="values">
			<map>
				<entry key="10^3 V/A" value="10^3 V/A" />
				<entry key="10^4 V/A" value="10^4 V/A" />
				<entry key="10^5 V/A" value="10^5 V/A" />
				<entry key="10^6 V/A" value="10^6 V/A" />
				<entry key="10^7 V/A" value="10^7 V/A" />
				<entry key="10^8 V/A" value="10^8 V/A" />
				<entry key="10^9 V/A" value="10^9 V/A" />
				<entry key="10^10 V/A" value="10^10 V/A" />
			</map>
		</property>
	</bean>

	<bean id="It_keithley" class="gda.device.enumpositioner.EpicsSimplePositioner">
		<property name="pvName" value="BL18I-EA-IAMP-03:Gain" />
		<property name="values">
			<map>
				<entry key="10^3 V/A" value="10^3 V/A" />
				<entry key="10^4 V/A" value="10^4 V/A" />
				<entry key="10^5 V/A" value="10^5 V/A" />
				<entry key="10^6 V/A" value="10^6 V/A" />
				<entry key="10^7 V/A" value="10^7 V/A" />
				<entry key="10^8 V/A" value="10^8 V/A" />
				<entry key="10^9 V/A" value="10^9 V/A" />
				<entry key="10^10 V/A" value="10^10 V/A" />
			</map>
		</property>
	</bean>

</beans>
