	<bean id="ml_x_motor" class="gda.device.motor.DummyMotor" />
	<bean id="ml_y_motor" class="gda.device.motor.DummyMotor" />
	<bean id="ml_z_motor" class="gda.device.motor.DummyMotor" />

	<bean id="D7A" class="gda.device.enumpositioner.DummyEnumPositioner">
		<property name="positions">
			<list>
				<value>2 mm Al</value>
				<value>0.1 mm Al</value>
				<value>0.05 mm Al</value>
				<value>0.8 mm Al</value>
				<value>0.75 mm Al</value>
