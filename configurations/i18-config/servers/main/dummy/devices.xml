<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<!-- ***************************** -->
	<!-- *** DUMMY MONITOR SECTION *** -->
	<!-- ***************************** -->
	<!-- (No dummy equivalents for most of the EPICS monitors) -->
	<bean id="bragg_speed" class="gda.device.monitor.DummyMonitor">
		<property name="local" value="true" />
	</bean>

	<bean id="machineModeMonitor" class="gda.device.monitor.DummyEpicsMonitor">
		<property name="value" value="Shutdown" />
	</bean>
	
	<bean id="ringModeMonitor" class="gda.device.monitor.DummyEpicsMonitor">
		<property name="value">
			<value>VMX</value>
		</property>
	</bean>

	<bean id="machineTopupMonitor" class="gda.device.scannable.DummyTopupScannable">
	</bean>
	
	<bean id="machineTopupPeriod" class="gda.device.monitor.DummyEpicsMonitor">
		<property name="value">
			<value type="double">10</value>
		</property>
	</bean>
	
	<bean id="ringcurrent" class="gda.device.monitor.DummyEpicsMonitor">
		<property name="value">
			<value type="double">300</value>
		</property> 
	</bean>
	
	<bean id="portshutter" class="gda.device.enumpositioner.DummyEnumPositioner">
		<property name="positions">
			<list>
				<value>Open</value>
				<value>Opening</value>
				<value>Closed</value>
				<value>Closing</value>
				<value>Fault</value>
			</list>
		</property>
	</bean>

	<!-- *************************** -->
	<!-- *** DUMMY MOTOR SECTION *** -->
	<!-- *************************** -->
	<bean id="idgap_motor" class="gda.device.motor.DummyMotor">
		<property name="speed" value="1000.0"></property>
	</bean>

	<bean id="d1motor_motor" class="gda.device.motor.DummyMotor" />
	<bean id="d2motor_motor" class="gda.device.motor.DummyMotor" />
