	<bean id="vma_zoom_motor" class="gda.device.motor.DummyMotor" />
	<bean id="vma_zoom_positioner" class="gda.device.enumpositioner.DummyEnumPositioner">
		<property name="positions">
			<list>
				<value>10</value>
				<value>20</value>
				<value>30</value>
				<value>40</value>
				<value>50</value>
				<value>60</value>
				<value>70</value>
				<value>80</value>
			</list>
		</property>
	</bean>
