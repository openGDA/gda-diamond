<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="table_x_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-75.0" />
		<property name="maxPosition" value="75.0" />
	</bean>

	<bean id="table_y_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-5.0" />
		<property name="maxPosition" value="25.0" />
	</bean>

	<bean id="table_z_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-35.0" />
		<property name="maxPosition" value="35.0" />
	</bean>

	<bean id="table_pitch_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-45.0" />
		<property name="maxPosition" value="45.0" />
	</bean>

</beans>
