<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:corba="http://www.diamond.ac.uk/schema/gda/corba"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
		http://www.springframework.org/schema/context
		http://www.springframework.org/schema/context/spring-context.xsd
		http://www.diamond.ac.uk/schema/gda/corba
		http://www.diamond.ac.uk/schema/gda/corba/gda-corba-1.1.xsd">

	<context:property-placeholder/>

	<bean class="gda.spring.FindableNameSetterPostProcessor" />

	<corba:import namespace="${gda.factory.factoryName}" names="plot_server" />

	<bean class="gda.rcp.util.OSGIServiceRegister">
		<property name="class" value="uk.ac.diamond.scisoft.analysis.PlotServer" />
		<property name="service" ref="plot_server" />
	</bean>

	<corba:adapterfactory namespace="${gda.factory.factoryName}" />

	<import resource="rmi_imports.xml" />
	<import resource="mapping.xml"/>
	<import resource="live_controls.xml"/>

</beans>
