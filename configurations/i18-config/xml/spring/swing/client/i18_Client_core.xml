<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
     
	<bean id="LogMessage" class="gda.util.LogPanel">
	<property name="name" value="LogMessage"/>
	</bean>
	
	<bean id="DebugMessage" class="gda.util.DebugPanel">
	<property name="name" value="DebugMessage"/>
	</bean>
	
	<bean id="JythonEditor" class="gda.jython.gui.JythonEditor">
	<property name="name" value="JythonEditor" ></property>
	</bean>
	
	<bean id="Plot1" class="gda.gui.dv.DataVectorPlot">
	<property name="name" value="Plot1"></property>
	</bean>
	
	<bean id="JythonTerminal" class="gda.jython.gui.JythonTerminalAcqPanel">
	<property name="name" value="JythonTerminal"></property>
	<property name="numberInitialGraphs" value="1"></property>
	<property name="extraGraphs" value="false"></property>
	</bean>
	<bean id="XspressMonitor" class="gda.device.xspress.XsPress2MonitorPanel">
	<property name="name" value="XspressMonitor"></property>
	<!-- for rcp server --><property name="xspressSystemName" value="sw_xspress2system" ></property>
	<!-- For swing server  <property name="xspressSystemName" value="xspress2system" ></property>-->
	<property name="counterTimerName" value="counterTimer01" ></property>
	</bean>
	
	<!-- from the oe plugin -->
	<bean id="OEMove" class="gda.gui.oemove.OEMovePanel">
	<property name="name" value="OEMove"/>
	</bean>

</beans>