<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
    <bean class="gda.configuration.epics.EpicsConfiguration">
		<constructor-arg value="${gda.config}/xml/epics/BL18I-gda-interface2-0.xml" />
	</bean>     
  
	<bean id="bragg_speed" class="gda.device.monitor.DummyMonitor">
		<property name="local" value="true" />
	</bean>
	<bean id="dcm_crystal" class="gda.device.enumpositioner.DummyEnumPositioner">
		<property name="positions">
			<list>
				<value>Si(111)</value>
				<value>Si(113)</value>
			</list>
		</property>
	</bean>
	<!-- ********************* -->
	<!-- *** MOTOR SECTION *** -->
	<!-- ********************* -->
	<!-- Insertion Device -->
	
	<bean id="idgap_motor" class="gda.device.motor.DummyMotor" >
	<property name="speed" value="1000.0"></property>
	</bean>
	 <!-- D1 -->
	 <bean id="d1motor_motor" class="gda.device.motor.DummyMotor" />
	 <!-- D2--> 
	 <bean id="d2motor_motor" class="gda.device.motor.DummyMotor" />
	  <!-- D3--> 
	 <bean id="d3motor_motor" class="gda.device.motor.DummyMotor" />
	  <!-- D5--> 
	 <bean id="d5amotor_motor" class="gda.device.motor.DummyMotor" />
	 <bean id="d5bmotor_motor" class="gda.device.motor.DummyMotor" />
	  <!-- D6--> 
	 <bean id="d6amotor_motor" class="gda.device.motor.DummyMotor" />
	 <bean id="d6bmotor_motor" class="gda.device.motor.DummyMotor" />
	  <!-- D7--> 
	 <bean id="d7amotor_motor" class="gda.device.motor.DummyMotor" />
	 <bean id="d7bmotor_motor" class="gda.device.motor.DummyMotor" />
	<!-- S1 Motors -->
	<!-- Compound slit motion motors -->
	<bean id="s1xpos_motor" class="gda.device.motor.DummyMotor" />	
	<bean id="s1xgap_motor" class="gda.device.motor.DummyMotor" />
	<bean id="s1ypos_motor" class="gda.device.motor.DummyMotor" />
	<bean id="s1ygap_motor" class="gda.device.motor.DummyMotor" />
	<!--  Toriod Mirror / HFM Optical Element --> 
	<bean id="toroid_sag_motor" class="gda.device.motor.DummyMotor" />
	<bean id="toroid_pitch_motor" class="gda.device.motor.DummyMotor" />
	<bean id="toroid_x_motor" class="gda.device.motor.DummyMotor" />
	<bean id="toroid_curve_motor" class="gda.device.motor.DummyMotor" />
	<bean id="toroid_ellipticity_motor" class="gda.device.motor.DummyMotor" />
	<bean id="toroid_y_motor" class="gda.device.motor.DummyMotor" />
	<bean id="toroid_yaw_motor" class="gda.device.motor.DummyMotor" />
	<bean id="toroid_roll_motor" class="gda.device.motor.DummyMotor" />
	<!-- Monochromator / DCM Motors -->
	<bean id="dcm_bragg_motor" class="gda.device.motor.DummyMotor" >
	<property name="speed" value="1000.0"></property>
	</bean>	   
	<bean id="dcm_energy_motor" class="gda.device.motor.DummyMotor" />	
	<bean id="dcm_crystal1_roll_motor" class="gda.device.motor.DummyMotor" />	
	<bean id="dcm_crystal2_roll_motor" class="gda.device.motor.DummyMotor" />	
	<bean id="dcm_crystal2_pitch_motor" class="gda.device.motor.DummyMotor" />	
	<bean id="dcm_perp_motor" class="gda.device.motor.DummyMotor" >
	<property name="speed" value="1000.0"></property>
	</bean>
	<!-- Secondary Slits / S2 Optical Element -->
	<bean id="s2xpos_motor" class="gda.device.motor.DummyMotor" />	
	<bean id="s2xgap_motor" class="gda.device.motor.DummyMotor" />
	<bean id="s2ypos_motor" class="gda.device.motor.DummyMotor" />
	<bean id="s2ygap_motor" class="gda.device.motor.DummyMotor" />
	<!-- Tertiary Slits / S3 Optical Element -->
	<bean id="s3xpos_motor" class="gda.device.motor.DummyMotor" />	
	<bean id="s3xgap_motor" class="gda.device.motor.DummyMotor" />
	<bean id="s3ypos_motor" class="gda.device.motor.DummyMotor" />
	<bean id="s3ygap_motor" class="gda.device.motor.DummyMotor" />
	<bean id="s3xplus_motor" class="gda.device.motor.DummyMotor" />	
	<bean id="s3xminus_motor" class="gda.device.motor.DummyMotor" />
	<bean id="s3yplus_motor" class="gda.device.motor.DummyMotor" />
	<bean id="s3yminus_motor" class="gda.device.motor.DummyMotor" />
	<!-- KB Mirrors Optical Element -->
	<bean id="kb_vfm_y_motor" class="gda.device.motor.DummyMotor" />	
	<bean id="kb_vfm_x_motor" class="gda.device.motor.DummyMotor" />
	<bean id="kb_vfm_pitch_motor" class="gda.device.motor.DummyMotor" />
	<bean id="kb_hfm_y_motor" class="gda.device.motor.DummyMotor" />
	<bean id="kb_hfm_x_motor" class="gda.device.motor.DummyMotor" />	
	<bean id="kb_hfm_pitch_motor" class="gda.device.motor.DummyMotor" />
	<bean id="kb_hfm_bend1_motor" class="gda.device.motor.DummyMotor" />
	<bean id="kb_hfm_bend2_motor" class="gda.device.motor.DummyMotor" />
	<bean id="kb_vfm_bend1_motor" class="gda.device.motor.DummyMotor" />
	<bean id="kb_vfm_bend2_motor" class="gda.device.motor.DummyMotor" />
	
	<!-- Pin Hole Optical Element -->
	<bean id="pinx_motor" class="gda.device.motor.DummyMotor" />
	<bean id="piny_motor" class="gda.device.motor.DummyMotor" />
	<!-- Beam Stop (Tungsten Wire) Optical Element -->
	<bean id="TungstenWireY_motor" class="gda.device.motor.DummyMotor" />

	<!-- Sample Stage Motors -->	  
	<bean id="sample_z_motor" class="gda.device.motor.DummyMotor" />	
	<bean id="sample_thetacoarse_motor" class="gda.device.motor.DummyMotor" />	  
	<bean id="sample_x_motor" class="gda.device.motor.DummyMotor" />	  
	<bean id="sample_y1_motor" class="gda.device.motor.DummyMotor" />	  
	<bean id="sample_y2_motor" class="gda.device.motor.DummyMotor" />	  
	<bean id="sample_y3_motor" class="gda.device.motor.DummyMotor" />	  
	<bean id="sample_thetafine_motor" class="gda.device.motor.DummyMotor" />
	
	<!-- Video Microscope (VM) Motors -->	    
	<bean id="vma_zoom_motor" class="gda.device.motor.DummyMotor" />
	<bean id="vma_focus_motor" class="gda.device.motor.DummyMotor" />
	
		<!-- WDS Motors -->	    
	<bean id="wds_x_motor" class="gda.device.motor.DummyMotor" />
	<bean id="wds_y_motor" class="gda.device.motor.DummyMotor" />
	<bean id="wds_z_motor" class="gda.device.motor.DummyMotor" />
	<bean id="wds_lambda_motor" class="gda.device.motor.DummyMotor" />
	<bean id="wds_slit_motor" class="gda.device.motor.DummyMotor" />
	
	<!-- Multilayer Detector Optical Element -->
	<bean id="ml_x_motor" class="gda.device.motor.DummyMotor" />
	<bean id="ml_y_motor" class="gda.device.motor.DummyMotor" />
	<bean id="ml_z_motor" class="gda.device.motor.DummyMotor" />
	<bean id="ml_angle_motor" class="gda.device.motor.DummyMotor" />
	<bean id="ml_screen_motor" class="gda.device.motor.DummyMotor" />
	<bean id="ml_det_motor" class="gda.device.motor.DummyMotor" />
	
	<!-- HR Mirrors Optical Element -->
	<bean id="hrm_combined_x_motor" class="gda.device.motor.DummyMotor" />
	<bean id="hrm_shr_x_motor" class="gda.device.motor.DummyMotor" />
	<bean id="hrm_combined_pitch_motor" class="gda.device.motor.DummyMotor" />

	<!-- Attenuators -->
	<bean id="D7A" class="gda.device.enumpositioner.DummyEnumPositioner">
		<property name="positions">
			<list>
				<value>0.1 mm Al</value>
				<value>0.05 mm Al</value>
				<value>0.8 mm Al</value>
				<value>0.75 mm Al</value>
				<value>0.55 mm Al</value>
				<value>0.5 mm Al</value>
				<value>0.3 mm Al</value>
				<value>0.25 mm Al</value>
				<value>Gap</value>
				<value>1.5 mm Al</value>
				<value>1.25 mm Al</value>				
			</list>
		</property>
	</bean>
	<bean id="D7B" class="gda.device.enumpositioner.DummyEnumPositioner">
		<property name="positions">
			<list>
				<value>Fluorescence</value>
				<value>ni drain</value>
				<value>au drain</value>
				<value>al drain</value>
				<value>gap</value>
				<value>in line diode</value>
			</list>
		</property>
	</bean>
	<!-- to use with xspress detector for continuous/trajectory scans -->
	<bean id="epicsTrajectoryScanController" class="gda.scan.DummyTrajectoryScanController">
		<property name="m1axis" value="sample_z_motor" />
		<property name="m2axis" value="sample_thetacoarse_motor" />
		<property name="m3axis" value="sample_x_motor" />
		<property name="m4axis" value="sample_thetafine_motor" />
		<property name="m5axis" value="empty" />
		<property name="m6axis" value="empty" />
		<property name="m7axis" value="empty" />
		<property name="m8axis" value="empty" />
		<property name="motorToMove" ref="sc_MicroFocusSampleX"/>
	</bean>
	
	<bean id="xpsDeferFlag" class="gda.device.controlpoint.DummyControlPoint"/>
	
	<!--  <bean id="qexafs_energy" class="gda.device.scannable.QexafsTestingScannable">
		<property name="motor" ref="dcm_bragg_motor" />
		<property name="lowerGdaLimits" value="2000" />
		<property name="upperGdaLimits" value="35000" />
		<property name="outputFormat">
			<list>
				<value>%.2f</value>
			</list>
		</property>
	</bean>-->
	<bean id="edxdcontroller" class="gda.device.detector.xmap.edxd.EDXDMappingController">
		<property name="epicsDeviceName" value="xmap_dxp"/>
		<property name="elementOffset" value="1"/>
		<property name="local" value="true"/>
		<property name="NUMBER_OF_ELEMENTS" value="4"/>
		<property name="hdf5" ref="hdf5"/>
</bean>
<bean id="xmapcontroller" class="gda.device.detector.xmap.EpicsXmapController3ROI">
		<property name="edxdControllerName" value="edxdcontroller"/>
		<property name="local" value="true"/>
</bean>
<bean id="hdf5" class="gda.device.detector.areadetector.v17.impl.NDFileHDF5Impl">
		<property name="basePVName" value="BL18I-EA-DET-04:HDF:" />
		<property name="file">
			<bean class="gda.device.detector.areadetector.v17.impl.NDFileImpl">
				<property name="basePVName" value="BL18I-EA-DET-04:HDF:" />
				<property name="initialAutoIncrement" value="0" />
				<property name="initialAutoSave" value="0" />
				<property name="initialWriteMode" value="0" />
				<property name="initialNumCapture" value="1" />
				<property name="initialFileName" value="helloworld" />
				<property name="initialFileTemplate" value="%s%s-%d.h5" />
				<property name="pluginBase">
					<bean class="gda.device.detector.areadetector.v17.impl.NDPluginBaseImpl">
						<property name="basePVName" value="BL18I-EA-DET-04:HDF:" />
					</bean>
				</property>
			</bean>
		</property>
	</bean>	
</beans>