<ObjectFactory>
	<name>${gda.factory.factoryName}</name>
	
	<!-- ************************ -->
	<!-- *** Optical Elements *** -->
	<!-- ************************ -->
	<!-- Insertion Device -->
	<GenericOE>
		<name>ID</name>
		<LinearPositioner>
			<name>idgap_positioner</name>
			<motorName>idgap_motor</motorName>
			<stepsPerUnit>1</stepsPerUnit>
			<softLimitLow>-100</softLimitLow>
			<softLimitHigh>1000</softLimitHigh>
			<poll>true</poll>
		</LinearPositioner>
		<SingleAxisLinearDOF>
			<name>idgap</name>
			<protectionLevel>0</protectionLevel>
			<acceptableUnit>mm</acceptableUnit>
			<moveableName>idgap_positioner</moveableName>
		</SingleAxisLinearDOF>
	</GenericOE>
	

	<!-- Slits1 (S1) OE -->
	<GenericOE>
		<name>Slits1</name>
		<LinearPositioner>
			<name>s1xpos_positioner</name>
			<motorName>s1xpos_motor</motorName>
			<stepsPerUnit>1</stepsPerUnit>
			<softLimitLow>0</softLimitLow>
			<softLimitHigh>10</softLimitHigh>
			<homeOffset>0.0</homeOffset>

		</LinearPositioner>
		<LinearPositioner>
			<name>s1xgap_positioner</name>
			<motorName>s1xgap_motor</motorName>
			<stepsPerUnit>1</stepsPerUnit>
			<softLimitLow>5.9</softLimitLow>
			<softLimitHigh>5.9</softLimitHigh>
			<homeOffset>0.0</homeOffset>

		</LinearPositioner>
		<LinearPositioner>
			<name>s1ypos_positioner</name>
			<motorName>s1ypos_motor</motorName>
			<stepsPerUnit>1</stepsPerUnit>
			<softLimitLow>0.5</softLimitLow>
			<softLimitHigh>9.5</softLimitHigh>
			<homeOffset>0.0</homeOffset>

		</LinearPositioner>
		<LinearPositioner>
			<name>s1ygap_positioner</name>
			<motorName>s1ygap_motor</motorName>
			<stepsPerUnit>1</stepsPerUnit>
			<softLimitLow>4.9</softLimitLow>
			<softLimitHigh>5.9</softLimitHigh>
			<homeOffset>0.0</homeOffset>

		</LinearPositioner>

		<SingleAxisLinearDOF>
			<name>s1xpos</name>
			<protectionLevel>0</protectionLevel>
			<acceptableUnit>mm</acceptableUnit>
			<moveableName>s1xpos_positioner</moveableName>
		</SingleAxisLinearDOF>
		<SingleAxisLinearDOF>
			<name>s1xgap</name>
			<protectionLevel>0</protectionLevel>
			<acceptableUnit>mm</acceptableUnit>
			<moveableName>s1xgap_positioner</moveableName>
		</SingleAxisLinearDOF>
		<SingleAxisLinearDOF>
			<name>s1ypos</name>
			<protectionLevel>0</protectionLevel>
			<acceptableUnit>mm</acceptableUnit>
			<moveableName>s1ypos_positioner</moveableName>
		</SingleAxisLinearDOF>
		<SingleAxisLinearDOF>
			<name>s1ygap</name>
			<protectionLevel>0</protectionLevel>
			<acceptableUnit>mm</acceptableUnit>
			<moveableName>s1ygap_positioner</moveableName>
		</SingleAxisLinearDOF>
	</GenericOE>
