<?xml version="1.0" encoding="utf-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="s4_x" class="gda.device.scannable.ScannableMotor">
		<property name="motor" ref="s4_x_motor"/>
		<property name="local" value="true"/>
		<property name="protectionLevel" value="3"/>
	</bean>

	<bean id="s4_y" class="gda.device.scannable.ScannableMotor">
		<property name="motor" ref="s4_y_motor"/>
		<property name="local" value="true"/>
		<property name="protectionLevel" value="3"/>
	</bean>

	<bean id="s4_x_gap" class="gda.device.scannable.ScannableMotor">
		<property name="motor" ref="s4_x_gap_motor"/>
		<property name="local" value="true"/>
		<property name="protectionLevel" value="3"/>
	</bean>

	<bean id="s4_y_gap" class="gda.device.scannable.ScannableMotor">
		<property name="motor" ref="s4_y_gap_motor"/>
		<property name="local" value="true"/>
		<property name="protectionLevel" value="3"/>
	</bean>

	<bean id="s4" class="gda.device.scannable.scannablegroup.ScannableGroup">
		<property name="groupMembers">
			<list>
				<ref bean="s4_x"/>
				<ref bean="s4_y"/>
				<ref bean="s4_x_gap"/>
				<ref bean="s4_y_gap"/>
			</list>
		</property>
		<property name="local" value="true"/>
		<property name="protectionLevel" value="3"/>
	</bean>
</beans>

