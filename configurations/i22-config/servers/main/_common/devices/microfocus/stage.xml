<?xml version="1.0" encoding="utf-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:gda="http://www.diamond.ac.uk/schema/gda/gda"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd
            http://www.diamond.ac.uk/schema/gda/gda
            http://www.diamond.ac.uk/schema/gda/gda/gda-gda-1.2.xsd">

    <gda:motor id="mfstage_x" pv="BL22I-MO-TABLE-05:X" protectionLevel="0" />
    <gda:motor id="mfstage_y" pv="BL22I-MO-TABLE-05:Y" protectionLevel="0" />
    <gda:motor id="mfstage_z" pv="BL22I-MO-TABLE-05:Z" protectionLevel="0" />

    <bean id="mfstage" class="gda.device.scannable.scannablegroup.ScannableGroup">
        <property name="groupMembers">
            <list>
                <ref bean="mfstage_x"/>
                <ref bean="mfstage_y"/>
                <ref bean="mfstage_z"/>
            </list>
        </property>
        <property name="protectionLevel" value="0"/>
    </bean>
</beans>
