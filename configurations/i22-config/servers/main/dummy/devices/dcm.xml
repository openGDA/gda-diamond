<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:aop="http://www.springframework.org/schema/aop"
    xmlns:corba="http://www.diamond.ac.uk/schema/gda/corba"
    xmlns:tx="http://www.springframework.org/schema/tx"
    xmlns:util="http://www.springframework.org/schema/util"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
        http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.5.xsd
        http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-2.5.xsd
        http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.5.xsd
        http://www.diamond.ac.uk/schema/gda/corba http://www.diamond.ac.uk/schema/gda/corba/gda-corba-1.0.xsd">

    <bean id="dcm_energy_motor" class="gda.device.motor.DummyMotor">
        <property name="local" value="true" />
    </bean>
    <bean id="dcm_offset_motor" class="gda.device.motor.DummyMotor">
        <property name="local" value="true" />
    </bean>
    <bean id="dcm_bragg_motor" class="gda.device.motor.DummyMotor">
        <property name="local" value="true" />
    </bean>
    <bean id="dcm_finepitch_motor" class="gda.device.motor.DummyMotor">
        <property name="local" value="true" />
    </bean>
    <bean id="dcm_fineroll_motor" class="gda.device.motor.DummyMotor">
        <property name="local" value="true" />
    </bean>
    <bean id="dcm_pitch_motor" class="gda.device.motor.DummyMotor">
        <property name="local" value="true" />
    </bean>
    <bean id="dcm_perp_motor" class="gda.device.motor.DummyMotor">
        <property name="local" value="true" />
    </bean>
    <bean id="dcm_roll_motor" class="gda.device.motor.DummyMotor">
        <property name="local" value="true" />
    </bean>
</beans>