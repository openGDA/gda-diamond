<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://www.springframework.org/schema/beans
			http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="hexapod_yaw_motor" class="gda.device.motor.ThreadlessDummyMotor"/>
	<bean id="hexapod_z_motor" class="gda.device.motor.ThreadlessDummyMotor"/>
	<bean id="hexapod_roll_motor" class="gda.device.motor.ThreadlessDummyMotor"/>
	<bean id="hexapod_x_motor" class="gda.device.motor.ThreadlessDummyMotor"/>
	<bean id="hexapod_pitch_motor" class="gda.device.motor.ThreadlessDummyMotor"/>
