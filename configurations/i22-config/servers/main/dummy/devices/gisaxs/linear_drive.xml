<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:util="http://www.springframework.org/schema/util"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-3.2.xsd">

	<bean id="linearDrive_motor" class="gda.device.motor.DummyMotor"/>

	<bean class="gda.device.monitor.DummyMonitor" id="linearDrive_baseTemp"/>
	<bean class="gda.device.monitor.DummyMonitor" id="linearDrive_pillarTemp"/>
	<bean class="gda.device.monitor.DummyMonitor" id="linearDrive_motorTemp"/>
	<bean class="gda.device.monitor.DummyMonitor" id="linearDrive_topTemp"/>
</beans>
