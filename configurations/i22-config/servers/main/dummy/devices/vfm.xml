    <bean id="vfm_y_motor" class="gda.device.motor.DummyMotor">
        <property name="local" value="true" />
    </bean>
    <bean id="vfm_x2_motor" class="gda.device.motor.DummyMotor">
        <property name="local" value="true" />
    </bean>
    <bean id="vfm_y2_motor" class="gda.device.motor.DummyMotor">
