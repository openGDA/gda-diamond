    </bean>
    <bean id="s3_xplus_motor" class="gda.device.motor.DummyMotor">
        <property name="local" value="true" />
    </bean>
    <bean id="s3_ycentre_motor" class="gda.device.motor.DummyMotor">
        <property name="local" value="true" />
    </bean>
    <bean id="s3_yminus_motor" class="gda.device.motor.DummyMotor">
        <property name="local" value="true" />
    </bean>
    <bean id="s3_yplus_motor" class="gda.device.motor.DummyMotor">
        <property name="local" value="true" />
    </bean>
</beans>