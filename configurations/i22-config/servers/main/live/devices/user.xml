<?xml version="1.0" encoding="utf-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="user_four_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL22I-MO-USER-01:MTR4"/>
		<property name="local" value="true"/>
	</bean>

	<bean id="user_three_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL22I-MO-USER-01:MTR3"/>
		<property name="local" value="true"/>
	</bean>
</beans>
