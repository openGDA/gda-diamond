<?xml version="1.0" encoding="utf-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="bco_y_motor" class="gda.device.motor.EpicsMotor" >
		<property name="pvName" value="BL22I-MO-BCO-01:TABLE:Y" />
	</bean>

	<bean id="bco_x_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL22I-MO-BCO-01:TABLE:X"/>
	</bean>

	<bean id="bco_pitch_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL22I-MO-BCO-01:TABLE:PITCH"/>
	</bean>

	<bean id="bco_yaw_motor" class="gda.device.motor.EpicsMotor" >
		<property name="pvName" value="BL22I-MO-BCO-01:TABLE:YAW" />
	</bean>

	<bean id="exitwindow_x_motor" class="gda.device.motor.EpicsMotor" >
		<property name="pvName" value="BL22I-MO-OAV-01:MIRROR:X" />
	</bean>
	
	<bean id="exitwindow_y_motor" class="gda.device.motor.EpicsMotor" >
		<property name="pvName" value="BL22I-MO-OAV-01:MIRROR:Y" />
	</bean>
</beans>
