<?xml version="1.0" encoding="utf-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="gonio_yaw_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL22I-EA-GONIO-01:YAW"/>
	</bean>

	<bean id="gonio_roll_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL22I-EA-GONIO-01:ROLL"/>
	</bean>

	<bean id="gonio_pitch_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL22I-EA-GONIO-01:PITCH"/>
	</bean>

</beans>
