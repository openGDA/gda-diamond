<?xml version="1.0" encoding="utf-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="feaptr_y_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="FE22I-AL-APTR-02:Y"/>
		<property name="local" value="true"/>
	</bean>

	<bean id="feaptr_x_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="FE22I-AL-APTR-02:X"/>
		<property name="local" value="true"/>
	</bean>
</beans>
