<?xml version="1.0" encoding="utf-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="mfplatform_y2_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL22I-MO-TABLE-05:Y2"/>
	</bean>

	<bean id="mfplatform_y_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL22I-MO-TABLE-05:Y"/>
	</bean>

	<bean id="mfplatform_y1_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL22I-MO-TABLE-05:Y1"/>
	</bean>

	<bean id="mfplatform_pitch_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL22I-MO-TABLE-05:PITCH"/>
	</bean>

	<bean id="mfplatform_yaw_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL22I-MO-TABLE-05:YAW"/>
	</bean>

	<bean id="mfplatform_x_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL22I-MO-TABLE-05:X"/>
	</bean>
</beans>
