<?xml version="1.0" encoding="utf-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="mfsax_z_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL22I-EA-DET-03:Z"/>
	</bean>

	<bean id="mfsax_x_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL22I-EA-DET-03:X"/>
	</bean>

	<bean id="mfsax_y_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL22I-EA-DET-03:Y"/>
	</bean>

	<bean id="mfsax_pitch_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL22I-EA-DET-03:PITCH"/>
	</bean>
</beans>
