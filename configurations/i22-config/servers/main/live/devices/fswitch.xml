<?xml version="1.0" encoding="utf-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="fswitch_x_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL22I-MO-FSWT-01:X"/>
		<property name="local" value="true"/>
	</bean>

	<bean id="fswitch_y_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL22I-MO-FSWT-01:Y"/>
		<property name="local" value="true"/>
	</bean>

	<bean id="fswitch_pitch_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL22I-MO-FSWT-01:PITCH"/>
		<property name="local" value="true"/>
	</bean>

	<bean id="fswitch_yaw_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL22I-MO-FSWT-01:YAW"/>
		<property name="local" value="true"/>
	</bean>
</beans>

