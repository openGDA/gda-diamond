<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="merlin_expsure_time_control" class="uk.ac.gda.client.livecontrol.ScannableValueControl">
		<property name="displayName" value="Exposure " />
		<property name="group" value="Merlin Acquire Time" />
		<property name="scannableName" value="merlinExposureTime" />
		<property name="textWidth" value="50" />
		<property name="userUnits" value="s" />
	</bean>
	<bean id="merlin_acquire_period_control" class="uk.ac.gda.client.livecontrol.ScannableValueControl">
		<property name="displayName" value="Period " />
		<property name="group" value="Marlin Acquire Time" />
		<property name="scannableName" value="merlinAcquirePeriod" />
		<property name="textWidth" value="50" />
		<property name="userUnits" value="s" />
	</bean>
</beans>