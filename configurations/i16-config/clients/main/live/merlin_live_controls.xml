<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="merlinExposureTime" class="gda.device.scannable.EpicsScannable">
		<property name="pvName" value="BL16I-EA-DET-13:Merlin2:AcquireTime" />
		<property name="useNameAsInputName" value="true" />
		<property name="hasUnits" value="false" />
		<property name="outputFormat">
			<list>
				<value>%4.3f</value>
			</list>
		</property>
	</bean>
	<bean id="merlinAcquirePeriod" class="gda.device.scannable.EpicsScannable">
		<property name="pvName" value="BL16I-EA-DET-13:Merlin2:AcquirePeriod" />
		<property name="useNameAsInputName" value="true" />
		<property name="hasUnits" value="false" />
		<property name="outputFormat">
			<list>
				<value>%4.3f</value>
			</list>
		</property>
	</bean>
</beans>