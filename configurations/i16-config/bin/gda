#!/bin/bash

if [ -f "/etc/profile.d/modules.sh" ]; then
  . /etc/profile.d/modules.sh
fi

module load java/gda826

PYTHON=/dls_sw/tools/bin/python2.5

GDA_VERSION_LOC=`readlink -f /dls_sw/$BEAMLINE/software/gda`

echo '      JAVA_HOME:' $JAVA_HOME
echo '         PYTHON:' $PYTHON
echo 'GDA_VERSION_LOC:' $GDA_VERSION_LOC

GDA=$GDA_VERSION_LOC/workspace_git/gda-core.git/uk.ac.gda.core/bin/gda
$PYTHON $GDA --config $GDA_VERSION_LOC/config $@

