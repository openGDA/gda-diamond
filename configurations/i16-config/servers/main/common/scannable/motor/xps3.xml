<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="xps3mot1" class="gda.device.scannable.ScannableMotor">
		<property name="motor" ref="XPS3P1Motor" />
	</bean>
	<bean id="xps3mot2" class="gda.device.scannable.ScannableMotor">
		<property name="motor" ref="XPS3P2Motor" />
	</bean>
	<bean id="xps3mot3" class="gda.device.scannable.ScannableMotor">
		<property name="motor" ref="XPS3P3Motor" />
	</bean>
	<bean id="xps3mot4" class="gda.device.scannable.ScannableMotor">
		<property name="motor" ref="XPS3P4Motor" />
	</bean>
	<bean id="xps3mot5" class="gda.device.scannable.ScannableMotor">
		<property name="motor" ref="XPS3P5Motor" />
	</bean>
	<bean id="xps3mot6" class="gda.device.scannable.ScannableMotor">
		<property name="motor" ref="XPS3P6Motor" />
	</bean>
	<bean id="xps3mot7" class="gda.device.scannable.ScannableMotor">
		<property name="motor" ref="XPS3P7Motor" />
	</bean>
	<bean id="xps3mot8" class="gda.device.scannable.ScannableMotor">
		<property name="motor" ref="XPS3P8Motor" />
	</bean>

	<bean id="xps3" class="gda.device.scannable.scannablegroup.ScannableGroup">
		<property name="groupMembers">
			<list>
				<ref bean="xps3mot1" />
				<ref bean="xps3mot2" />
				<ref bean="xps3mot3" />
				<ref bean="xps3mot4" />
				<ref bean="xps3mot5" />
				<ref bean="xps3mot6" />
				<ref bean="xps3mot7" />
				<ref bean="xps3mot8" />
			</list>
		</property>
	</bean>

</beans>
