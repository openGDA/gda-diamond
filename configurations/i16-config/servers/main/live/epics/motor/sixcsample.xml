<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
	
	<bean id="DSxMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="D.SX" />
		<property name="local" value="true" />
	</bean>
	<bean id="DSyMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="D.SY" />
		<property name="local" value="true" />
	</bean>
	<bean id="DSzMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="D.SZ" />
		<property name="local" value="true" />
	</bean>
	
</beans>
