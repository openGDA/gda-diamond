<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
	
	<bean id="S6YSizeMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S6.YG" />
		<property name="local" value="true" />
	</bean>
	<bean id="S6YTransMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S6.YT" />
		<property name="local" value="true" />
	</bean>
	<bean id="S6XSizeMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S6.XG" />
		<property name="local" value="true" />
	</bean>
	<bean id="S6XTransMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S6.XT" />
		<property name="local" value="true" />
	</bean>
	
</beans>
