<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
	
	<bean id="DBh1Motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="D.BH1" />
		<property name="local" value="true" />
	</bean>
	<bean id="DBh2Motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="D.BH2" />
		<property name="local" value="true" />
	</bean>
	<bean id="DBh3Motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="D.BH3" />
		<property name="local" value="true" />
	</bean>
	<bean id="DBtMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="D.BT" />
		<property name="local" value="true" />
	</bean>
	
</beans>
