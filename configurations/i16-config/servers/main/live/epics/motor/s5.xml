<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="S5YSizeMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S5.YG" />
		<property name="local" value="true" />
	</bean>
	<bean id="S5YTransMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S5.YT" />
		<property name="local" value="true" />
	</bean>
	<bean id="S5XSizeMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S5.XG" />
		<property name="local" value="true" />
	</bean>
	<bean id="S5XTransMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S5.XT" />
		<property name="local" value="true" />
	</bean>


</beans>
