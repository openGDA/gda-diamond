<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="D1Motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="D1" />
		<property name="local" value="true" />
	</bean>

	<bean id="D3AMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="D3.A" />
		<property name="local" value="true" />
	</bean>
	<bean id="D3DMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="D3.D" />
		<property name="local" value="true" />
	</bean>

	<bean id="D4AMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="D4.A" />
		<property name="local" value="true" />
	</bean>
	<bean id="D4DMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="D4.D" />
		<property name="local" value="true" />
	</bean>

	<bean id="D5AMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="D5.A" />
		<property name="local" value="true" />
	</bean>
	<bean id="D5DMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="D5.D" />
		<property name="local" value="true" />
	</bean>



</beans>
