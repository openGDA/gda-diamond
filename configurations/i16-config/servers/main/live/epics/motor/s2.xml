<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="S2XCentreMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S2XCENTRE" />
		<property name="local" value="true" />
	</bean>
	<bean id="S2XSizeMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S2XSIZE" />
		<property name="local" value="true" />
	</bean>
	<bean id="S2XNegMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S2XNEG" />
		<property name="local" value="true" />
	</bean>
	<bean id="S2XPosMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S2XPOS" />
		<property name="local" value="true" />
	</bean>
	<bean id="S2YCentreMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S2YCENTRE" />
		<property name="local" value="true" />
	</bean>
	<bean id="S2YSizeMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S2YSIZE" />
		<property name="local" value="true" />
	</bean>
	<bean id="S2YNegMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S2YNEG" />
		<property name="local" value="true" />
	</bean>
	<bean id="S2YPosMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S2YPOS" />
		<property name="local" value="true" />
	</bean>

</beans>
