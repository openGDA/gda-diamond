<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- Virtual Motors -->

	<bean id="SGxMotor"
			class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL16I-MO-SGON-01:X" />
	</bean>

	<bean id="SGyMotor"
			class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL16I-MO-SGON-01:Y" />
	</bean>

	<bean id="SGzMotor"
			class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL16I-MO-SGON-01:Z" />
	</bean>

	<bean id="SGomegaMotor"
			class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL16I-MO-SGON-01:OMEGA" />
	</bean>

	<bean id="SGphiMotor"
			class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL16I-MO-SGON-01:PHI" />
	</bean>

	<bean id="SGchiMotor"
			class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL16I-MO-SGON-01:CHI" />
	</bean>

	<!-- Real Motors -->

	<bean id="SGomegaRealMotor"
			class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL16I-MO-SGON-01:MOTOR_6" />
	</bean>

	<bean id="SGphiRealMotor"
			class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL16I-MO-SGON-01:MOTOR_5" />
	</bean>

</beans>
