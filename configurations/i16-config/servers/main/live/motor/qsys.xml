<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="ppxMotor"
			class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL16I-OP-QSYS-01:X" />
	</bean>

	<bean id="ppyMotor"
			class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL16I-OP-QSYS-01:Y" />
	</bean>

	<bean id="ppyawMotor"
			class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL16I-OP-QSYS-01:YAW" />
	</bean>

	<bean id="pppitchMotor"
			class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL16I-OP-QSYS-01:PITCH" />
	</bean>
</beans>