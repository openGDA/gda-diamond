<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
			http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">


	<context:property-placeholder />	

	<bean id="ringCurrent" class="gda.device.scannable.DummyScannable">
		<constructor-arg value="ringCurrent" />
		<constructor-arg value="298.246" />
	</bean>
	
	<bean id="idGap" class="gda.device.scannable.DummyScannable">
		<constructor-arg value="idGap" />
		<constructor-arg value="24.7" />
	</bean>

	<bean id="timeToRefill" class="gda.device.scannable.DummyScannable">
		<constructor-arg value="timeToRefill" />
		<constructor-arg value="580.348" />
	</bean>
	
	<bean id="expt_sht_stat" class="gda.device.enumpositioner.DummyEnumPositioner">
		<property name="positions">
			<list>
				<value>Open</value>
				<value>Closed</value>
			</list>
		</property>
	</bean>

	<bean id="fast_shtr_stat" class="gda.device.enumpositioner.DummyEnumPositioner">
		<property name="positions">
			<list>
				<value>Open</value>
				<value>Closed</value>
			</list>
		</property>
	</bean>
	
	<import resource="../../../devices/dummy/ion_chamber.xml" />		

	<import resource="../_common/client.xml" />
</beans>