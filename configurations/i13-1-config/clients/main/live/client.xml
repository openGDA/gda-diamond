<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
			http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">


	<context:property-placeholder/>

	<import resource="live_client_common.xml" />
	<import resource="../../../devices/live/pco1_ndplugins.xml" />

	<bean class="java.util.Hashtable">
		<constructor-arg>
			<map>
				<entry key="SERVICE_NAME" value="i13" />
			</map>
		</constructor-arg>
	</bean>

	<bean id="expt_sht_stat" class="gda.device.scannable.EpicsScannable">
		<property name="hasUnits" value="False" />
		<property name="getAsString" value="True" />
		<property name="pvName" value="BL13J-PS-SHTR-02:STA" />
	</bean>

	<bean id="fast_shtr_stat" class="gda.device.scannable.EpicsScannable">
		<property name="hasUnits" value="False" />
		<property name="getAsString" value="True" />
		<property name="pvName" value="BL13J-EA-FSHTR-01:CONTROL" />
	</bean>

</beans>