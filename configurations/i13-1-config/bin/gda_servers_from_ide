echo $@
export JAVA_OPTS="-Xms128m -Xmx512m -XX:MaxPermSize=128m"
python  $1/bin/gda  --config=$2 --smart --trace nameserver -v --mode=$3 $4 
python  $1/bin/gda  --config=$2 --smart --trace eventserver -v --mode=$3 $4 
export JAVA_OPTS="-Xms256m -Xmx4024m -XX:MaxPermSize=256m"
python  $1/bin/gda  --config=$2 --smart --trace objectserver -v --mode=$3 $4 $5 $6 --debug -p 8005