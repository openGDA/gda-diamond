# start logpanel if not already running

#identify install folder and JAVA_HOME
source /dls_sw/i13-1/etc/i13-1_profile.sh


# Start log panel if not already started
LOGPANEL=$(ps -ef | grep NameManager | grep -v grep)
if [ "X$LOGPANEL" == "X" ] ; then
/dls_sw/i13-1/software/gda_versions/gda_trunk2/plugins/uk.ac.gda.core/bin/gda --config=$GDAFOLDER/$BEAMLINE-config --restart  --mode=$GDAMODE namemanager
else
wmctrl -R "JacORB NameManager"
fi
