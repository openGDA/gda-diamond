<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="s2_xsize_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="s2.xsize" />
		<property name="local" value="true" />
	</bean>
	<bean id="s2_xcenter_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="s2.xcenter" />
		<property name="local" value="true" />
	</bean>
	<bean id="s2_ysize_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="s2.ysize" />
		<property name="local" value="true" />
	</bean>
	<bean id="s2_ycenter_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="s2.ycenter" />
		<property name="local" value="true" />
	</bean>
	
	<bean id="s2_xplus_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="s2.xplus" />
		<property name="local" value="true" />
	</bean>
	<bean id="s2_xminus_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="s2.xminus" />
		<property name="local" value="true" />
	</bean>
	<bean id="s2_yplus_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="s2.yplus" />
		<property name="local" value="true" />
	</bean>
	<bean id="s2_yminus_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="s2.yminus" />
		<property name="local" value="true" />	
	</bean>
</beans>	