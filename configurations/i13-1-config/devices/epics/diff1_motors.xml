<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="diff1_x_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="diff1.x" />
		<property name="local" value="true" />
	</bean>
	<bean id="diff1_y_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="diff1.y" />
		<property name="local" value="true" />
	</bean>
	<bean id="diff1_z_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="diff1.z" />
		<property name="local" value="true" />
	</bean>
	<bean id="diff1_omega_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="diff1.omega" />
		<property name="local" value="true" />
	</bean>
	<bean id="diff1_alpha_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="diff1.alpha" />
		<property name="local" value="true" />
	</bean>
	<bean id="diff1_theta_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="diff1.theta" />
		<property name="local" value="true" />
	</bean>

</beans>	