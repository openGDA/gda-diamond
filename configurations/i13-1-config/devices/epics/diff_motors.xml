<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="diff_x_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="diff.x" />
		<property name="local" value="true" />
	</bean>
	<bean id="diff_y_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="diff.y" />
		<property name="local" value="true" />
	</bean>
	<bean id="diff_z_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="diff.z" />
		<property name="local" value="true" />
	</bean>
		
	
</beans>	