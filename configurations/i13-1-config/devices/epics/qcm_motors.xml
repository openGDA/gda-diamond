<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="qcm_bragg1_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="qcm.bragg1" />
		<property name="local" value="true" />
	</bean>
	<bean id="qcm_pitch1_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="qcm.pitch1" />
		<property name="local" value="true" />
	</bean>
	<bean id="qcm_roll1_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="qcm.roll1" />
		<property name="local" value="true" />
	</bean>
	<bean id="qcm_bragg2_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="qcm.bragg2" />
		<property name="local" value="true" />
	</bean>
	<bean id="qcm_pitch2_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="qcm.pitch2" />
		<property name="local" value="true" />
	</bean>
	<bean id="qcm_roll2_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="qcm.roll2" />
		<property name="local" value="true" />
	</bean>
	<bean id="qcm_x_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="qcm.x" />
		<property name="local" value="true" />
	</bean>
	<bean id="qcm_y_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="qcm.y" />
		<property name="local" value="true" />
	</bean>
</beans>	