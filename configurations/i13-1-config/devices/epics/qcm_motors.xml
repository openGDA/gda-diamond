<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="qcm_cage1_coarsepitch" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="qcm.cage1.coarsepitch" />
		<property name="local" value="true" />
	</bean>
	<bean id="qcm_cage2_coarsepitch" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="qcm.cage2.coarsepitch" />
		<property name="local" value="true" />
	</bean>
	<bean id="qcm_cage1_coarseroll" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="qcm.cage1.coarseroll" />
		<property name="local" value="true" />
	</bean>
	<bean id="qcm_cage2_coarseroll" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="qcm.cage2.coarseroll" />
		<property name="local" value="true" />
	</bean>
	<bean id="qcm_x2_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="qcm.x2" />
		<property name="local" value="true" />
	</bean>
	<bean id="qcm_y_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="qcm.y" />
		<property name="local" value="true" />
	</bean>
</beans>	