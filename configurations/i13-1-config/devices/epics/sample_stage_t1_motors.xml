<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="t1_sx_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="t1.sx" />
		<property name="local" value="true" />
	</bean>
	<bean id="t1_sy_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="t1.sy" />
		<property name="local" value="true" />
	</bean>
	<bean id="t1_sz_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="t1.sz" />
		<property name="local" value="true" />
	</bean>
	
</beans>	