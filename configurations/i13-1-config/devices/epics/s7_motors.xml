<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="s7_xsize_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="s7.xsize" />
		<property name="local" value="true" />
	</bean>
	<bean id="s7_xcenter_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="s7.xcenter" />
		<property name="local" value="true" />
	</bean>
	<bean id="s7_ysize_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="s7.ysize" />
		<property name="local" value="true" />
	</bean>
	<bean id="s7_ycenter_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="s7.ycenter" />
		<property name="local" value="true" />
	</bean>
	
	<bean id="s7_xplus_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="s7.xplus" />
		<property name="local" value="true" />
	</bean>
	<bean id="s7_xminus_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="s7.xminus" />
		<property name="local" value="true" />
	</bean>
	<bean id="s7_yplus_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="s7.yplus" />
		<property name="local" value="true" />
	</bean>
	<bean id="s7_yminus_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="s7.yminus" />
		<property name="local" value="true" />
	</bean>	
		
	
</beans>	