<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xmlns:tx="http://www.springframework.org/schema/tx" xmlns:aop="http://www.springframework.org/schema/aop"
	xmlns:corba="http://www.diamond.ac.uk/schema/gda/corba"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.5.xsd
	http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-2.5.xsd
	http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.5.xsd
	http://www.diamond.ac.uk/schema/gda/corba http://www.diamond.ac.uk/schema/gda/corba/gda-corba-1.0.xsd">

	<bean id="pco1_cam_pvProvider"
		class="gda.device.detector.areadetector.v17.impl.SimplePVProvider">
		<property name="prefix" value="BL13J-EA-DET-01:CAM:" />
	</bean>
	<bean id="pco1_cam_base" class="gda.device.detector.areadetector.v17.impl.ADBaseImpl">
		<property name="pvProvider" ref="pco1_cam_pvProvider" />
	</bean>

	<bean id="pco1_arr_pvProvider"
		class="gda.device.detector.areadetector.v17.impl.SimplePVProvider">
		<property name="prefix" value="BL13J-EA-DET-01:ARR:" />
	</bean>

	<bean id="pco1_arr" class="gda.device.detector.areadetector.v17.impl.NDArrayImpl">
		<property name="pvProvider" ref="pco1_arr_pvProvider" />
		<property name="pluginBase">
			<bean class="gda.device.detector.areadetector.v17.impl.NDPluginBaseImpl">
				<property name="pvProvider" ref="pco1_arr_pvProvider" />
			</bean>
		</property>
	</bean>


	<bean id="pco1_stat_pvProvider"
		class="gda.device.detector.areadetector.v17.impl.SimplePVProvider">
		<property name="prefix" value="BL13J-EA-DET-01:STAT:" />
	</bean>

	<bean id="pco1_stat" class="gda.device.detector.areadetector.v17.impl.NDStatsImpl">
		<property name="pvProvider" ref="pco1_stat_pvProvider" />
		<property name="pluginBase">
			<bean class="gda.device.detector.areadetector.v17.impl.NDPluginBaseImpl">
				<property name="pvProvider" ref="pco1_stat_pvProvider" />
			</bean>
		</property>
	</bean>

	<bean id="pco1_roi1_pvProvider"
		class="gda.device.detector.areadetector.v17.impl.SimplePVProvider">
		<property name="prefix" value="BL13J-EA-DET-01:ROI1:" />
	</bean>

	<bean id="pco1_roi1" class="gda.device.detector.areadetector.v17.impl.NDROIImpl">
		<property name="pvProvider" ref="pco1_roi1_pvProvider" />
		<property name="pluginBase">
			<bean class="gda.device.detector.areadetector.v17.impl.NDPluginBaseImpl">
				<property name="pvProvider" ref="pco1_roi1_pvProvider" />
			</bean>
		</property>
	</bean>

	<bean id="pco1_roi2_pvProvider"
		class="gda.device.detector.areadetector.v17.impl.SimplePVProvider">
		<property name="prefix" value="BL13J-EA-DET-01:ROI2:" />
	</bean>

	<bean id="pco1_roi2" class="gda.device.detector.areadetector.v17.impl.NDROIImpl">
		<property name="pvProvider" ref="pco1_roi1_pvProvider" />
		<property name="pluginBase">
			<bean class="gda.device.detector.areadetector.v17.impl.NDPluginBaseImpl">
				<property name="pvProvider" ref="pco1_roi2_pvProvider" />
			</bean>
		</property>
	</bean>

	<bean id="pco1_ffmpeg1_pvProvider"
		class="gda.device.detector.areadetector.v17.impl.SimplePVProvider">
		<property name="prefix" value="BL13J-EA-DET-01:MPG1:" />
	</bean>

	<bean id="pco1_ffmpeg1"
		class="gda.device.detector.areadetector.v17.impl.FfmpegStreamImpl">
		<property name="pvProvider" ref="pco1_ffmpeg1_pvProvider" />
		<property name="pluginBase">
			<bean class="gda.device.detector.areadetector.v17.impl.NDPluginBaseImpl">
				<property name="pvProvider" ref="pco1_ffmpeg1_pvProvider" />
			</bean>
		</property>
	</bean>

	<bean id="pco1_ffmpeg2_pvProvider"
		class="gda.device.detector.areadetector.v17.impl.SimplePVProvider">
		<property name="prefix" value="BL13J-EA-DET-01:MPG2:" />
	</bean>

	<bean id="pco1_ffmpeg2"
		class="gda.device.detector.areadetector.v17.impl.FfmpegStreamImpl">
		<property name="pvProvider" ref="pco1_ffmpeg2_pvProvider" />
		<property name="pluginBase">
			<bean class="gda.device.detector.areadetector.v17.impl.NDPluginBaseImpl">
				<property name="pvProvider" ref="pco1_ffmpeg2_pvProvider" />
			</bean>
		</property>
	</bean>

	<bean id="pco1_ndfilehdf5_pvProvider"
		class="gda.device.detector.areadetector.v17.impl.SimplePVProvider">
		<property name="prefix" value="BL13J-EA-DET-01:HDF:" />
	</bean>

	<bean id="pco1_ndfilehdf5_base" class="gda.device.detector.areadetector.v17.impl.NDFileImpl">
		<property name="pvProvider" ref="pco1_ndfilehdf5_pvProvider" />
		<property name="pluginBase">
			<bean class="gda.device.detector.areadetector.v17.impl.NDPluginBaseImpl">
				<property name="pvProvider" ref="pco1_ndfilehdf5_pvProvider" />
			</bean>
		</property>
		<property name="resetToInitialValues" value="false"/>
		<property name="filePathConverter">
			<bean class="uk.ac.gda.util.UnixToWindowsFilePathConverter">
				<property name="unixSubString" value="/dls/i13/data" />
				<property name="windowsSubString" value="z:\data" />
			</bean>
		</property>
	</bean>
	<bean id="pco1_ndhdf5file" class="gda.device.detector.areadetector.v17.impl.NDFileHDF5Impl">
		<property name="pvProvider" ref="pco1_ndfilehdf5_pvProvider" />
		<property name="file" ref="pco1_ndfilehdf5_base"/>
	</bean>

	<bean id="pco1_ndfiletif_pvProvider"
		class="gda.device.detector.areadetector.v17.impl.SimplePVProvider">
		<property name="prefix" value="BL13J-EA-DET-01:TIF:" />
	</bean>

	<bean id="pco1_ndfiletif" class="gda.device.detector.areadetector.v17.impl.NDFileImpl">
		<property name="pvProvider" ref="pco1_ndfiletif_pvProvider" />
		<property name="pluginBase">
			<bean class="gda.device.detector.areadetector.v17.impl.NDPluginBaseImpl">
				<property name="pvProvider" ref="pco1_ndfiletif_pvProvider" />
			</bean>
		</property>
		<property name="resetToInitialValues" value="false"/>
		<property name="filePathConverter">
			<bean class="uk.ac.gda.util.UnixToWindowsFilePathConverter">
				<property name="unixSubString" value="/dls/i13/data" />
				<property name="windowsSubString" value="z:\data" />
			</bean>
		</property>
	</bean>

	<bean id="pco1_proc1_pvProvider"
		class="gda.device.detector.areadetector.v17.impl.SimplePVProvider">
		<property name="prefix" value="BL13J-EA-DET-01:PRO1:" />
	</bean>

	<bean id="pco1_proc1" class="gda.device.detector.areadetector.v17.impl.NDProcessImpl">
		<property name="pvProvider" ref="pco1_proc1_pvProvider" />
		<property name="pluginBase">
			<bean class="gda.device.detector.areadetector.v17.impl.NDPluginBaseImpl">
				<property name="pvProvider" ref="pco1_proc1_pvProvider" />
			</bean>
		</property>
	</bean>

	<bean id="pco1_proc2_pvProvider"
		class="gda.device.detector.areadetector.v17.impl.SimplePVProvider">
		<property name="prefix" value="BL13J-EA-DET-01:PRO2:" />
	</bean>

	<bean id="pco1_proc2" class="gda.device.detector.areadetector.v17.impl.NDProcessImpl">
		<property name="pvProvider" ref="pco1_proc2_pvProvider" />
		<property name="pluginBase">
			<bean class="gda.device.detector.areadetector.v17.impl.NDPluginBaseImpl">
				<property name="pvProvider" ref="pco1_proc2_pvProvider" />
			</bean>
		</property>
	</bean>



</beans>	
