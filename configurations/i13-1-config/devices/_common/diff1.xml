<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">


	<bean id="diff1_x" class="gda.device.scannable.ScannableMotor">
		<property name="motor" ref="diff1_x_motor" />
		<property name="userUnits" value="mm"/>
	</bean>
	<bean id="diff1_y" class="gda.device.scannable.ScannableMotor">
		<property name="motor" ref="diff1_y_motor" />
		<property name="userUnits" value="mm"/>
	</bean>
	<bean id="diff1_z" class="gda.device.scannable.ScannableMotor">
		<property name="motor" ref="diff1_z_motor" />
		<property name="userUnits" value="mm"/>
	</bean>
	<bean id="diff1_omega" class="gda.device.scannable.ScannableMotor">
		<property name="motor" ref="diff1_omega_motor" />
		<property name="userUnits" value="deg"/>
	</bean>
	<bean id="diff1_alpha" class="gda.device.scannable.ScannableMotor">
		<property name="motor" ref="diff1_alpha_motor" />
		<property name="userUnits" value="deg"/>
	</bean>
	<bean id="diff1_theta" class="gda.device.scannable.ScannableMotor">
		<property name="motor" ref="diff1_theta_motor" />
		<property name="userUnits" value="deg"/>
	</bean>

</beans>