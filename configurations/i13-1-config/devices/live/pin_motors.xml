<?xml version='1.0' encoding='UTF-8'?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="pin_x_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13J-AL-APTR-01:X" />
	</bean>

	<bean id="pin_y_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13J-AL-APTR-01:Y" />
	</bean>

	<bean id="pin_z_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13J-AL-APTR-01:Z" />
	</bean>

</beans>