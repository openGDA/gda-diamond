<?xml version='1.0' encoding='UTF-8'?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="s2_xsize_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13J-AL-SLITS-02:X:SIZE" />
		<property name="local" value="true" />
	</bean>

	<bean id="s2_xcenter_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13J-AL-SLITS-02:X:CENTER" />
		<property name="local" value="true" />
	</bean>

	<bean id="s2_ysize_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13J-AL-SLITS-02:Y:SIZE" />
		<property name="local" value="true" />
	</bean>

	<bean id="s2_ycenter_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13J-AL-SLITS-02:Y:CENTER" />
		<property name="local" value="true" />
	</bean>

	<bean id="s2_xplus_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13J-AL-SLITS-02:X:PLUS" />
		<property name="local" value="true" />
	</bean>

	<bean id="s2_xminus_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13J-AL-SLITS-02:X:MINUS" />
		<property name="local" value="true" />
	</bean>

	<bean id="s2_yplus_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13J-AL-SLITS-02:Y:PLUS" />
		<property name="local" value="true" />
	</bean>

	<bean id="s2_yminus_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13J-AL-SLITS-02:Y:MINUS" />
		<property name="local" value="true" />
	</bean>

</beans>