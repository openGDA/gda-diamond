<?xml version='1.0' encoding='UTF-8'?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="s1_xsize_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13J-AL-SLITS-01:X:SIZE" />
		<property name="local" value="true" />
	</bean>

	<bean id="s1_xcenter_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13J-AL-SLITS-01:X:CENTER" />
		<property name="local" value="true" />
	</bean>

	<bean id="s1_ysize_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13J-AL-SLITS-01:Y:SIZE" />
		<property name="local" value="true" />
	</bean>

	<bean id="s1_ycenter_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13J-AL-SLITS-01:Y:CENTER" />
		<property name="local" value="true" />
	</bean>

	<bean id="s1_xplus_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13J-AL-SLITS-01:X:PLUS" />
		<property name="local" value="true" />
	</bean>

	<bean id="s1_xminus_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13J-AL-SLITS-01:X:MINUS" />
		<property name="local" value="true" />
	</bean>

	<bean id="s1_yplus_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13J-AL-SLITS-01:Y:PLUS" />
		<property name="local" value="true" />
	</bean>

	<bean id="s1_yminus_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13J-AL-SLITS-01:Y:MINUS" />
		<property name="local" value="true" />
	</bean>

</beans>