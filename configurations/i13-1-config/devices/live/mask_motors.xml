<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="mask_x_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13J-MO-SMAR-01:MASK:X" />
	</bean>
	<bean id="mask_y_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13J-MO-SMAR-01:MASK:Y" />
	</bean>
	<bean id="mask_z_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13J-MO-SMAR-01:MASK:Z" />
	</bean>

</beans>	