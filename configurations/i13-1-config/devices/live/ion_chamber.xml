<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">


	<description> Ion chamber in Experiment Hutch</description>

	<bean id="ionc_i" class="gda.device.monitor.EpicsMonitor">
		<property name="pvName" value="BL13J-DI-IONC-01:I"/>
	</bean>

	<bean id="ionc_gain" class="gda.device.monitor.EpicsMonitor">
		<property name="pvName" value="BL13J-DI-FEMTO-06:GAIN"/>
	</bean>

	<bean id="ionc_gainmode" class="gda.device.monitor.EpicsMonitor">
		<property name="pvName" value="BL13J-DI-FEMTO-06:GAINMODE" />
	</bean>

	<bean id="ionc_coupling" class="gda.device.monitor.EpicsMonitor">
		<property name="pvName" value="BL13J-DI-FEMTO-06:ACDC"/>
	</bean>

	<bean id="ionc_photonflux" class="gda.device.monitor.EpicsMonitor">
		<property name="pvName" value="BL13J-DI-IONC-01:HRPHOTONRATE"/>
	</bean>

	<bean id="ic2" class="gda.device.monitor.EpicsMonitor">
		<property name="pvName" value="BL13J-DI-IONC-02:I"/>
	</bean>

	<bean id="ic2_rate" class="gda.device.monitor.EpicsMonitor">
		<property name="pvName" value="BL13J-DI-IONC-02:HRPHOTONRATE"/>
	</bean>

</beans>
