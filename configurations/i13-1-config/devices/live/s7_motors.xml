<?xml version='1.0' encoding='UTF-8'?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="s7_xsize_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13J-AL-SLITS-07:X:SIZE" />
	</bean>

	<bean id="s7_xcenter_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13J-AL-SLITS-07:X:CENTER" />
	</bean>

	<bean id="s7_ysize_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13J-AL-SLITS-07:Y:SIZE" />
	</bean>

	<bean id="s7_ycenter_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13J-AL-SLITS-07:Y:CENTER" />
	</bean>

	<bean id="s7_xplus_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13J-AL-SLITS-07:X:PLUS" />
	</bean>

	<bean id="s7_xminus_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13J-AL-SLITS-07:X:MINUS" />
	</bean>

	<bean id="s7_yplus_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13J-AL-SLITS-07:Y:PLUS" />
	</bean>

	<bean id="s7_yminus_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13J-AL-SLITS-07:Y:MINUS" />
	</bean>

</beans>