<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="sample_lab_x_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13J-MO-PTYCH-01:SX"/>
		<property name="local" value="true" />
	</bean>
	<bean id="sample_lab_y_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13J-MO-PTYCH-01:SY"/>
		<property name="local" value="true" />
	</bean>
	<bean id="sample_lab_z_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13J-MO-PTYCH-01:SZ"/>
		<property name="local" value="true" />
	</bean>
</beans>	