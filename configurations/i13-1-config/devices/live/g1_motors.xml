<?xml version='1.0' encoding='UTF-8'?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="g1_pitch_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13J-MO-GONI-01:PITCH" />
	</bean>

	<bean id="g1_roll_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13J-MO-GONI-01:ROLL" />
	</bean>

</beans>