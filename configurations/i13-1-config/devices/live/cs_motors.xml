<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- central-stop motors -->

	<bean id="cs_x_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13J-MO-SMAR-01:CS:X"/>
	</bean>
	<bean id="cs_y_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13J-MO-SMAR-01:CS:Y"/>
	</bean>
	<bean id="cs_z_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13J-MO-SMAR-01:CS:Z"/>
	</bean>
</beans>