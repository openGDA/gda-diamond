<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="newp_x_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13J-MO-NEWP-01:X"/>
		<property name="local" value="true" />
	</bean>
	<bean id="newp_y_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13J-MO-NEWP-01:Y"/>
		<property name="local" value="true" />
	</bean>
	<bean id="newp_z_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13J-MO-NEWP-01:Z"/>
		<property name="local" value="true" />
	</bean>

</beans>
