<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<!-- order-sorting-aperture motors -->

	<bean id="misc_smaract_x_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13J-MO-SMAR-01:MISCX"/>
	</bean>
	<bean id="misc_smaract_y_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13J-MO-SMAR-01:MISCY"/>
	</bean>
	<bean id="misc_smaract_z_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13J-MO-SMAR-01:MISCZ"/>
	</bean>
</beans>