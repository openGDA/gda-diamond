<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
           
    <!-- zone-plate motors -->
    
	<bean id="zp_x_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-9000.000" />
		<property name="maxPosition" value="80000.000" />
		<property name="position" value="-5.614" />
	</bean>
           
	<bean id="zp_y_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-80000.000" />
		<property name="maxPosition" value="8000.000" />
		<property name="position" value="-5000.614" />
	</bean>
           
	<bean id="zp_z_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-8000.000" />
		<property name="maxPosition" value="8000.000" />
		<property name="position" value="5.656" />
	</bean>

</beans>