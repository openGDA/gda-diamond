<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="newp_x_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="0.0" />
		<property name="maxPosition" value="25.6" />
		<property name="position" value="12.4" />
	</bean>
           
	<bean id="newp_y_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="0.0" />
		<property name="maxPosition" value="25.7" />
		<property name="position" value="0.0" />
	</bean>
           
	<bean id="newp_z_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="0.0" />
		<property name="maxPosition" value="25.8" />
		<property name="position" value="8.1" />
	</bean>
</beans>
