<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="fes1_xsize_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-40.824" />
		<property name="maxPosition" value="48.524" />
		<property name="position" value="-3.1769" />
	</bean>

	<bean id="fes1_xcenter_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-20.152" />
		<property name="maxPosition" value="22.822" />
		<property name="position" value="-1.3283" />
	</bean>

	<bean id="fes1_ysize_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-6.2616" />
		<property name="maxPosition" value="93.460" />
		<property name="position" value="33.409" />
	</bean>

	<bean id="fes1_ycenter_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-18.884" />
		<property name="maxPosition" value="20.925" />
		<property name="position" value="0.9508" />
	</bean>

	<bean id="portshutter" class="gda.device.monitor.DummyEpicsMonitor">
		<property name="value" value="Open" />
	</bean>

</beans>