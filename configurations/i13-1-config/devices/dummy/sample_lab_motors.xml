<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
           
	<bean id="sample_lab_x_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-8000.000" />
		<property name="maxPosition" value="8000.000" />
		<property name="position" value="1000.000" />
	</bean>
           
	<bean id="sample_lab_y_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-8000.000" />
		<property name="maxPosition" value="8000.000" />
		<property name="position" value="0.000" />
	</bean>
           
	<bean id="sample_lab_z_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-8000.000" />
		<property name="maxPosition" value="8000.000" />
		<property name="position" value="675.800" />
	</bean>

</beans>