<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<description> Ion chamber in Experiment Hutch</description>

	<bean id="ionc_i" class="gda.device.scannable.SimpleScannable">
		<property name="currentPosition" value="1.33" />
		<property name="inputNames" value="ionc_i" />
	</bean>

	<bean id="ionc_gain" class="gda.device.scannable.SimpleScannable">
		<property name="inputNames" value="ionc_gain" />
		<property name="currentPosition" value="10^6 low noise" />
	</bean>

	<bean id="ionc_coupling" class="gda.device.scannable.SimpleScannable">
		<property name="inputNames" value="ionc_coupling" />
		<property name="currentPosition" value="DC" />
	</bean>

	<bean id="ionc_photonflux" class="gda.device.scannable.SimpleScannable">
		<property name="inputNames" value="ionc_photonflux" />
		<property name="currentPosition" value="617.815" />
	</bean>

	<bean id="ic2" class="gda.device.scannable.SimpleScannable">
		<property name="inputNames" value="ic2" />
		<property name="currentPosition" value="-0.00473966" />
	</bean>

	<bean id="ic2_rate" class="gda.device.scannable.SimpleScannable">
		<property name="inputNames" value="ic2_rate" />
		<property name="currentPosition" value="0" />
	</bean>

</beans>
	
