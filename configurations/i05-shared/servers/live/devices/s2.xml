<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="s2_xcentre_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL05I-AL-SLITS-02:XCENTRE" />
	</bean>

	<bean id="s2_xgap_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL05I-AL-SLITS-02:XSIZE" />
	</bean>

	<bean id="s2_ycentre_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL05I-AL-SLITS-02:YCENTRE" />
	</bean>

	<bean id="s2_ygap_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL05I-AL-SLITS-02:YSIZE" />
	</bean>

	<bean id="s2_xring_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL05I-AL-SLITS-02:XRING" />
	</bean>

	<bean id="s2_xhall_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL05I-AL-SLITS-02:XHALL" />
	</bean>

	<bean id="s2_ytop_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL05I-AL-SLITS-02:YPLUS" />
	</bean>

	<bean id="s2_ybottom_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL05I-AL-SLITS-02:YMINUS" />
	</bean>
</beans>