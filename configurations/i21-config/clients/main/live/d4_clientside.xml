<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:corba="http://www.diamond.ac.uk/schema/gda/corba"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">


	<bean id="d4CamBase" class="gda.spring.V17ADBaseFactoryBean">
		<property name="prefix" value="BL21I-DI-DCAM-04:CAM:" />
	</bean>

	<bean id="d4Arr" class="gda.spring.V17NDArrayFactoryBean">
		<property name="prefix" value="BL21I-DI-DCAM-04:ARR:" />
	</bean>

	<bean id="d4Stat" class="gda.spring.V17NDStatsFactoryBean">
		<property name="prefix" value="BL21I-DI-DCAM-04:STAT:" />
	</bean>

	<bean id="d4Proc" class="gda.spring.V17NDProcessFactoryBean">
		<property name="prefix" value="BL21I-DI-DCAM-04:PROC:" />
	</bean>

	<bean id="d4Roi" class="gda.spring.V17NDROIFactoryBean">
		<property name="prefix" value="BL21I-DI-DCAM-04:ROI:" />
	</bean>

	<bean id="d4Ffmpeg" class="gda.spring.V17FfmpegStreamFactoryBean">
		<property name="prefix" value="BL21I-DI-DCAM-04:FFMPEG:" />
	</bean>

</beans>