<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:corba="http://www.diamond.ac.uk/schema/gda/corba"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">


	<bean id="d1CamBase" class="gda.spring.V17ADBaseFactoryBean">
		<property name="prefix" value="BL21I-DI-DCAM-01:CAM:" />
	</bean>

	<bean id="d1Arr" class="gda.spring.V17NDArrayFactoryBean">
		<property name="prefix" value="BL21I-DI-DCAM-01:ARR:" />
	</bean>

	<bean id="d1Stat" class="gda.spring.V17NDStatsFactoryBean">
		<property name="prefix" value="BL21I-DI-DCAM-01:STAT:" />
	</bean>

	<bean id="d1Proc" class="gda.spring.V17NDProcessFactoryBean">
		<property name="prefix" value="BL21I-DI-DCAM-01:PROC:" />
	</bean>

	<bean id="d1Roi" class="gda.spring.V17NDROIFactoryBean">
		<property name="prefix" value="BL21I-DI-DCAM-01:ROI:" />
	</bean>

	<bean id="d1Ffmpeg" class="gda.spring.V17FfmpegStreamFactoryBean">
		<property name="prefix" value="BL21I-DI-DCAM-01:FFMPEG:" />
	</bean>

</beans>