<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:corba="http://www.diamond.ac.uk/schema/gda/corba"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">


	<bean id="d2CamBase" class="gda.spring.V17ADBaseFactoryBean">
		<property name="prefix" value="BL21I-DI-DCAM-02:CAM:" />
	</bean>

	<bean id="d2Arr" class="gda.spring.V17NDArrayFactoryBean">
		<property name="prefix" value="BL21I-DI-DCAM-02:ARR:" />
	</bean>

	<bean id="d2Stat" class="gda.spring.V17NDStatsFactoryBean">
		<property name="prefix" value="BL21I-DI-DCAM-02:STAT:" />
	</bean>

	<bean id="d2Proc" class="gda.spring.V17NDProcessFactoryBean">
		<property name="prefix" value="BL21I-DI-DCAM-02:PROC:" />
	</bean>

	<bean id="d2Roi" class="gda.spring.V17NDROIFactoryBean">
		<property name="prefix" value="BL21I-DI-DCAM-02:ROI:" />
	</bean>

	<bean id="d2Ffmpeg" class="gda.spring.V17FfmpegStreamFactoryBean">
		<property name="prefix" value="BL21I-DI-DCAM-02:FFMPEG:" />
	</bean>

</beans>