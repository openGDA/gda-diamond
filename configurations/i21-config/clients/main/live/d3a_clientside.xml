<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:corba="http://www.diamond.ac.uk/schema/gda/corba"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">


	<bean id="d3CamBase" class="gda.spring.V17ADBaseFactoryBean">
		<property name="prefix" value="BL21I-DI-DCAM-03:CAM:" />
	</bean>

	<bean id="d3Arr" class="gda.spring.V17NDArrayFactoryBean">
		<property name="prefix" value="BL21I-DI-DCAM-03:ARR:" />
	</bean>

	<bean id="d3Stat" class="gda.spring.V17NDStatsFactoryBean">
		<property name="prefix" value="BL21I-DI-DCAM-03:STAT:" />
	</bean>

	<bean id="d3Proc" class="gda.spring.V17NDProcessFactoryBean">
		<property name="prefix" value="BL21I-DI-DCAM-03:PROC:" />
	</bean>

	<bean id="d3Roi" class="gda.spring.V17NDROIFactoryBean">
		<property name="prefix" value="BL21I-DI-DCAM-03:ROI:" />
	</bean>

	<bean id="d3Ffmpeg" class="gda.spring.V17FfmpegStreamFactoryBean">
		<property name="prefix" value="BL21I-DI-DCAM-03:FFMPEG:" />
	</bean>

</beans>