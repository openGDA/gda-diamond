<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:corba="http://www.diamond.ac.uk/schema/gda/corba"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">


	<bean id="s5CamBase" class="gda.spring.V17ADBaseFactoryBean">
		<property name="prefix" value="BL21I-DI-DCAM-55:CAM:" />
	</bean>

	<bean id="s5Arr" class="gda.spring.V17NDArrayFactoryBean">
		<property name="prefix" value="BL21I-DI-DCAM-55:ARR:" />
	</bean>

	<bean id="s5Stat" class="gda.spring.V17NDStatsFactoryBean">
		<property name="prefix" value="BL21I-DI-DCAM-55:STAT:" />
	</bean>

	<bean id="s5Proc" class="gda.spring.V17NDProcessFactoryBean">
		<property name="prefix" value="BL21I-DI-DCAM-55:PROC:" />
	</bean>

	<bean id="s5Roi" class="gda.spring.V17NDROIFactoryBean">
		<property name="prefix" value="BL21I-DI-DCAM-55:ROI:" />
	</bean>

	<bean id="s5Ffmpeg" class="gda.spring.V17FfmpegStreamFactoryBean">
		<property name="prefix" value="BL21I-DI-DCAM-55:FFMPEG:" />
	</bean>

</beans>