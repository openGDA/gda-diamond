<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:corba="http://www.diamond.ac.uk/schema/gda/corba"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.5.xsd">


	<bean id="simsapolar" class="gda.device.scannable.ScannableMotor">
		<property name="motor" ref="smprzDummyMotor" />
		<property name="outputFormat">
			<list>
				<value>%10.6f</value>
			</list>
		</property>
		<property name="userUnits" value="deg" />
	</bean>

	<bean id="simsax" class="gda.device.scannable.ScannableMotor">
		<property name="motor" ref="smpxDummyMotor" />
		<property name="outputFormat">
			<list>
				<value>%10.6f</value>
			</list>
		</property>
		<property name="userUnits" value="mm" />
	</bean>

	<bean id="simsay" class="gda.device.scannable.ScannableMotor">
		<property name="motor" ref="smpyDummyMotor" />
		<property name="outputFormat">
			<list>
				<value>%10.6f</value>
			</list>
		</property>
		<property name="userUnits" value="mm" />
	</bean>

	<bean id="simsaz" class="gda.device.scannable.ScannableMotor">
		<property name="motor" ref="smpzDummyMotor" />
		<property name="outputFormat">
			<list>
				<value>%10.6f</value>
			</list>
		</property>
		<property name="userUnits" value="mm" />
	</bean>
	<bean id="simsaazimuth" class="gda.device.scannable.ScannableMotor">
		<property name="motor" ref="smpazimuthDummyMotor" />
		<property name="outputFormat">
			<list>
				<value>%10.6f</value>
			</list>
		</property>
		<property name="userUnits" value="deg" />
	</bean>
	<bean id="simdiodetth" class="gda.device.scannable.ScannableMotor">
		<property name="motor" ref="smpdiodetthDummyMotor" />
		<property name="outputFormat">
			<list>
				<value>%10.6f</value>
			</list>
		</property>
		<property name="userUnits" value="deg" />
	</bean>
	<bean id="simsatilt" class="gda.device.scannable.ScannableMotor">
		<property name="motor" ref="smptiltDummyMotor" />
		<property name="outputFormat">
			<list>
				<value>%10.6f</value>
			</list>
		</property>
		<property name="userUnits" value="deg" />
	</bean>
	
	<bean id="simm5tth" class="gda.device.scannable.ScannableMotor">
		<property name="motor" ref="m5rstageDummyMotor" />
		<property name="userUnits" value="deg"/>
	</bean>
	
	<!-- Group d4-related objects -->	
 	<bean id="simsmp" class="gda.device.scannable.scannablegroup.ScannableGroup">
		<property name="groupMembers">
			<list>
				<ref bean="simsapolar" />
				<ref bean="simsax" />
				<ref bean="simsay" />
				<ref bean="simsaz" />
				<ref bean="simsaazimuth" />
				<ref bean="simdiodetth" />
				<ref bean="simsatilt" />
				<ref bean="simm5tth" />
			</list>
		</property>
		<property name="local" value="true" />
	</bean>

 </beans>