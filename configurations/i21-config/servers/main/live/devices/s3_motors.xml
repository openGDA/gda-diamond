<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="s3hcentreMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-AL-SLITS-03:X:CENTER" />
	</bean>

	<bean id="s3hsizeMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-AL-SLITS-03:X:SIZE" />
	</bean>

	<bean id="s3vcentreMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-AL-SLITS-03:Y:CENTER" />
	</bean>

	<bean id="s3vsizeMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-AL-SLITS-03:Y:SIZE" />
	</bean>

	<bean id="s3offsideMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-AL-SLITS-03:OFFSIDE" />
	</bean>

	<bean id="s3nearsideMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-AL-SLITS-03:NEARSIDE" />
	</bean>

	<bean id="s3upperMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-AL-SLITS-03:UPPER" />
	</bean>

	<bean id="s3lowerMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-AL-SLITS-03:LOWER" />
	</bean>

</beans>
