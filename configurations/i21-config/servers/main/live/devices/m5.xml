		<property name="pvName" value="BL21I-MO-POD-03:M5LQ:RY" />
		<property name="local" value="true" />
	</bean>

	<bean id="m5lqrzMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-MO-POD-03:M5LQ:RZ" />
		<property name="local" value="true" />
	</bean>

	<!-- M5 stage -->
	<bean id="m5vstageMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-MO-POD-02:VERT" />
		<property name="local" value="true" />
	</bean>

	<bean id="m5rstageMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-MO-POD-02:ROT" />
		<property name="local" value="true" />
