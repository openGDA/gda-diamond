<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd">

<!-- Left Mirror -->
	<bean id="m5hqxt" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-MO-POD-02:M5HQ:X" />
		<property name="local" value="true" />
	</bean>

	<bean id="m5hqyt" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-MO-POD-02:M5HQ:Y" />
		<property name="local" value="true" />
	</bean>

	<bean id="m5hqzt" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-MO-POD-02:M5HQ:Z" />
		<property name="local" value="true" />
	</bean>

	<bean id="m5hqrxMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-MO-POD-02:M5HQ:RX" />
		<property name="local" value="true" />
	</bean>

	<bean id="m5hqryMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-MO-POD-02:M5HQ:RY" />
		<property name="local" value="true" />
	</bean>

	<bean id="m5hqrzMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-MO-POD-02:M5HQ:RZ" />
		<property name="local" value="true" />
	</bean>

<!-- Right Mirror -->
	<bean id="m5lqxt" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-MO-POD-03:M5LQ:X" />
		<property name="local" value="true" />
	</bean>

	<bean id="m5lqyt" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-MO-POD-03:M5LQ:Y" />
		<property name="local" value="true" />
	</bean>

	<bean id="m5lqzt" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-MO-POD-03:M5LQ:Z" />
		<property name="local" value="true" />
	</bean>

	<bean id="m5lqrxMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-MO-POD-03:M5LQ:RX" />
