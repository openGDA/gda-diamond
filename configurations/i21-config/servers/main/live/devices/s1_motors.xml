<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="s1xcenterMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-AL-SLITS-01:X:CENTER.MOVN" />
		<property name="local" value="true" />
	</bean>
	
	<bean id="s1xsizeMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-AL-SLITS-01:X:SIZE.MOVN" />
		<property name="local" value="true" />
	</bean>
	
	<bean id="s1ycenterMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-AL-SLITS-01:Y:CENTER.MOVN" />
		<property name="local" value="true" />
	</bean>
	
	<bean id="s1ysizeMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-AL-SLITS-01:Y:SIZE.MOVN" />
		<property name="local" value="true" />
	</bean>

</beans>
