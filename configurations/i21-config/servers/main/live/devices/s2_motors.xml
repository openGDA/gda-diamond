<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="s2hcentreMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-AL-SLITS-02:X:CENTER" />
	</bean>

	<bean id="s2hsizeMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-AL-SLITS-02:X:SIZE" />
	</bean>

	<bean id="s2vcentreMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-AL-SLITS-02:Y:CENTER" />
	</bean>

	<bean id="s2vsizeMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-AL-SLITS-02:Y:SIZE" />
	</bean>

	<bean id="s2offsideMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-AL-SLITS-02:OFFSIDE" />
	</bean>

	<bean id="s2nearsideMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-AL-SLITS-02:NEARSIDE" />
	</bean>

	<bean id="s2upperMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-AL-SLITS-02:UPPER" />
	</bean>

	<bean id="s2lowerMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-AL-SLITS-02:LOWER" />
	</bean>

</beans>
