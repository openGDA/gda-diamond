<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="m4xTransMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-MO-POD-01:X" />
	</bean>

	<bean id="m4yTransMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-MO-POD-01:Y" />
	</bean>

	<bean id="m4zTransMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-MO-POD-01:Z" />
	</bean>

	<bean id="m4xRotMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-MO-POD-01:RX" />
	</bean>

	<bean id="m2yRotMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-MO-POD-01:RY" />
	</bean>

	<bean id="m4zRotMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-MO-POD-01:RZ" />
	</bean>

	<bean id="m4VerticalStage" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-MO-POD-01:VERT" />
	</bean>

	<!-- Femto -->
	<bean id="m4femto1" class="gda.device.currentamplifier.EpicsCurrAmpSingle">
		<property name="pvName" value="BL21I-MO-POD-01:FEMTO1" />
	</bean>
	<bean id="m4femto2" class="gda.device.currentamplifier.EpicsCurrAmpSingle">
		<property name="pvName" value="BL21I-MO-POD-01:FEMTO2" />
	</bean>

</beans>