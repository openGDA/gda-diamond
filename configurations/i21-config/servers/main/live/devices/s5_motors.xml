<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="s5v1gapMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-AL-SLITS-05:V1GAP" />
		<property name="local" value="true" />
	</bean>

	<bean id="s5v2gapMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-AL-SLITS-05:V2GAP" />
		<property name="local" value="true" />
	</bean>

	<bean id="s5hgapMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-AL-SLITS-05:HGAP" />
		<property name="local" value="true" />
	</bean>

	<bean id="s5sutMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-AL-SLITS-05:SUT" />
		<property name="local" value="true" />
	</bean>

	<bean id="s5vdso1Motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-AL-SLITS-05:VDSO1" />
		<property name="local" value="true" />
	</bean>

	<bean id="s5vdso2Motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-AL-SLITS-05:VDSO2" />
		<property name="local" value="true" />
	</bean>

	<bean id="s5hdsoMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-AL-SLITS-05:HDSO" />
		<property name="local" value="true" />
	</bean>

</beans>
