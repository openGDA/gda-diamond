<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="polariserStickMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-MO-POL-01:STICK" />
	</bean>
	<bean id="polariserSigmaMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-MO-POL-02:SIGMA:Z" />
	</bean>

</beans>
