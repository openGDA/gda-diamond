<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="eax" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-MO-ELAN-01:X" />
	</bean>
	<bean id="eay" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-MO-ELAN-01:Y" />
	</bean>
	<bean id="eaz" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-MO-ELAN-01:Z" />
	</bean>
	<bean id="eapolar" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL21I-MO-ELAN-01:POLAR" />
	</bean>

</beans>
