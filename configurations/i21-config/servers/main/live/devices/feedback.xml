<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="feedbackStart" class="gda.device.scannable.EpicsScannable">
		<property name="pvName" value="BL21I-OP-MIRR-01:FBCTRL:START.PROC"/>
		<property name="useNameAsInputName" value="true"/>	
		<property name="hasUnits" value="false"/>	
		<property name="outputFormat">
			<list>
				<value>%1d</value>
			</list>
		</property>	
	</bean>

</beans>
