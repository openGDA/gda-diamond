<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd">


	<!-- Stick motor -->
	<bean id="d7motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-1000" />
		<property name="maxPosition" value="1000" />
		<property name="speed" value="90" />
	</bean>
	<!-- Femto -->
	<bean id="d7femto1" class="gda.device.currentamplifier.DummyEpicsCurrAmpSingle"/>
	<bean id="d7femto2" class="gda.device.currentamplifier.DummyEpicsCurrAmpSingle"/>

</beans>
