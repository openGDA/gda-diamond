<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="polariserStickMotor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-1000" />
		<property name="maxPosition" value="1000" />
		<property name="speed" value="0.60" />
		<property name="local" value="true" />
	</bean>

	<bean id="polariserGammaMotor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-1000" />
		<property name="maxPosition" value="1000" />
		<property name="speed" value="7.20" />
		<property name="local" value="true" />
	</bean>

</beans>
