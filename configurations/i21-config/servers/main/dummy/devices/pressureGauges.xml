<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="vaGauge01" class="gda.device.monitor.DummyEpicsMonitorVaryingValue">
		<property name="unit" value="mbar" />
		<property name="value">
			<value type="double">1.4e-6</value>
		</property>
		<property name="lowerLimit" value="0" />
		<property name="upperLimit" value="3.0e-6" />
		<property name="increment" value="0.2491e-6" />
	</bean>

</beans>
