	<bean id="m2temp1" class="gda.device.monitor.DummyEpicsMonitorDouble">
		<property name="unit" value="°C" />
		<property name="value">
			<value type="double">24.6</value>
		</property>
		<property name="lowerLimit" value="20" />
		<property name="upperLimit" value="30" />
