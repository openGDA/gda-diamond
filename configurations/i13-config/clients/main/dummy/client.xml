<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
    
    <import resource="../../../devices/dummy/pco1_ndplugins.xml"/>
	<import resource="../../../devices/dummy/ion_chamber.xml" />

    <bean id="idGap" class="gda.device.scannable.DummyScannable">
		<property name="name" value="idGap"/>
	</bean>
    
    <bean id="timeToRefill" class="gda.device.scannable.DummyScannable">
		<property name="name" value="timeToRefill"/>
	</bean>
    
    <bean id="ringCurrent" class="gda.device.scannable.DummyScannable">
		<property name="name" value="ringCurrent"/>
	</bean>
    
    <bean id="pco_binx_rbv" class="gda.device.scannable.DummyScannable">
		<property name="name" value="pco_binx_rbv"/>
	</bean>
    
    <bean id="pco_biny_rbv" class="gda.device.scannable.DummyScannable">
		<property name="name" value="pco_biny_rbv"/>
	</bean>
    
    <bean id="expt_sht_stat" class="gda.device.scannable.DummyScannable">
		<property name="name" value="expt_sht_stat"/>
	</bean>
    
    <bean id="fast_shtr_stat" class="gda.device.scannable.DummyScannable">
		<property name="name" value="fast_shtr_stat"/>
	</bean>
    
    <import resource="../_common/client.xml"/>

</beans>