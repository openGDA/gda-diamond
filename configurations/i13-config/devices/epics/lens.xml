<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

 	<bean id="lens" class="gda.device.enumpositioner.EpicsPneumaticCallback">
		<property name="controlPv" value="BL13I-EA-TURR-01:DEMAND"/>
		<property name="statusPv" value="BL13I-EA-TURR-01:CURRENTPOS"/>
		<property name="statusPvIndicatesPositionOnly" value="true"/>
		<property name="initialPositions">
			<list>
				<value>Empty</value>
				<value>Empty</value>
				<value>Empty</value>
				<value>Empty</value>
				<value>10x Pink</value>
				<value>4x Pink</value>
				<value>2x Pink</value>
			</list>
		</property>
	</bean>

</beans>