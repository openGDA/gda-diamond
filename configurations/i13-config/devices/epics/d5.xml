<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xmlns:tx="http://www.springframework.org/schema/tx" xmlns:aop="http://www.springframework.org/schema/aop"
	xmlns:corba="http://www.diamond.ac.uk/schema/gda/corba"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.5.xsd
	http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-2.5.xsd
	http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.5.xsd
	http://www.diamond.ac.uk/schema/gda/corba http://www.diamond.ac.uk/schema/gda/corba/gda-corba-1.0.xsd">

	<description> Diode 5 In Experiment Hutch after experiment hutch shutter</description>
	<bean id="d5_pos" class="gda.device.enumpositioner.EpicsPositioner">
		<property name="deviceName" value="d5.mp" />
	</bean>

	<bean id="d5_y_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="d5.y" />
		<property name="local" value="true" />
	</bean>

	<bean id="d5_i" class="gda.device.scannable.EpicsScannable">
		<property name="useNameAsExtraName" value="true" />
		<property name="hasUnits" value="false" />
		<property name="pvProvider">
			<bean class="gda.epics.interfaceSpec.GDAEpicsInterfacePVProvider">
				<property name="deviceName" value="D5" />
				<property name="fieldName" value="RECORD" />
			</bean>
		</property>
	</bean>

	<bean id="d5_gain" class="gda.device.scannable.EpicsScannable">
		<property name="useNameAsInputName" value="true" />
		<property name="hasUnits" value="false" />
		<property name="pvProvider">
			<bean class="gda.epics.interfaceSpec.GDAEpicsInterfacePVProvider">
				<property name="deviceName" value="d5.femto" />
				<property name="fieldName" value="SETGAIN" />
			</bean>
		</property>
	</bean>

	<bean id="d5_coupling" class="gda.device.scannable.EpicsScannable">
		<property name="useNameAsInputName" value="true" />
		<property name="hasUnits" value="false" />
		<property name="pvProvider">
			<bean class="gda.epics.interfaceSpec.GDAEpicsInterfacePVProvider">
				<property name="deviceName" value="d5.femto" />
				<property name="fieldName" value="SETACDC" />
			</bean>
		</property>
	</bean>

	<bean id="d5_window_temp" class="gda.device.scannable.EpicsScannable">
		<property name="pvName" value="BL13I-DI-PHDGN-06:T1" />
		<property name="useNameAsExtraName" value="true" />
		<property name="hasUnits" value="false" />
	</bean>

	<bean id="d5_return_temp" class="gda.device.scannable.EpicsScannable">
		<property name="pvName" value="BL13I-DI-PHDGN-06:T2" />
		<property name="useNameAsExtraName" value="true" />
		<property name="hasUnits" value="false" />
	</bean>

	<bean id="d5_cam_pvProvider"
		class="gda.device.detector.areadetector.v17.impl.SimplePVProvider">
		<property name="prefix" value="BL13I-DI-PHDGN-06:CAM:" />
	</bean>
	<bean id="d5_cam_base" class="gda.device.detector.areadetector.v17.impl.ADBaseImpl">
		<property name="pvProvider" ref="d5_cam_pvProvider" />
	</bean>

	<bean id="d5_arr_pvProvider"
		class="gda.device.detector.areadetector.v17.impl.SimplePVProvider">
		<property name="prefix" value="BL13I-DI-PHDGN-06:ARR:" />
	</bean>

	<bean id="d5_arr_ndpluginbase"
		class="gda.device.detector.areadetector.v17.impl.NDPluginBaseImpl">
		<property name="pvProvider" ref="d5_arr_pvProvider" />
	</bean>

	<bean id="d5_arr" class="gda.device.detector.areadetector.v17.impl.NDArrayImpl">
		<property name="pvProvider" ref="d5_arr_pvProvider" />
		<property name="pluginBase" ref="d5_arr_ndpluginbase" />
	</bean>


	<bean id="d5_stat_pvProvider"
		class="gda.device.detector.areadetector.v17.impl.SimplePVProvider">
		<property name="prefix" value="BL13I-DI-PHDGN-06:STAT:" />
	</bean>

	<bean id="d5_stat_ndpluginbase"
		class="gda.device.detector.areadetector.v17.impl.NDPluginBaseImpl">
		<property name="pvProvider" ref="d5_stat_pvProvider" />
	</bean>

	<bean id="d5_stat" class="gda.device.detector.areadetector.v17.impl.NDStatsImpl">
		<property name="pvProvider" ref="d5_stat_pvProvider" />
		<property name="pluginBase" ref="d5_stat_ndpluginbase" />
	</bean>

	<bean id="d5_roi_pvProvider"
		class="gda.device.detector.areadetector.v17.impl.SimplePVProvider">
		<property name="prefix" value="BL13I-DI-PHDGN-06:ROI:" />
	</bean>

	<bean id="d5_roi_ndpluginbase"
		class="gda.device.detector.areadetector.v17.impl.NDPluginBaseImpl">
		<property name="pvProvider" ref="d5_roi_pvProvider" />
</bean>

	<bean id="d5_roi" class="gda.device.detector.areadetector.v17.impl.NDROIImpl">
		<property name="pvProvider" ref="d5_roi_pvProvider" />
		<property name="pluginBase" ref="d5_roi_ndpluginbase" />
	</bean>

	<bean id="d5_ffmpeg_pvProvider"
		class="gda.device.detector.areadetector.v17.impl.SimplePVProvider">
		<property name="prefix" value="BL13I-DI-PHDGN-06:FFMPEG:" />
	</bean>

	<bean id="d5_ffmpeg_ndpluginbase"
		class="gda.device.detector.areadetector.v17.impl.NDPluginBaseImpl">
		<property name="pvProvider" ref="d5_ffmpeg_pvProvider" />
	</bean>

	<bean id="d5_ffmpeg"
		class="gda.device.detector.areadetector.v17.impl.FfmpegStreamImpl">
		<property name="pvProvider" ref="d5_ffmpeg_pvProvider" />
		<property name="pluginBase" ref="d5_ffmpeg_ndpluginbase" />
	</bean>


	<bean id="d5_cam_ad_plugins" class="gda.device.FindableObjectHolder">
		<property name="map">
			<util:map>
				<entry key="d5_cam_base" value-ref="d5_cam_base" />
				<entry key="d5_arr" value-ref="d5_arr" />
				<entry key="d5_ffmpeg" value-ref="d5_ffmpeg" />
				<entry key="d5_roi" value-ref="d5_roi" />
				<entry key="d5_ndhdf5file" value-ref="d5_ndhdf5file"/>
				<entry key="d5_ndfiletif" value-ref="d5_ndfiletif"/>
			</util:map>
		</property>
	</bean>

	<bean id="d5_cam_ad" class="gda.device.detector.addetector.ADDetector">
		<property name="adBase" ref="d5_cam_base" />
		<property name="ndStats" ref="d5_stat" />
		<property name="ndArray" ref="d5_arr" />
		<property name="collectionStrategy">
			<bean class="gda.device.detector.addetector.triggering.SingleExposureStandard">
				<constructor-arg ref="d5_cam_base" />
				<constructor-arg value=".0" />
			</bean>
		</property>
		<property name="ndFile" ref="d5_ndfiletif" />
		<property name="fileWriter">
			<bean class="gda.device.detector.addetector.filewriter.SingleImagePerFileWriter">
				<constructor-arg ref="d5_ndfiletif" />
				<constructor-arg value="d5"/>
				<constructor-arg value="%%s%d%%s-%%d-d5.tif"/>
				<constructor-arg value="true"/>
				<constructor-arg value="true"/>
				<property name="enable" value="true"/>
			</bean>
		</property>
		<property name="computeStats" value="false"/>
		<property name="computeCentroid" value="false"/>
		<property name="readArray" value="true"/>
		<property name="readFilepath" value="false" />
	</bean>

	<bean id="d5_ndfilehdf5_pvProvider"
		class="gda.device.detector.areadetector.v17.impl.SimplePVProvider">
		<property name="prefix" value="BL13I-DI-PHDGN-06:HDF:" />
	</bean>

	<bean id="d5_ndfilehdf5_base" class="gda.device.detector.areadetector.v17.impl.NDFileImpl">
		<property name="pvProvider" ref="d5_ndfilehdf5_pvProvider" />
		<property name="pluginBase">
			<bean class="gda.device.detector.areadetector.v17.impl.NDPluginBaseImpl">
				<property name="pvProvider" ref="d5_ndfilehdf5_pvProvider" />
			</bean>
		</property>
		<property name="resetToInitialValues" value="false"/>
	</bean>
	<bean id="d5_ndhdf5file" class="gda.device.detector.areadetector.v17.impl.NDFileHDF5Impl">
		<property name="pvProvider" ref="d5_ndfilehdf5_pvProvider" />
		<property name="file" ref="d5_ndfilehdf5_base"/>
	</bean>

	<bean id="d5_ndfiletif_pvProvider"
		class="gda.device.detector.areadetector.v17.impl.SimplePVProvider">
		<property name="prefix" value="BL13I-DI-PHDGN-06:TIFF:" />
	</bean>

	<bean id="d5_ndfiletif" class="gda.device.detector.areadetector.v17.impl.NDFileImpl">
		<property name="pvProvider" ref="d5_ndfiletif_pvProvider" />
		<property name="pluginBase">
			<bean class="gda.device.detector.areadetector.v17.impl.NDPluginBaseImpl">
				<property name="pvProvider" ref="d5_ndfiletif_pvProvider" />
			</bean>
		</property>
		<property name="resetToInitialValues" value="false"/>
	</bean>

</beans>
	
