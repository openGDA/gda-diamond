<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="cs1_x_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="stage2.x" />
		<property name="local" value="true" />
	</bean>

	<bean id="cs1_y_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="stage2.y" />
		<property name="local" value="true" />
	</bean>

	<bean id="cs1_z_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="stage2.z" />
		<property name="local" value="true" />
	</bean>

</beans>	