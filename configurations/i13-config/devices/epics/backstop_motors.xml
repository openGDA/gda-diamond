<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="bs1_x_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13I-RS-ABSB-05:X"/>
		<property name="local" value="true" />
	</bean>

	<bean id="bs1_y_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13I-RS-ABSB-05:Y"/>
		<property name="local" value="true" />
	</bean>

	<bean id="bs1_z_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13I-RS-ABSB-05:Z"/>
		<property name="local" value="true" />
	</bean>

</beans>	