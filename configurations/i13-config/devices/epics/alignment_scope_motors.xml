<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="alignment_x_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="scope1.x" />
		<property name="local" value="true" />
		<property name="unitStringOverride" value="kcts"/>		
	</bean>
	<bean id="alignment_y_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="scope1.y" />
		<property name="local" value="true" />
		<property name="unitStringOverride" value="kcts"/>
	</bean>
	<bean id="alignment_z_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="scope1.z" />
		<property name="local" value="true" />
		<property name="unitStringOverride" value="kcts"/>
	</bean>
	
</beans>	