<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">


	<bean id="pco_model_rbv" class="gda.device.scannable.SimpleScannable">
		<property name="name" value="pco_model_rbv"/>
		<property name="currentPosition" value="PCO.Camera Edge" />
	</bean>
	
	<bean id="pco_binx_rbv" class="gda.device.scannable.SimpleScannable">
		<property name="name" value="pco_binx_rbv"/>
		<property name="currentPosition">
			<bean class="java.lang.Integer">
				<constructor-arg value="1" />
			</bean>
		</property>
	</bean>
	
	<bean id="pco_biny_rbv" class="gda.device.scannable.SimpleScannable">
		<property name="name" value="pco_biny_rbv"/>
		<property name="currentPosition">
			<bean class="java.lang.Integer">
				<constructor-arg value="1" />
			</bean>
		</property>
	</bean>

</beans>
