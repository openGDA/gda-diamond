<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="pco_model_rbv" class="gda.device.scannable.SimpleScannable">
		<property name="currentPosition" value="pco.edge" />
	</bean>

	<bean id="pco_binx_rbv" class="gda.device.scannable.SimpleScannable">
		<property name="currentPosition">
			<bean class="java.lang.Integer">
				<constructor-arg value="1" />
			</bean>
		</property>
	</bean>
	
	<bean id="pco_biny_rbv" class="gda.device.scannable.SimpleScannable">
		<property name="currentPosition">
			<bean class="java.lang.Integer">
				<constructor-arg value="1" />
			</bean>
		</property>
	</bean>

</beans>
