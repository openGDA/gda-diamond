<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="lowprof_samplex_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-1012.5" />
		<property name="maxPosition" value="987.5" />
		<property name="position" value="0.5" />
		<property name="unitString" value="mm" />
	</bean>

	<bean id="lowprof_samplez_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-987.5" />
		<property name="maxPosition" value="1012.5" />
		<property name="position" value="0.6" />
		<property name="unitString" value="mm" />
	</bean>

	<bean id="lowprof_rot_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-367.0" />
		<property name="maxPosition" value="365.0" />
		<property name="position" value="326.5" />
		<property name="unitString" value="deg" />
	</bean>

</beans>	
