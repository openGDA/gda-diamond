<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="condenser_x_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13I-OP-COND-01:X"/>
	</bean>
	<bean id="condenser_y_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13I-OP-COND-01:Y"/>
	</bean>
	<bean id="condenser_z_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13I-OP-COND-01:Z"/>
	</bean>

</beans>	