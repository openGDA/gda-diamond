<?xml version='1.0' encoding='UTF-8'?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="cs1_x_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13I-MO-STAGE-02:X" />
		<property name="local" value="true" />
	</bean>

	<bean id="cs1_y_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13I-MO-STAGE-02:Y" />
		<property name="local" value="true" />
	</bean>

	<bean id="cs1_z_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13I-MO-STAGE-02:Z" />
		<property name="local" value="true" />
	</bean>

</beans>