<?xml version='1.0' encoding='UTF-8'?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="s5_xsize_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13I-AL-SLITS-05:X:SIZE" />
	</bean>

	<bean id="s5_xcenter_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13I-AL-SLITS-05:X:CENTER" />
	</bean>

	<bean id="s5_ysize_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13I-AL-SLITS-05:Y:SIZE" />
	</bean>

	<bean id="s5_ycenter_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13I-AL-SLITS-05:Y:CENTER" />
	</bean>

	<bean id="s5_xplus_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13I-AL-SLITS-05:X:PLUS" />
	</bean>

	<bean id="s5_xminus_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13I-AL-SLITS-05:X:MINUS" />
	</bean>

	<bean id="s5_yplus_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13I-AL-SLITS-05:Y:PLUS" />
	</bean>

	<bean id="s5_yminus_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL13I-AL-SLITS-05:Y:MINUS" />
	</bean>

</beans>