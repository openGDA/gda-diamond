<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="ab1_t1" class="gda.device.scannable.EpicsScannable">
		<property name="pvName" value="BL13I-RS-ABSB-01:T1" />
		<property name="useNameAsInputName" value="true"/>
		<!-- unit C is not supported -->
		<property name="hasUnits" value="false" />
	</bean>
	<bean id="ab1_t2" class="gda.device.scannable.EpicsScannable">
		<property name="pvName" value="BL13I-RS-ABSB-01:T2" />
		<property name="useNameAsInputName" value="true"/>
		<!-- unit C is not supported -->
		<property name="hasUnits" value="false" />
	</bean>
	<bean id="ab1_t3" class="gda.device.scannable.EpicsScannable">
		<property name="pvName" value="BL13I-RS-ABSB-01:T3" />
		<property name="useNameAsInputName" value="true"/>
		<!-- unit C is not supported -->
		<property name="hasUnits" value="false" />
	</bean>
	<bean id="ab1_t4" class="gda.device.scannable.EpicsScannable">
		<property name="pvName" value="BL13I-RS-ABSB-01:T4" />
		<property name="useNameAsInputName" value="true"/>
		<!-- unit C is not supported -->
		<property name="hasUnits" value="false" />
	</bean>

</beans>	