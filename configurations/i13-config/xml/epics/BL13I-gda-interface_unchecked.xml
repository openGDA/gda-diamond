<?xml version="1.0" ?>
<devices xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://serv0002.cs.diamond.ac.uk/repos/controls/diamond/release/support/gda/2011-6/gdaApp/src/genericBeamlineSchema.xsd">
	<simpleMbbinary desc="Front End Shutter 1" name="FE.SHTR1">
		<RECORD desc="Status" pv="FE13I-PS-SHTR-01:STA" ro="true" type="mbbinary" />
	</simpleMbbinary>
	<simpleMbbinary desc="Front End Shutter 2" name="FE.SHTR2">
		<RECORD desc="Status" pv="FE13I-PS-SHTR-02:STA" ro="true" type="mbbinary" />
	</simpleMbbinary>

	<simpleMotor desc="Front end slits x size" name="fes1.xsize">
		<RECORD desc="Motor" pv="FE13I-AL-SLITS-01:X:SIZE" ro="false"
			type="motor" />
	</simpleMotor>
	<simpleMotor desc="Front end slits x center" name="fes1.xcenter">
		<RECORD desc="Motor" pv="FE13I-AL-SLITS-01:X:CENTER" ro="false"
			type="motor" />
	</simpleMotor>
	<simpleMotor desc="Front end slits y size" name="fes1.ysize">
		<RECORD desc="Motor" pv="FE13I-AL-SLITS-01:Y:SIZE" ro="false"
			type="motor" />
	</simpleMotor>
	<simpleMotor desc="Front end slits y center" name="fes1.ycenter">
		<RECORD desc="Motor" pv="FE13I-AL-SLITS-01:Y:CENTER" ro="false"
			type="motor" />
	</simpleMotor>

	<simpleMbbinary desc="Fast Mask J Branch" name="fastmask.shtr">
		<RECORD desc="Status" pv="BL13I-VA-FMASK-02:STA" ro="true"
			type="mbbinary" />
	</simpleMbbinary>
	
    <simpleMotor desc="Undulator Gap" name="Undulator.Gap">
        <RECORD desc="Motor" pv="SR13I-MO-SERVC-01:BLGAPMTR" ro="false" type="motor"/>
    </simpleMotor>
    	
	<pneumaticCallback desc="Experiment Hutch Shutter" name="eh_shtr">
		<CONTROL desc="CONTROL" pv="BL13I-PS-SHTR-02:CON" ro="false" type="mbbinary"/>
		<STA desc="STA" pv="BL13I-PS-SHTR-02:STA" ro="true" type="mbbinary"/>
	</pneumaticCallback>

	<pneumaticCallback desc="Optics Hutch 4 Shutter" name="oh4_shtr">
		<CONTROL desc="CONTROL" pv="BL13I-PS-SHTR-01:CON" ro="false" type="mbbinary"/>
		<STA desc="STA" pv="BL13I-PS-SHTR-01:STA" ro="true" type="mbbinary"/>
	</pneumaticCallback>


</devices>