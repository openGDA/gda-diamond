<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="BPM1" class="gda.device.scannable.scannablegroup.ScannableGroup">
		<property name="groupMembers">
			<list>
				<ref bean="bpm1x" />
				<ref bean="bpm1y" />
				<ref bean="bpm1pow" />
				<ref bean="bpm2x" />
				<ref bean="bpm2y" />
				<ref bean="bpm2pow" />
				<ref bean="bpm3x" />
				<ref bean="bpm3y" />
				<ref bean="bpm3pow" />
			</list>
		</property>
		<property name="protectionLevel" value="0" />
		<property name="local" value="true"/>
	</bean>

	<!-- Scannable motors that control the Laser Stabilisation System -->
	<bean id="fsm1x" class="gda.device.scannable.ScannableMotor">
		<property name="motor" ref="MotorX_LFSM1" />
		<property name="protectionLevel" value="0" />
		<property name="initialUserUnits" value="V" />
		<property name="hardwareUnitString" value="V" />
	</bean>
	
	<bean id="fsm1y" class="gda.device.scannable.ScannableMotor">
		<property name="motor" ref="MotorY_LFSM1" />
		<property name="protectionLevel" value="0" />
		<property name="initialUserUnits" value="V" />
		<property name="hardwareUnitString" value="V" />
	</bean>
	
	<!-- 
	<bean id="fsm2x" class="gda.device.scannable.ScannableMotor">
		<property name="motor" ref="MotorX_LFSM2" />
		<property name="protectionLevel" value="0" />
		<property name="initialUserUnits" value="V" />
		<property name="hardwareUnitString" value="V" />
	</bean>
	
	<bean id="fsm2y" class="gda.device.scannable.ScannableMotor">
		<property name="motor" ref="MotorY_LFSM2" />
		<property name="protectionLevel" value="0" />
		<property name="initialUserUnits" value="V" />
		<property name="hardwareUnitString" value="V" />
	</bean>
	 -->
	
	<bean id="fsm3x" class="gda.device.scannable.ScannableMotor">
		<property name="motor" ref="MotorX_LFSM3" />
		<property name="protectionLevel" value="0" />
		<property name="initialUserUnits" value="V" />
		<property name="hardwareUnitString" value="V" />
	</bean>
	
	<bean id="fsm3y" class="gda.device.scannable.ScannableMotor">
		<property name="motor" ref="MotorY_LFSM3" />
		<property name="protectionLevel" value="0" />
		<property name="initialUserUnits" value="V" />
		<property name="hardwareUnitString" value="V" />
	</bean>
	
	<bean id="fsm4x" class="gda.device.scannable.ScannableMotor">
		<property name="motor" ref="MotorX_LFSM4" />
		<property name="protectionLevel" value="0" />
		<property name="initialUserUnits" value="V" />
		<property name="hardwareUnitString" value="V" />
	</bean>
	
	<bean id="fsm4y" class="gda.device.scannable.ScannableMotor">
		<property name="motor" ref="MotorY_LFSM4" />
		<property name="protectionLevel" value="0" />
		<property name="initialUserUnits" value="V" />
		<property name="hardwareUnitString" value="V" />
	</bean>
	
	<bean id="fsm22x" class="gda.device.scannable.ScannableMotor">
		<property name="motor" ref="MotorX_LFSM22" />
		<property name="protectionLevel" value="0" />
		<property name="initialUserUnits" value="V" />
		<property name="hardwareUnitString" value="V" />
	</bean>
	
	<bean id="fsm22y" class="gda.device.scannable.ScannableMotor">
		<property name="motor" ref="MotorY_LFSM22" />
		<property name="protectionLevel" value="0" />
		<property name="initialUserUnits" value="V" />
		<property name="hardwareUnitString" value="V" />
	</bean>

	<bean id="fsm23x" class="gda.device.scannable.ScannableMotor">
		<property name="motor" ref="MotorX_LFSM23" />
		<property name="protectionLevel" value="0" />
		<property name="initialUserUnits" value="V" />
		<property name="hardwareUnitString" value="V" />
	</bean>
	
	<bean id="fsm23y" class="gda.device.scannable.ScannableMotor">
		<property name="motor" ref="MotorY_LFSM23" />
		<property name="protectionLevel" value="0" />
		<property name="initialUserUnits" value="V" />
		<property name="hardwareUnitString" value="V" />
	</bean>


	<bean id="FSM" class="gda.device.scannable.scannablegroup.ScannableGroup">
		<property name="groupMembers">
			<list>
				<ref bean="fsm1x" />
				<ref bean="fsm1y" />
				<!-- 
				<ref bean="fsm2x" />
				<ref bean="fsm2y" />
				 -->
				<ref bean="fsm3x" />
				<ref bean="fsm3y" />
				<ref bean="fsm4x" />
				<ref bean="fsm4y" />
				<ref bean="fsm22x" />
				<ref bean="fsm22y" />
				<ref bean="fsm23x" />
				<ref bean="fsm23y" />

				<ref bean="fsm1xfsp" />
				<ref bean="fsm1yfsp" />
				<!-- 
				<ref bean="fsm2xfsp" />
				<ref bean="fsm2yfsp" />
				 -->
				<ref bean="fsm3xfsp" />
				<ref bean="fsm3yfsp" />
				<ref bean="fsm4xfsp" />
				<ref bean="fsm4yfsp" />
				<ref bean="fsm22xfsp" />
				<ref bean="fsm22yfsp" />
				<ref bean="fsm23xfsp" />
				<ref bean="fsm23yfsp" />
			</list>
		</property>
		<property name="protectionLevel" value="0" />
		<property name="local" value="true"/>
	</bean>

</beans>
