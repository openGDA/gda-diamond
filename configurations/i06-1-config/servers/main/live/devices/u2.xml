<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- Patch Panel U2 Analog Output ControlPoint EpicsControlPoint -->
	<bean id="dac21" class="gda.device.controlpoint.EpicsControlPoint">
		<property name="deviceName" value="U2.AO1" />
		<property name="local" value="true"/>
	</bean>

	<bean id="dac22" class="gda.device.controlpoint.EpicsControlPoint">
		<property name="deviceName" value="U2.AO2" />
		<property name="local" value="true"/>
	</bean>

	<bean id="dac23" class="gda.device.controlpoint.EpicsControlPoint">
		<property name="deviceName" value="U2.AO3" />
		<property name="local" value="true"/>
	</bean>

	<bean id="dac24" class="gda.device.controlpoint.EpicsControlPoint">
		<property name="deviceName" value="U2.AO4" />
		<property name="local" value="true"/>
	</bean>

	<bean id="dac25" class="gda.device.controlpoint.EpicsControlPoint">
		<property name="deviceName" value="U2.AO5" />
		<property name="local" value="true"/>
	</bean>

	<bean id="dac26" class="gda.device.controlpoint.EpicsControlPoint">
		<property name="deviceName" value="U2.AO6" />
		<property name="local" value="true"/>
	</bean>

	<bean id="dac27" class="gda.device.controlpoint.EpicsControlPoint">
		<property name="deviceName" value="U2.AO7" />
		<property name="local" value="true"/>
	</bean>

	<bean id="dac28" class="gda.device.controlpoint.EpicsControlPoint">
		<property name="deviceName" value="U2.AO8" />
		<property name="local" value="true"/>
	</bean>

	<!-- Patch Panel U2 Analog Input Monitor -->
	<bean id="adc21" class="gda.device.monitor.EpicsMonitor">
		<property name="deviceName" value="U2.AI1" />
		<property name="local" value="true"/>
	</bean>

	<bean id="adc22" class="gda.device.monitor.EpicsMonitor">
		<property name="deviceName" value="U2.AI2" />
		<property name="local" value="true"/>
	</bean>

	<bean id="adc23" class="gda.device.monitor.EpicsMonitor">
		<property name="deviceName" value="U2.AI3" />
		<property name="local" value="true"/>
	</bean>

	<bean id="adc24" class="gda.device.monitor.EpicsMonitor">
		<property name="deviceName" value="U2.AI4" />
		<property name="local" value="true"/>
	</bean>

</beans>
