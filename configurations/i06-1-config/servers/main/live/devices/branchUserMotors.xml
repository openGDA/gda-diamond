<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- Branchline User Motors U2 Motors -->
	<bean id="MotorMTR1_U2" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="U2.MTR1" />
		<property name="local" value="true"/>
	</bean>

	<bean id="MotorMTR2_U2" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="U2.MTR2" />
		<property name="local" value="true"/>
	</bean>

	<bean id="MotorMTR3_U2" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="U2.MTR3" />
		<property name="local" value="true"/>
	</bean>

	<bean id="MotorMTR4_U2" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="U2.MTR4" />
		<property name="local" value="true"/>
	</bean>

	<bean id="MotorMTR5_U2" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="U2.MTR5" />
		<property name="local" value="true"/>
	</bean>

	<bean id="MotorMTR6_U2" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="U2.MTR6" />
		<property name="local" value="true"/>
	</bean>

</beans>
