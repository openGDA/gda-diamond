<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- Branchline Exit Slits S6 Motors -->
	<bean id="MotorXDisp_S6" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S6.XDISP" />
		<property name="local" value="true"/>
	</bean>

	<bean id="MotorYDisp_S6" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S6.YDISP" />
		<property name="local" value="true"/>
	</bean>

	<bean id="MotorZ_S6" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S6.Z" />
		<property name="local" value="true"/>
	</bean>

	<bean id="MotorXSize_S6" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL06J-AL-SLITS-01:X:SIZE" />
		<property name="local" value="true"/>
	</bean>
	<bean id="MotorYSize_S6" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL06J-AL-SLITS-01:Y:SIZE" />
		<property name="local" value="true"/>
	</bean>
</beans>
