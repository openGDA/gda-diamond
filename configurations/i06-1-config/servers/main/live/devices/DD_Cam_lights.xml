<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="cl1_switch" class="gda.device.enumpositioner.EpicsSimpleBinary">
		<property name="pvName" value="BL06J-EA-DDIFF-01:LED1:TOGGLE"/>
	</bean>
	<bean id="cl2_switch" class="gda.device.enumpositioner.EpicsSimpleBinary">
		<property name="pvName" value="BL06J-EA-DDIFF-01:LED2:TOGGLE"/>
	</bean>
	<bean id="cl3_switch" class="gda.device.enumpositioner.EpicsSimpleBinary">
		<property name="pvName" value="BL06J-EA-DDIFF-01:LED3:TOGGLE"/>
	</bean>
	<bean id="cl4_switch" class="gda.device.enumpositioner.EpicsSimpleBinary">
		<property name="pvName" value="BL06J-EA-DDIFF-01:LED4:TOGGLE"/>
	</bean>
	<bean id="cl5_switch" class="gda.device.enumpositioner.EpicsSimpleBinary">
		<property name="pvName" value="BL06J-EA-DDIFF-01:LED5:TOGGLE"/>
	</bean>

	<bean id="cl1_intensity" class="gda.device.scannable.PVScannable">
		<property name="pvName" value="BL06J-EA-DDIFF-01:LED1:PWMDEMAND"/>
	</bean>
	<bean id="cl2_intensity" class="gda.device.scannable.PVScannable">
		<property name="pvName" value="BL06J-EA-DDIFF-01:LED2:PWMDEMAND"/>
	</bean>
	<bean id="cl3_intensity" class="gda.device.scannable.PVScannable">
		<property name="pvName" value="BL06J-EA-DDIFF-01:LED3:PWMDEMAND"/>
	</bean>
	<bean id="cl4_intensity" class="gda.device.scannable.PVScannable">
		<property name="pvName" value="BL06J-EA-DDIFF-01:LED4:PWMDEMAND"/>
	</bean>
	<bean id="cl5_intensity" class="gda.device.scannable.PVScannable">
		<property name="pvName" value="BL06J-EA-DDIFF-01:LED5:PWMDEMAND"/>
	</bean>
	
</beans>
