		<property name="speed" value="1.0" />
		<property name="unitString" value="mm"/>
		<property name="local" value="true" />
	</bean>

	<bean id="MotorZ_S6" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-200.000000" />
		<property name="maxPosition" value="190.000000" />
		<property name="speed" value="1.0" />
		<property name="unitString" value="mm"/>
		<property name="local" value="true" />
	</bean>

	<bean id="MotorXSize_S6" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-0.0810" />
