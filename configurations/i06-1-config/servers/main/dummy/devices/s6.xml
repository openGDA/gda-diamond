	<bean id="MotorYSize_S6" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-0.0308" />
		<property name="maxPosition" value="3.9702" />
		<property name="speed" value="1.0" />
		<property name="unitString" value="mm"/>
		<property name="local" value="true" />
	</bean>
</beans>
