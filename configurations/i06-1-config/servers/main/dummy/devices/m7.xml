	<!-- Branchline Focusing Mirrow M7 Motors -->
	<bean id="MotorX_M7" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-10" />
		<property name="maxPosition" value="10" />
		<property name="speed" value="1" />
		<property name="unitString" value="mm"/>
	</bean>

	<bean id="MotorY_M7" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-25" />
		<property name="maxPosition" value="25" />
