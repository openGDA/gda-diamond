<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="scmx_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-10" />
		<property name="maxPosition" value="10" />
		<property name="speed" value="1" />
		<property name="unitString" value="mm"/>
	</bean>
	<bean id="scmy_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-10" />
		<property name="maxPosition" value="10" />
		<property name="speed" value="1" />
		<property name="unitString" value="mm"/>
	</bean>

	<bean id="scmth_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-10" />
		<property name="maxPosition" value="10" />
		<property name="speed" value="1" />
		<property name="unitString" value="deg"/>
	</bean>
</beans>
