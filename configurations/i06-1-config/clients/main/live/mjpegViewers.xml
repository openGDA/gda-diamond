<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd">


<!-- setup CameraView -->
       <bean id="d3_cam_config" class="uk.ac.gda.video.views.EpicsCameraParameters">
              <property name="name" value="D3 Cam1 Preview" />
              <property name="exposurePV" value="BL06I-DI-PHDGN-99:CAM:AcquireTime" />
              <property name="acqPeriodPV" value="BL06I-DI-PHDGN-99:CAM:AcquirePeriod" />
              <property name="gainPV" value="BL06I-DI-PHDGN-99:CAM:Gain" />
              <property name="acquirePV" value="BL06I-DI-PHDGN-99:CAM:Acquire" />
              <property name="urlPV" value="BL06I-DI-PHDGN-99:MJPG:MJPG_URL_RBV" />
       </bean>
       
       <bean id="d4_cam_config" class="uk.ac.gda.video.views.EpicsCameraParameters">
              <property name="name" value="D4 Cam1 Preview" />
              <property name="exposurePV" value="BL06I-DI-PHDGN-02:CAM:AcquireTime" />
              <property name="acqPeriodPV" value="BL06I-DI-PHDGN-02:CAM:AcquirePeriod" />
              <property name="gainPV" value="BL06I-DI-PHDGN-02:CAM:Gain" />
