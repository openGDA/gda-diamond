<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:gda="http://www.diamond.ac.uk/schema/gda/gda"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	           http://www.diamond.ac.uk/schema/gda/gda
           http://www.diamond.ac.uk/schema/gda/gda/gda-gda-1.2.xsd">


	<bean id="debenSpeed" class="gda.device.enumpositioner.DummyEnumPositioner">
		<property name="positions">
			<list>
				<value>0</value>
				<value>1</value>
			</list>
		</property>
	</bean>

	<bean id="debenRunning" class="gda.device.enumpositioner.DummyEnumPositioner">
		<property name="positions">
			<list>
				<value>0</value>
				<value>1</value>
			</list>
		</property>
	</bean>

	<bean id="debenForce" class="gda.device.scannable.DummyScannable">
		<property name="inputNames" value="debenForce"/>
	</bean>

	<bean id="debenExt" class="gda.device.scannable.DummyScannable">
		<property name="inputNames" value="debenExt"/>
	</bean>

	<bean id="debenApart" class="gda.device.scannable.DummyScannable">
		<property name="name" value="debenApart" />
	</bean>

	<bean id="debenTogether" class="gda.device.scannable.DummyScannable">
		<property name="name" value="debenTogether" />
	</bean>

	<bean id="debenStop" class="gda.device.scannable.DummyScannable">
		<property name="name" value="debenStop" />
	</bean>

	<bean id="debenConnect" class="gda.device.scannable.DummyScannable">
		<property name="name" value="debenConnect" />
	</bean>

	<bean id="debenDisconnect" class="gda.device.scannable.DummyScannable">
		<property name="name" value="debenDisconnect" />
	</bean>

</beans>
