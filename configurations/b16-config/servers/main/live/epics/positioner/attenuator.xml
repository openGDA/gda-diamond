<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	
	<bean id="att1us" class ="gda.device.enumpositioner.EpicsPositioner">
		<property name="deviceName" value="A1.USPOSN"/>
		<property name="local" value="true"/>
	</bean>
	<bean id="att1ds" class ="gda.device.enumpositioner.EpicsPositioner">
		<property name="deviceName" value="A1.DSPOSN"/>
		<property name="local" value="true"/>
	</bean>
	<bean id="att2" class ="gda.device.enumpositioner.EpicsPositioner">
		<property name="deviceName" value="A2.POSN"/>
		<property name="local" value="true"/>
	</bean>

</beans>