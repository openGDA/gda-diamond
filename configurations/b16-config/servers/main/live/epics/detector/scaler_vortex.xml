<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="struck1" class="gda.device.detector.EpicsScaler">
		<property name="pvName" value="BL16B-EA-DET-01:SCALER1" />
		<property name="frequency" value="50000000" />
		<property name="local" value="true" />
	</bean>
	
	<bean id="struck2" class="gda.device.detector.EpicsScaler">
		<property name="pvName" value="BL16B-EA-DET-01:SCALER2" />
		<property name="frequency" value="50000000" />
		<property name="local" value="true" />
	</bean>
	
</beans>