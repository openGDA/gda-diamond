           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">


	<bean id="pcoedgedet" class="gda.device.detector.addetector.ADPco">

		<property name="readAcquisitionTime" value="true" />
		
		<property name="readFilepath" value="true" />
		
		<property name="readAcquisitionPeriod" value="false" />
		
		<property name="computeStats" value="false" />
		
		<property name="computeCentroid" value="false" />
		
		<property name="readArray" value="false" />

		<property name="createsOwnFiles" value="true" />
		
		<property name="forceArrayCallbacks" value="true" />
		
		<property name="readoutTime" value="-1" />
		
		<property name="armModePvName" value="BL16B-EA-DET-03:CAM:ARM_MODE" />

		<!-- ADBase desc="pco.cam ADBase class instance" name="pco.cam_ADBase"-->
		<property name="adBase">
			<bean class="gda.device.detector.areadetector.v17.impl.ADBaseImpl">
				<property name="deviceName" value="pco.cam_ADBase" />
			</bean>
		</property>
		
<!--	<pcocam desc="pco.cam pcocam class instance" name="pco.cam_pcocam">-->

<!--	<NDFileTIFF desc="pco.tif NDFileTIFF class instance" name="pco.tif_NDFileTIFF"/>-->
<!--	<NDFile desc="pco.tif NDFile class instance" name="pco.tif_NDFile">-->
<!--	<NDPluginBase desc="pco.tif NDPluginBase class instance" name="pco.tif_NDPluginBase">-->
		<property name="ndFile">
			<bean class="gda.device.detector.areadetector.v17.impl.NDFileImpl">
				<property name="deviceName" value="pco.tif_NDFile" />
				<property name="initialAutoSave" value="Not hooked up to anything!" /> <!-- currently yes -->
				<property name="initialAutoIncrement" value="Not hooked up to anything!" /> <!-- currently true -->
<!--				<property name="initialWriteMode" value="0" />  Single(current), Capture, Stream, -->
				<property name="initialNumCapture" value="1" /> <!-- current = 1 -->
				<property name="initialFileName" value="pcoedge" />
				<property name="initialFileTemplate" value="%s%s%05d.tif" />
				<property name="pluginBase">
					<bean class="gda.device.detector.areadetector.v17.impl.NDPluginBaseImpl">
						<property name="deviceName" value="pco.tif_NDPluginBase" />
					</bean>
				</property>
			</bean>
		</property>
		
		<!-- property name="ndArray">
			<bean class="gda.device.detector.areadetector.v17.impl.NDArrayImpl">
				<property name="deviceName" value="???.Image1_NDStdArrays" />
				<property name="pluginBase">
					<bean class="gda.device.detector.areadetector.v17.impl.NDPluginBaseImpl">
						<property name="deviceName" value="???.Image1_NDPluginBase" />
					</bean>
				</property>
			</bean>
		</property-->
		
<!--	<NDStats desc="pco.stat NDStats class instance" name="pco.stat_NDStats">-->
<!--	<NDPluginBase desc="pco.stat NDPluginBase class instance" name="pco.stat_NDPluginBase">-->
		<property name="ndStats">
			<bean class="gda.device.detector.areadetector.v17.impl.NDStatsImpl">
				<property name="deviceName" value="pco.stat_NDStats" />
				<property name="pluginBase">
					<bean class="gda.device.detector.areadetector.v17.impl.NDPluginBaseImpl">
						<property name="deviceName" value="pco.stat_NDPluginBase" />
					</bean>
				</property>
			</bean>
		</property>
	</bean>

<!--	<NDROI desc="pco.roi1 NDROI class instance" name="pco.roi1_NDROI">-->
<!--	<NDPluginBase desc="pco.roi1 NDPluginBase class instance" name="pco.roi1_NDPluginBase">-->
	<bean id="pilatus1.ROI1_NDROI" class="gda.device.detector.areadetector.v17.impl.NDROIImpl">
		<property name="deviceName" value="pco.roi1_NDROI" />
		<property name="pluginBase">
			<bean class="gda.device.detector.areadetector.v17.impl.NDPluginBaseImpl">
				<property name="deviceName" value="pco.roi1_NDPluginBase" />
			</bean>
		</property>
		<property name="initialBinX" value="0" />
		<property name="initialBinY" value="0" />
		<property name="initialMinX" value="0" />
		<property name="initialMinY" value="0" />
		<property name="initialSizeX" value="487" />
		<property name="initialSizeY" value="195" />
	</bean>
	
