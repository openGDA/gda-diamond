<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="DR1ARMThetaMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="DR1ARM.THETA" />
		<property name="local" value="true" />
	</bean>
	<bean id="DR1ARMTthetaMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="DR1ARM.TTHETA" />
		<property name="local" value="true" />
	</bean>

</beans>
