<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="S2YPlusMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL16B-OP-SLITS-02:Y:PLUS" />
	</bean>
	<bean id="S2YMinusMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL16B-OP-SLITS-02:Y:MINUS" />
	</bean>
	<bean id="S2XPlusMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL16B-OP-SLITS-02:X:PLUS" />
	</bean>
	<bean id="S2XMinusMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL16B-OP-SLITS-02:X:MINUS" />
	</bean>
	<bean id="S2XCentreMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL16B-OP-SLITS-02:X:CENTER" />
	</bean>
	<bean id="S2XSizeMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL16B-OP-SLITS-02:X:SIZE" />
	</bean>
	<bean id="S2YCentreMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL16B-OP-SLITS-02:Y:CENTER" />
	</bean>
	<bean id="S2YSizeMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL16B-OP-SLITS-02:Y:SIZE" />
	</bean>

</beans>
