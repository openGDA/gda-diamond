<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="S2YPlusMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S2.YPLUS" />
		<property name="local" value="true" />
	</bean>
	<bean id="S2YMinusMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S2.YMINUS" />
		<property name="local" value="true" />
	</bean>
	<bean id="S2XPlusMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S2.XPLUS" />
		<property name="local" value="true" />
	</bean>
	<bean id="S2XMinusMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S2.XMINUS" />
		<property name="local" value="true" />
	</bean>
	<bean id="S2XCentreMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S2.XCENTRE" />
		<property name="local" value="true" />
	</bean>
	<bean id="S2XSizeMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S2.XSIZE" />
		<property name="local" value="true" />
	</bean>
	<bean id="S2YCentreMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S2.YCENTRE" />
		<property name="local" value="true" />
	</bean>
	<bean id="S2YSizeMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S2.YSIZE" />
		<property name="local" value="true" />
	</bean>

</beans>
