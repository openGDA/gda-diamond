<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="S6YPlusMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S6.YPLUS" />
		<property name="local" value="true" />
	</bean>
	<bean id="S6YMinusMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S6.YMINUS" />
		<property name="local" value="true" />
	</bean>
	<bean id="S6XPlusMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S6.XPLUS" />
		<property name="local" value="true" />
	</bean>
	<bean id="S6XMinusMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S6.XMINUS" />
		<property name="local" value="true" />
	</bean>
	<bean id="S6XCentreMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S6.XCENTRE" />
		<property name="local" value="true" />
	</bean>
	<bean id="S6XSizeMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S6.XSIZE" />
		<property name="local" value="true" />
	</bean>
	<bean id="S6YCentreMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S6.YCENTRE" />
		<property name="local" value="true" />
	</bean>
	<bean id="S6YSizeMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S6.YSIZE" />
		<property name="local" value="true" />
	</bean>
</beans>
