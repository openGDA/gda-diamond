<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="S9XCentreMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL16B-OP-SLITS-09:X:CENTER" />
		<property name="local" value="true" />
	</bean>
	<bean id="S9XSizeMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL16B-OP-SLITS-09:X:SIZE" />
		<property name="deviceName" value="S9.XGAP" />
		<property name="local" value="true" />
	</bean>
	<bean id="S9YCentreMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL16B-OP-SLITS-09:Y:CENTER" />
		<property name="local" value="true" />
	</bean>
	<bean id="S9YSizeMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL16B-OP-SLITS-09:Y:SIZE" />
		<property name="deviceName" value="S9.YGAP" />
		<property name="local" value="true" />
	</bean>
	
</beans>
