<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="TOMOHEIGHTMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL16B-MO-TOMO-01:HEIGHT" />
		<property name="local" value="true" />
	</bean>

	<bean id="TOMOROLLMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL16B-MO-TOMO-01:ROLL" />
		<property name="local" value="true" />
	</bean>

	<bean id="TOMOPITCHMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL16B-MO-TOMO-01:PITCH" />
		<property name="local" value="true" />
	</bean>

	<bean id="TOMOTHETAMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL16B-MO-TOMO-01:THETA" />
		<property name="local" value="true" />
	</bean>

</beans>
