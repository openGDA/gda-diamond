<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="M1BendMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="M1.BEND" />
		<property name="local" value="true" />
	</bean>
	<bean id="M1UsyMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="M1.USY" />
		<property name="local" value="true" />
	</bean>
	<bean id="M1UsxMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="M1.USX" />
		<property name="local" value="true" />
	</bean>
	<bean id="M1DsobyMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="M1.DSOBY" />
		<property name="local" value="true" />
	</bean>
	<bean id="M1DsxMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="M1.DSX" />
		<property name="local" value="true" />
	</bean>
	<bean id="M1DsibyMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="M1.DSIBY" />
		<property name="local" value="true" />
	</bean>
	<bean id="M1HeightMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="M1.HEIGHT" />
		<property name="local" value="true" />
	</bean>
	<bean id="M1PitchMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="M1.PITCH" />
		<property name="local" value="true" />
	</bean>
	<bean id="M1RollMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="M1.ROLL" />
		<property name="local" value="true" />
	</bean>

</beans>
