<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="DR1BASEY1Motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL16B-MO-DIFF-01:B:Y1" />
	</bean>
	<bean id="DR1BASEY2Motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL16B-MO-DIFF-01:B:Y2" />
	</bean>
	<bean id="DR1BASEXMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL16B-MO-DIFF-01:B:X" />
	</bean>
	<bean id="DR1BASEPitchMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL16B-MO-DIFF-01:B:PITCH" />
	</bean>
	<bean id="DR1BASEHeightMotor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL16B-MO-DIFF-01:B:HEIGHT" />
	</bean>

</beans>
