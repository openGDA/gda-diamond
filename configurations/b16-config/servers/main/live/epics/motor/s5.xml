<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="S5YPlusMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S5.YPLUS" />
		<property name="local" value="true" />
	</bean>
	<bean id="S5YMinusMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S5.YMINUS" />
		<property name="local" value="true" />
	</bean>
	<bean id="S5XPlusMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S5.XPLUS" />
		<property name="local" value="true" />
	</bean>
	<bean id="S5XMinusMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S5.XMINUS" />
		<property name="local" value="true" />
	</bean>
	<bean id="S5XCentreMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S5.XCENTRE" />
		<property name="local" value="true" />
	</bean>
	<bean id="S5XSizeMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S5.XSIZE" />
		<property name="local" value="true" />
	</bean>
	<bean id="S5YCentreMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S5.YCENTRE" />
		<property name="local" value="true" />
	</bean>
	<bean id="S5YSizeMotor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S5.YSIZE" />
		<property name="local" value="true" />
	</bean>

</beans>
