<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="sm1wfs1" class="gda.device.monitor.EpicsMonitor">
		<property name="pvName" value="BL09J-MO-SM-01:WFS1:STATUS" />
	</bean>

	<bean id="sm1x_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL09J-MO-SM-01:X" />
	</bean>
	<bean id="sm1y_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL09J-MO-SM-01:Y" />
	</bean>
	<bean id="sm1z_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL09J-MO-SM-01:Z" />
	</bean>
	<bean id="sm1pitch_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL09J-MO-SM-01:PITCH" />
	</bean>
	<bean id="sm1roll_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL09J-MO-SM-01:ROLL" />
	</bean>
	<bean id="sm1yaw_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL09J-MO-SM-01:YAW" />
	</bean>
	<bean id="sm1fpitch_motor" class="gda.device.motor.PiezoEpicsMotor">
		<property name="pvName" value="BL09J-MO-SM-01:FPITCH" />
	</bean>
</beans>