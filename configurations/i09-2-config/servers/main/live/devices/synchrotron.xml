<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="facility_mode" class="gda.device.enumpositioner.EpicsSimpleMbbinary">
		<property name="recordName" value="CS-CS-MSTAT-01:MODE"/>
		<property name="readOnly" value="true"/>
	</bean>

	<bean id="fill_mode" class="gda.device.enumpositioner.EpicsSimpleMbbinary">
		<property name="recordName" value="LI-TI-MTGEN-01:MODE"/>
		<property name="readOnly" value="true"/>
	</bean>

</beans>