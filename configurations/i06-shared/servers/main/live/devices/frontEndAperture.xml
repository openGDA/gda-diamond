<?xml version='1.0' encoding='UTF-8'?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- Front End: Custom Aperture Epics Motors -->
	<bean id="MotorX_FrontEndAperture" class="gda.device.motor.EpicsMotor">
		<property name="local" value="true" />
	<property name="pvName" value="FE06I-AL-APTR-02:X" /></bean>

	<bean id="MotorY_FrontEndAperture" class="gda.device.motor.EpicsMotor">
		<property name="local" value="true" />
	<property name="pvName" value="FE06I-AL-APTR-02:Y" /></bean>

</beans>