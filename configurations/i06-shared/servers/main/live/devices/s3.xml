<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- Cleanup Slits S3 Motors -->
	<bean id="MotorXPos_S3" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S3.XPOS" />
		<property name="local" value="true"/>
	</bean>


	<bean id="MotorXNeg_S3" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S3.XNEG" />
		<property name="local" value="true"/>
	</bean>

	<bean id="MotorYPos_S3" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S3.YPOS" />
		<property name="local" value="true"/>
	</bean>

	<bean id="MotorYNeg_S3" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S3.YNEG" />
		<property name="local" value="true"/>
	</bean>

	<bean id="MotorXSize_S3" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S3.XSIZE" />
		<property name="local" value="true"/>
	</bean>

	<bean id="MotorXCentre_S3" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S3.XCENTRE" />
		<property name="local" value="true"/>
	</bean>

	<bean id="MotorYSize_S3" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S3.YSIZE" />
		<property name="local" value="true"/>
	</bean>

	<bean id="MotorYCentre_S3" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S3.YCENTRE" />
		<property name="local" value="true"/>
	</bean>

</beans>
