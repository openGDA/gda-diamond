<?xml version='1.0' encoding='UTF-8'?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- Cleanup Slits S3 Motors -->
	<bean id="MotorXPos_S3" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL06I-AL-SLITS-03:X:POS" />
	</bean>

	<bean id="MotorXNeg_S3" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL06I-AL-SLITS-03:X:NEG" />
	</bean>

	<bean id="MotorYPos_S3" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL06I-AL-SLITS-03:Y:POS" />
	</bean>

	<bean id="MotorYNeg_S3" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL06I-AL-SLITS-03:Y:NEG" />
	</bean>

	<bean id="MotorXSize_S3" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL06I-AL-SLITS-03:X:SIZE" />
	</bean>

	<bean id="MotorXCentre_S3" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL06I-AL-SLITS-03:X:CENTER" />
	</bean>

	<bean id="MotorYSize_S3" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL06I-AL-SLITS-03:Y:SIZE" />
	</bean>

	<bean id="MotorYCentre_S3" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL06I-AL-SLITS-03:Y:CENTER" />
	</bean>

</beans>