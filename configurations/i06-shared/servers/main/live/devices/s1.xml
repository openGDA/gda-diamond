<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- Primary Slits S1 Motors -->
	<bean id="MotorXPos_S1" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S1.XPOS" />
		<property name="local" value="true"/>
	</bean>

	<bean id="MotorXNeg_S1" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S1.XNEG" />
		<property name="local" value="true"/>
	</bean>

	<bean id="MotorYPos_S1" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S1.YPOS" />
		<property name="local" value="true"/>
	</bean>

	<bean id="MotorYNeg_S1" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S1.YNEG" />
		<property name="local" value="true"/>
	</bean>

	<bean id="MotorXSize_S1" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S1.XSIZE" />
		<property name="local" value="true"/>
	</bean>

	<bean id="MotorXCentre_S1" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S1.XCENTRE" />
		<property name="local" value="true"/>
	</bean>

	<bean id="MotorYSize_S1" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S1.YSIZE" />
		<property name="local" value="true"/>
	</bean>

	<bean id="MotorYCentre_S1" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="S1.YCENTRE" />
		<property name="local" value="true"/>
	</bean>

</beans>
