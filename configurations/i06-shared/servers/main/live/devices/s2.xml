<?xml version='1.0' encoding='UTF-8'?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- Secondary Slits S2 Motors -->
	<bean id="MotorXPos_S2" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL06I-AL-SLITS-02:X:POS" />
	</bean>

	<bean id="MotorXNeg_S2" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL06I-AL-SLITS-02:X:NEG" />
	</bean>

	<bean id="MotorYPos_S2" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL06I-AL-SLITS-02:Y:POS" />
	</bean>

	<bean id="MotorYNeg_S2" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL06I-AL-SLITS-02:Y:NEG" />
	</bean>

	<bean id="MotorXSize_S2" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL06I-AL-SLITS-02:X:SIZE" />
	</bean>

	<bean id="MotorXCentre_S2" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL06I-AL-SLITS-02:X:CENTER" />
	</bean>

	<bean id="MotorYSize_S2" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL06I-AL-SLITS-02:Y:SIZE" />
	</bean>

	<bean id="MotorYCentre_S2" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL06I-AL-SLITS-02:Y:CENTER" />
	</bean>

</beans>