	<!-- Switching Mirrow M6 Motors -->
	<bean id="MotorX_M6" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-10" />
		<property name="maxPosition" value="10" />
		<property name="speed" value="1" />
		<property name="unitString" value="mm"/>
	</bean>

	<bean id="MotorY_M6" class="gda.device.motor.DummyMotor">
