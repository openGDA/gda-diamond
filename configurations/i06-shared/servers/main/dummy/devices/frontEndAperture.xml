<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- Front End: Custom Aperture Epics Motors -->
	<bean id="MotorX_FrontEndAperture" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-10" />
		<property name="maxPosition" value="10" />
		<property name="speed" value="1" />
	</bean>

	<bean id="MotorY_FrontEndAperture" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-10" />
		<property name="maxPosition" value="10" />
		<property name="speed" value="1" />
	</bean>
</beans>
