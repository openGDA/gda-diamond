				<value>d10b_x_positioner</value>
				<!-- d21b -->
				<value>d21b_y</value>
				<value>d21b_y_positioner</value>
				<!-- sm21b -->
				<value>sm21b_x</value>
				<value>sm21b_z</value>
				<value>sm21b_y</value>
				<value>sm21b_roty</value>
				<value>sm21b_y_positioner</value>
				<!-- sm52b -->
				<value>sm52b_x</value>
				<value>sm52b_y</value>
				<value>sm52b_z</value>
				<value>sm52b_roty</value>
