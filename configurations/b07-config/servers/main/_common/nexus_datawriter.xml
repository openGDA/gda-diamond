				<value>expt_shutter</value>
				<value>pss_shutter1_con</value>
				<value>pss_shutter2_con</value>
				<value>fast_shutter_con</value>
				<!-- others -->
				<value>img22b</value>
				<value>img23b</value>
				<value>cap21b</value>
				<value>cap22b</value>
			</set>
		</property>
	</bean>

	<bean id="DefaultDataWriterFactory" class="gda.data.scan.datawriter.DefaultDataWriterFactory">
		<property name="dataWriterExtenders">
			<list>
				<ref bean="file_registrar" />
			</list>
		</property>
	</bean>

</beans>
