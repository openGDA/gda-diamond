				<value>s1b_ysize</value>
				<value>s1b_xpos</value>
				<value>s1b_xneg</value>
				<!-- s2b -->
				<value>s2b_xsize</value>
				<value>s2b_ysize</value>
				<value>s2b_xcentre</value>
				<value>s2b_ycentre</value>
				<!-- s3b -->
				<value>s3b_xsize</value>
				<value>s3b_ysize</value>
				<value>s3b_xcentre</value>
				<value>s3b_ycentre</value>
				<!-- s4b -->
				<value>s4b_xgapsize</value>
				<value>s4b_ygapsize</value>
				<value>s4b_z</value>
				<!-- m1b -->
				<value>m1b_x</value>
				<value>m1b_y</value>
				<value>m1b_pitch</value>
				<value>m1b_roll</value>
				<value>m1b_yaw</value>
				<value>m1b_mask_temp</value>
				<value>m1b_mirror_temp</value>
				<value>m1b_chiller</value>
				<!-- m1c -->
				<value>m1c_mirror_temp</value>
				<value>m1c_mask_temp</value>
				<!-- m3b -->
				<value>m3b_x</value>
				<value>m3b_y</value>
				<value>m3b_pitch</value>
				<value>m3b_roll</value>
				<value>m3b_yaw</value>
				<!-- m4b -->
				<value>m4b_x</value>
				<value>m4b_y</value>
				<value>m4b_pitch</value>
				<value>m4b_roll</value>
				<value>m4b_yaw</value>
				<value>m4b_y_base</value>
				<value>m4b_y_base_positioner</value>
				<value>m4b_y_combined</value>
				<value>m4b_mirror</value>
