				<value>m5b_y_base_positioner</value>
				<value>m5b_y_combined</value>
				<value>m5b_mirror</value>
				<!-- d1b -->
				<value>d1b_y</value>
				<!-- d2b -->
				<value>d2b_y</value>
				<value>d2b_y_positioner</value>
				<!-- d3b -->
				<value>d3b_y</value>
				<value>d3b_y_positioner</value>
				<!-- d4b -->
				<value>d4b_y</value>
				<value>d4b_y_positioner</value>
