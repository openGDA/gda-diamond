<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd" >

	<!-- Slits -->
	<import resource="s1b.xml"/>
	<import resource="s2b.xml"/>
	<import resource="s3b.xml"/>
	<import resource="s4b.xml"/>

	<!-- Mirrors -->
	<import resource="m1b.xml"/>
	<import resource="m1c.xml"/>
	<import resource="m3b.xml"/>
	<import resource="m4b.xml"/>
	<import resource="m5b.xml"/>

	<!-- Diagnostics -->
	<import resource="d1b.xml"/>
	<import resource="d2b.xml"/>
	<import resource="d3b.xml"/>
	<import resource="d4b.xml"/>
	<import resource="d5b.xml"/>
	<import resource="d6b.xml"/>
	<import resource="d7b.xml"/>
	<import resource="d8b.xml"/>
	<import resource="d9b.xml"/>
	<import resource="d10b.xml"/>
	<import resource="d21b.xml"/>
	<import resource="femtos.xml"/>
	<import resource="stanfords.xml"/>

	<!-- Shutters -->
	<import resource="shutter.xml"/>

	<!-- Simulated Area Detector -->
	<import resource="adSimulator.xml"/>

	<!-- Monochromators -->
	<import resource="pgm.xml"/>

	<!-- Endstations -->
	<import resource="es2b_cam1.xml"/>
	<import resource="es2b_cam2.xml"/>
	<import resource="sm21b.xml"/>
	
	<!-- Pressures -->
	<import resource="pressures.xml" />

	<!-- Do not remove anything below here -->
	<import resource="file:${gda.config}/servers/main/_common/server.xml" />

</beans>
