<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="sm21b_x_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL07B-EA-SM-21:X" />
	</bean>

	<bean id="sm21b_z_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL07B-EA-SM-21:Z" />
	</bean>

	<bean id="sm21b_y_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL07B-EA-SM-21:Y" />
	</bean>

	<bean id="sm21b_roty_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL07B-EA-SM-21:ROTY" />
	</bean>

	<bean id="sm21b_y_positioner" class="gda.spring.EpicsEnumPositionerFactoryBean">
		<property name="pvBase" value="BL07B-EA-SM-21:Y:MP"/>
	</bean>
</beans>
