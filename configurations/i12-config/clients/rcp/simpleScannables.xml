<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
	<description>
		Handles to the ss1 motors that needs to be used in the rcp
		client
	</description>

	
	<bean id="idummyscannablemotor_x" class="uk.ac.gda.remoting.client.GdaRmiProxyFactoryBean">
		<property name="serviceUrl" value="${gda.rmi.prefix}/gda/idummyscannablemotor_x" />
		<property name="serviceInterface" value="gda.device.IScannableMotor" />
		<property name="refreshStubOnConnectFailure" value="true" />
	</bean>
</beans>