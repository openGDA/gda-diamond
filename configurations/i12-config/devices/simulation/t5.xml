<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<!-- Define all the motors -->
	<bean id="t5_x_motor" class="gda.spring.EpicsMotorFactoryBean">
		<property name="deviceName" value="t5.x" />
	</bean>

	<bean id="t5_y_motor" class="gda.spring.EpicsMotorFactoryBean">
		<property name="deviceName" value="t5.y" />
	</bean>

	<bean id="t5_z_motor" class="gda.spring.EpicsMotorFactoryBean">
		<property name="deviceName" value="t5.z" />
	</bean>

</beans>
