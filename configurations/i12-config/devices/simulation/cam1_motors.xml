<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
	<!--  Define all the motors -->
	<bean id="cam1_roll_motor" class="gda.spring.EpicsMotorFactoryBean">
		<property name="deviceName" value="cam1.roll" />
	</bean>
	<bean id="cam1_x_motor" class="gda.spring.EpicsMotorFactoryBean">
		<property name="deviceName" value="cam1.x" />
	</bean>
	<bean id="cam1_z_motor" class="gda.spring.EpicsMotorFactoryBean">
		<property name="deviceName" value="cam1.z" />
	</bean>

</beans>
