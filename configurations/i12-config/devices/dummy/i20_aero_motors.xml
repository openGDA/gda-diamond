<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">


	<bean id="analyser_x_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-12.5" />
		<property name="maxPosition" value="12.5" />
		<property name="position" value="6.3" />
		<property name="unitString" value="mm" />
	</bean>

	<bean id="analyser_y_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-2.2" />
		<property name="maxPosition" value="15" />
		<property name="position" value="-1" />
		<property name="unitString" value="mm" />
	</bean>

	<bean id="aero_pitch_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="0" />
		<property name="maxPosition" value="20" />
		<property name="position" value="7.2" />
		<property name="unitString" value="mm" />
	</bean>

	<bean id="aero_roll_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-12.3" />
		<property name="maxPosition" value="12.5" />
		<property name="position" value="3.8" />
		<property name="unitString" value="mm" />
	</bean>

</beans>