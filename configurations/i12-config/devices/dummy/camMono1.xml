<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="camMono1_ad_pluginBase" class="gda.device.detector.areadetector.v17.impl.NDPluginBaseSimulator">
		<property name="dims" value="1000,1000" />
		<property name="datatype" value="UINT32" />
	</bean>

	<bean id="camMono1_adDriverPco1" class="gda.device.detector.areadetector.v17.impl.DummyADDriverPco" />

	<bean id="camMono1.cam" class="gda.device.detector.areadetector.v17.impl.ADBaseSimulator">
		<property name="model" value="camMono1" />
	</bean>

	<bean id="camMono1.roi1" class="gda.device.detector.areadetector.v17.impl.NDROISimulator">
		<property name="pluginBase" ref="camMono1_ad_pluginBase" />
	</bean>

	<bean id="camMono1.roi2" class="gda.device.detector.areadetector.v17.impl.NDROISimulator">
		<property name="pluginBase" ref="camMono1_ad_pluginBase" />
	</bean>

	<bean id="camMono1.arr" class="gda.device.detector.areadetector.v17.impl.NDArraySimulator">
		<property name="pluginBase" ref="camMono1_ad_pluginBase" />
	</bean>

	<bean id="camMono1.proc1" class="gda.device.detector.areadetector.v17.impl.NDProcessSimulator">
		<property name="pluginBase" ref="camMono1_ad_pluginBase" />
	</bean>

	<bean id="camMono1.proc2" class="gda.device.detector.areadetector.v17.impl.NDProcessSimulator">
		<property name="pluginBase" ref="camMono1_ad_pluginBase" />
	</bean>

	<bean id="camMono1.stat" class="gda.device.detector.areadetector.v17.impl.NDStatsSimulator">
		<property name="pluginBase" ref="camMono1_ad_pluginBase" />
	</bean>

	<bean id="camMono1.draw" class="gda.device.detector.areadetector.v17.impl.DummyNDOverlay" />

	<bean id="camMono1.mjpeg1" class="gda.device.detector.areadetector.v17.impl.FfmpegStreamSimulator">
		<property name="pluginBase" ref="camMono1_ad_pluginBase" />
		<property name="mjpg_url" value="DummySwtVideoReceiver" />
	</bean>

	<bean id="camMono1.mjpeg2" class="gda.device.detector.areadetector.v17.impl.FfmpegStreamSimulator">
		<property name="pluginBase" ref="camMono1_ad_pluginBase" />
		<property name="mjpg_url" value="DummySwtVideoReceiver" />
	</bean>

	<bean id="camMono1.tif" class="gda.device.detector.areadetector.v17.impl.NDFileSimulator">
		<property name="pluginBase" ref="camMono1_ad_pluginBase" />
	</bean>

	<bean id="camMono1_ndfilehdf5_base"
		class="gda.device.detector.areadetector.v17.impl.NDFileSimulator">
		<property name="pluginBase" ref="camMono1_ad_pluginBase" />
	</bean>

	<bean id="camMono1.hdf" class="gda.device.detector.areadetector.v17.impl.NDFileHDF5Simulator">
		<property name="file" ref="camMono1_ndfilehdf5_base" />
	</bean>

</beans>