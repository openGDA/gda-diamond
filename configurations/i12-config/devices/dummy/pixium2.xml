<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="pixium10_ad_pluginBase" class="gda.device.detector.areadetector.v17.impl.NDPluginBaseSimulator">
		<property name="dims" value="1000,1000" />
		<property name="datatype" value="UINT32" />
	</bean>

	<bean id="pixium10_cam_base" class="gda.device.detector.areadetector.v17.impl.ADBaseSimulator">
		<property name="model" value="PCOEdge" />
	</bean>

	<bean id="pixium10_arr" class="gda.device.detector.areadetector.v17.impl.NDArraySimulator">
		<property name="pluginBase" ref="pixium10_ad_pluginBase" />
	</bean>

	<bean id="pixium10_stat" class="gda.device.detector.areadetector.v17.impl.NDStatsSimulator">
		<property name="pluginBase" ref="pixium10_ad_pluginBase" />
	</bean>

	<bean id="pixium10_stat1" class="gda.device.detector.areadetector.v17.impl.NDStatsSimulator">
		<property name="pluginBase" ref="pixium10_ad_pluginBase" />
	</bean>

	<bean id="pixium10_stat2" class="gda.device.detector.areadetector.v17.impl.NDStatsSimulator">
		<property name="pluginBase" ref="pixium10_ad_pluginBase" />
	</bean>

	<bean id="pixium10_roi" class="gda.device.detector.areadetector.v17.impl.NDROISimulator">
		<property name="pluginBase" ref="pixium10_ad_pluginBase" />
	</bean>

	<bean id="pixium10_roi1" class="gda.device.detector.areadetector.v17.impl.NDROISimulator">
		<property name="pluginBase" ref="pixium10_ad_pluginBase" />
	</bean>

	<bean id="pixium10_roi2" class="gda.device.detector.areadetector.v17.impl.NDROISimulator">
		<property name="pluginBase" ref="pixium10_ad_pluginBase" />
	</bean>

	<bean id="pixium10_ffmpeg" class="gda.device.detector.areadetector.v17.impl.FfmpegStreamSimulator">
		<property name="pluginBase" ref="pixium10_ad_pluginBase" />
		<property name="mjpg_url" value="DummySwtVideoReceiver" />
	</bean>

	<bean id="pixium10_ndfilehdf5_base" class="gda.device.detector.areadetector.v17.impl.NDFileSimulator">
		<property name="pluginBase" ref="pixium10_ad_pluginBase" />
	</bean>

	<bean id="pixium10_ndfilehdf" class="gda.device.detector.areadetector.v17.impl.NDFileHDF5Simulator">
		<property name="file" ref="pixium10_ndfilehdf5_base" />
	</bean>

	<bean id="pixium10_ndfiletif" class="gda.device.detector.areadetector.v17.impl.NDFileSimulator">
		<property name="pluginBase" ref="pixium10_ad_pluginBase" />
	</bean>

	<bean id="pixium10_proc" class="gda.device.detector.areadetector.v17.impl.NDProcessSimulator">
		<property name="pluginBase" ref="pixium10_ad_pluginBase" />
	</bean>

</beans>	
