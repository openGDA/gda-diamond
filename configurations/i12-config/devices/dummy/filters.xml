<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="f1" class="gda.device.enumpositioner.DummyEnumPositioner">
		<property name="positions">
			<list>
				<value>2mm</value>
				<value>clear</value>
				<value>8mm</value>
			</list>
		</property>
	</bean>

	<bean id="f2" class="gda.device.enumpositioner.DummyEnumPositioner">
		<property name="positions">
			<list>
				<value>1mm</value>
				<value>clear</value>
				<value>4mm</value>
			</list>
		</property>
	</bean>

</beans>