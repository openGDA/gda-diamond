		<property name="motor" ref="t3m3_rz_motor" />
		<property name="tolerance" value="0.01" />
		<property name="outputFormat">
			<list>
				<value>%.3f</value>
			</list>
		</property>
	</bean>
	<bean id="t3_m4x" class="gda.device.scannable.ScannableMotor">
		<property name="name" value="t3_m4x" />
		<property name="motor" ref="t3m4_x_motor" />
		<property name="tolerance" value="0.01" />
		<property name="outputFormat">
			<list>
				<value>%.3f</value>
			</list>
		</property>
	</bean>
	<bean id="t3_m4y" class="gda.device.scannable.ScannableMotor">
		<property name="name" value="t3_m4y" />
		<property name="motor" ref="t3m4_y_motor" />
		<property name="tolerance" value="0.01" />
