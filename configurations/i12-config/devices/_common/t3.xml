		<property name="outputFormat">
			<list>
				<value>%.3f</value>
			</list>
		</property>
	</bean>
	<bean id="t3_m3rx" class="gda.device.scannable.ScannableMotor">
		<property name="name" value="t3_m3rx" />
		<property name="motor" ref="t3m3_rx_motor" />
		<property name="tolerance" value="0.01" />
		<property name="outputFormat">
			<list>
				<value>%.3f</value>
			</list>
		</property>
	</bean>
	<bean id="t3_m3ry" class="gda.device.scannable.ScannableMotor">
		<property name="name" value="t3_m3ry" />
		<property name="motor" ref="t3m3_ry_motor" />
		<property name="tolerance" value="0.01" />
		<property name="outputFormat">
