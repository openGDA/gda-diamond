		</property>
	</bean>
	<bean id="t3_m4rx" class="gda.device.scannable.ScannableMotor">
		<property name="name" value="t3_m4rx" />
		<property name="motor" ref="t3m4_rx_motor" />
		<property name="tolerance" value="0.01" />
		<property name="outputFormat">
			<list>
				<value>%.3f</value>
			</list>
		</property>
	</bean>
	<bean id="t3_m4ry" class="gda.device.scannable.ScannableMotor">
		<property name="name" value="t3_m4ry" />
		<property name="motor" ref="t3m4_ry_motor" />
