			</list>
		</property>
	</bean>
	<bean id="t3_m2z" class="gda.device.scannable.ScannableMotor">
		<property name="name" value="t3_m2z" />
		<property name="motor" ref="t3m2_z_motor" />
		<property name="tolerance" value="0.01" />
		<property name="outputFormat">
			<list>
				<value>%.3f</value>
			</list>
		</property>
	</bean>
	<bean id="t3_m3y" class="gda.device.scannable.ScannableMotor">
		<property name="name" value="t3_m3y" />
		<property name="motor" ref="t3m3_y_motor" />
		<property name="tolerance" value="0.01" />
		<property name="outputFormat">
			<list>
				<value>%.3f</value>
