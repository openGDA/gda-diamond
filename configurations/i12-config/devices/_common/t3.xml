		</property>
	</bean>

	<gda:rmi-export
		serviceName="gda/t3_x"
		service="t3_x"
		serviceInterface="gda.device.IScannableMotor" />

	<gda:rmi-export
		serviceName="gda/t3_m1y"
		service="t3_m1y"
		serviceInterface="gda.device.IScannableMotor" />

	<gda:rmi-export
		serviceName="gda/t3_m1z"
		service="t3_m1z"
		serviceInterface="gda.device.IScannableMotor" />

</beans>