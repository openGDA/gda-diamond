<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:gda="http://www.diamond.ac.uk/schema/gda/gda"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
		http://www.diamond.ac.uk/schema/gda/gda
		http://www.diamond.ac.uk/schema/gda/gda/gda-gda-1.1.xsd">

	<!-- Define the scannable group -->
	<bean id="t3" class="gda.device.scannable.scannablegroup.ScannableGroup">
		<property name="groupMembers">
			<list>
				<!-- Scannables in the group -->
				<ref bean="t3_m1y" />
				<ref bean="t3_m1z" />
				<ref bean="t3_m2y" />
				<ref bean="t3_m2z" />
				<ref bean="t3_m3rx" />
				<ref bean="t3_m3ry" />
				<ref bean="t3_m3rz" />
				<ref bean="t3_m3y" />
				<ref bean="t3_m3z" />
				<ref bean="t3_m4rx" />
				<ref bean="t3_m4ry" />
				<ref bean="t3_m4x" />
				<ref bean="t3_m4y" />
				<ref bean="t3_x" />
			</list>
		</property>
	</bean>
	<bean id="t3_x" class="gda.device.scannable.ScannableMotor">
		<property name="name" value="t3_x" />
		<property name="motor" ref="t3_x_motor" />
		<property name="tolerance" value="0.01" />
		<property name="outputFormat">
			<list>
				<value>%.3f</value>
			</list>
		</property>
	</bean>
	<bean id="t3_m1y" class="gda.device.scannable.ScannableMotor">
		<property name="name" value="t3_m1y" />
		<property name="motor" ref="t3m1_y_motor" />
		<property name="tolerance" value="0.01" />
		<property name="outputFormat">
			<list>
				<value>%.3f</value>
			</list>
		</property>
	</bean>
	<bean id="t3_m1z" class="gda.device.scannable.ScannableMotor">
		<property name="name" value="t3_m1z" />
		<property name="motor" ref="t3m1_z_motor" />
		<property name="tolerance" value="0.01" />
		<property name="outputFormat">
			<list>
				<value>%.3f</value>
			</list>
		</property>
	</bean>
	<bean id="t3_m2y" class="gda.device.scannable.ScannableMotor">
		<property name="name" value="t3_m2y" />
		<property name="motor" ref="t3m2_y_motor" />
