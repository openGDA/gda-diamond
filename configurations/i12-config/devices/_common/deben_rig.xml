<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="deben_rot_top" class="gda.device.scannable.ScannableMotor">
		<property name="motor" ref="deben_rot_top_motor" />
	</bean>

	<bean id="deben_rot_bottom" class="gda.device.scannable.ScannableMotor">
		<property name="motor" ref="deben_rot_bottom_motor" />
	</bean>

	<bean id="deben_y" class="gda.device.scannable.ScannableMotor">
		<property name="motor" ref="deben_lin_motor" />
	</bean>

</beans>