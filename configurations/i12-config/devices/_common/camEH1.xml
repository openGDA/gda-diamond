<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="camEH1" class="uk.ac.gda.devices.edxd.AdDetectorExtRoiDraw">
		<property name="adBase" ref="camEH1.cam" />
		<property name="ndStats" ref="camEH1.stat" />
		<property name="ndArray" ref="camEH1.arr" />
		<property name="ndFile" ref="camEH1.tif" />
		<property name="ndRoi1" ref="camEH1.roi1" />
		<property name="ndRoi2" ref="camEH1.roi2" />
		<property name="ndProc1" ref="camEH1.proc1" />
		<property name="ndProc2" ref="camEH1.proc2" />
		<property name="ndDraw" ref="camEH1.draw" />
		<property name="mjpeg1" ref="camEH1.mjpeg1" />
		<property name="mjpeg2" ref="camEH1.mjpeg2" />
		<property name="computeCentroid" value="true" />
		<property name="computeStats" value="true" />
	</bean>

</beans>