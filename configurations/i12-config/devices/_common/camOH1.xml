<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="camOH1" class="uk.ac.gda.devices.edxd.AdDetectorExtRoiDraw">
		<property name="adBase" ref="camOH1.cam" />
		<property name="ndStats" ref="camOH1.stat" />
		<property name="ndArray" ref="camOH1.arr" />
		<property name="ndFile" ref="camOH1.tif" />
		<property name="ndRoi1" ref="camOH1.roi1" />
		<property name="ndRoi2" ref="camOH1.roi2" />
		<property name="ndProc1" ref="camOH1.proc1" />
		<property name="ndProc2" ref="camOH1.proc2" />
		<property name="ndDraw" ref="camOH1.draw" />
		<property name="mjpeg1" ref="camOH1.mjpeg1" />
		<property name="mjpeg2" ref="camOH1.mjpeg2" />
		<property name="computeCentroid" value="true" />
		<property name="computeStats" value="true" />
	</bean>

</beans>