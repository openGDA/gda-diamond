
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
	<!--  Define all the motors 
  -->
	<bean id="s3_top_motor" class="gda.spring.EpicsMotorFactoryBean">
		<property name="deviceName" value="s3.top" />
	</bean>
	<bean id="s3_bottom_motor" class="gda.spring.EpicsMotorFactoryBean">
		<property name="deviceName" value="s3.bottom" />
	</bean>
	<bean id="s3_in_motor" class="gda.spring.EpicsMotorFactoryBean">
		<property name="deviceName" value="s3.in" />
	</bean>
	<bean id="s3_out_motor" class="gda.spring.EpicsMotorFactoryBean">
		<property name="deviceName" value="s3.out" />
	</bean>
	<bean id="s3_xs_motor" class="gda.spring.EpicsMotorFactoryBean">
		<property name="deviceName" value="s3.xs" />
	</bean>
	<bean id="s3_xc_motor" class="gda.spring.EpicsMotorFactoryBean">
		<property name="deviceName" value="s3.xc" />
	</bean>
	<bean id="s3_ys_motor" class="gda.spring.EpicsMotorFactoryBean">
		<property name="deviceName" value="s3.ys" />
	</bean>
	<bean id="s3_yc_motor" class="gda.spring.EpicsMotorFactoryBean">
		<property name="deviceName" value="s3.yc" />
	</bean>
</beans>