<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<!-- Define all the motors -->
	<bean id="t8_xcoarse_motor" class="gda.spring.EpicsMotorFactoryBean">
		<property name="deviceName" value="t8.xcoarse" />
	</bean>

	<bean id="t8_x_motor" class="gda.spring.EpicsMotorFactoryBean">
		<property name="deviceName" value="t8.x" />
	</bean>

	<bean id="t8_y_motor" class="gda.spring.EpicsMotorFactoryBean">
		<property name="deviceName" value="t8.y" />
	</bean>

</beans>
