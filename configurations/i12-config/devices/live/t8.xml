<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- Define all the motors -->
	<bean id="t8_xcoarse_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL12I-MO-TAB-08:X" />
	</bean>

	<bean id="t8_x_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL12I-MO-TAB-08:BENCH:X" />
	</bean>

	<bean id="t8_y_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL12I-MO-TAB-08:BENCH:Y" />
	</bean>

</beans>
