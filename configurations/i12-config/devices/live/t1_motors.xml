<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- Define all the motors -->
	<bean id="t1_x_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL12I-MO-TAB-01:X" />
	</bean>

	<bean id="t1_y_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL12I-MO-TAB-01:Y" />
	</bean>

	<bean id="t1_z_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL12I-MO-TAB-01:Z" />
	</bean>

</beans>
