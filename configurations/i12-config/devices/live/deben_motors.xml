<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">


	<bean id="deben_rot_top_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL12I-EA-DOF-01:RIG:TOP:MOTOR"/>
		<property name="local" value="true" />
	</bean>
	<bean id="deben_rot_bottom_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL12I-EA-DOF-01:RIG:BOT:MOTOR"/>
		<property name="local" value="true" />
	</bean>
	<bean id="deben_lin_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL12I-EA-DOF-01:RIG:LIN:MOTOR"/>
		<property name="local" value="true" />
	</bean>


</beans>	
