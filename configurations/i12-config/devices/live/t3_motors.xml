
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!--  Define all the motors -->
	<bean id="t3_x_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL12I-MO-TAB-03:X" />
	</bean>

	<bean id="t3m1_y_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL12I-MO-TAB-03:MOD1:Y" />
	</bean>

	<bean id="t3m1_z_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL12I-MO-TAB-03:MOD1:Z" />
	</bean>

	<bean id="t3m2_y_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL12I-MO-TAB-03:MOD2:Y" />
	</bean>

	<bean id="t3m2_z_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL12I-MO-TAB-03:MOD2:Z" />
	</bean>

	<bean id="t3m3_y_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL12I-MO-TAB-03:MOD3:Y" />
	</bean>

	<bean id="t3m3_z_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL12I-MO-TAB-03:MOD3:Z" />
	</bean>

	<bean id="t3m3_rx_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL12I-MO-TAB-03:MOD3:X:ROT" />
	</bean>

	<bean id="t3m3_ry_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL12I-MO-TAB-03:MOD3:Y:ROT" />
	</bean>

	<bean id="t3m3_rz_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL12I-MO-TAB-03:MOD3:Z:ROT" />
	</bean>

	<bean id="t3m4_x_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL12I-MO-TAB-03:MOD4:X" />
	</bean>

	<bean id="t3m4_y_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL12I-MO-TAB-03:MOD4:Y" />
	</bean>

	<bean id="t3m4_rx_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL12I-MO-TAB-03:MOD4:X:ROT" />
	</bean>

	<bean id="t3m4_ry_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL12I-MO-TAB-03:MOD4:Y:ROT" />
	</bean>

</beans>