<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<!-- Set up the Area Detector base class which will manage the camFloat2.cam -->
	<bean id="camFloat2.cam" class="gda.device.detector.areadetector.v17.impl.ADBaseImpl">
		<property name="basePVName" value="BL12I-DI-PHDGN-07:CAM:" />
		<property name="initialDataType" value="UInt8" />
		<property name="initialMinX" value="1" />
		<property name="initialMinY" value="1" />
		<property name="initialBinX" value="1" />
		<property name="initialBinY" value="1" />
	</bean>

	<bean id="camFloat2.roi1" class="gda.device.detector.areadetector.v17.impl.NDROIImpl">
		<property name="basePVName" value="BL12I-DI-PHDGN-07:ROI1:" />
		<property name="pluginBase">
			<bean class="gda.device.detector.areadetector.v17.impl.NDPluginBaseImpl">
				<property name="basePVName" value="BL12I-DI-PHDGN-07:ROI1:" />
				<property name="initialArrayPort" value="camFloat2.cam" />
				<property name="initialArrayAddress" value="0" />
				<property name="initialEnableCallbacks" value="1" />
				<property name="initialBlockingCallbacks" value="false" />
			</bean>
		</property>
		<property name="initialEnableX" value="true" />
		<property name="initialEnableY" value="true" />
		<property name="initialMinX" value="0" />
		<property name="initialMinY" value="0" />
		<property name="initialBinX" value="1" />
		<property name="initialBinY" value="1" />
	</bean>

	<bean id="camFloat2.roi2" class="gda.device.detector.areadetector.v17.impl.NDROIImpl">
		<property name="basePVName" value="BL12I-DI-PHDGN-07:ROI2:" />
		<property name="pluginBase">
			<bean class="gda.device.detector.areadetector.v17.impl.NDPluginBaseImpl">
				<property name="basePVName" value="BL12I-DI-PHDGN-07:ROI2:" />
				<property name="initialArrayPort" value="camFloat2.cam" />
				<property name="initialArrayAddress" value="0" />
				<property name="initialEnableCallbacks" value="0" />
				<property name="initialBlockingCallbacks" value="false" />
			</bean>
		</property>
		<property name="initialEnableX" value="true" />
		<property name="initialEnableY" value="true" />
		<property name="initialMinX" value="1" />
		<property name="initialMinY" value="1" />
		<property name="initialBinX" value="1" />
		<property name="initialBinY" value="1" />
	</bean>

	<bean id="camFloat2.proc1" class="gda.device.detector.areadetector.v17.impl.NDProcessImpl">
		<property name="basePVName" value="BL12I-DI-PHDGN-07:PROC1:" />
		<property name="pluginBase">
			<bean class="gda.device.detector.areadetector.v17.impl.NDPluginBaseImpl">
				<property name="basePVName" value="BL12I-DI-PHDGN-07:PROC1:" />
				<property name="initialArrayPort" value="camFloat2.roi1" />
				<property name="initialArrayAddress" value="0" />
				<property name="initialEnableCallbacks" value="0" />
				<property name="initialBlockingCallbacks" value="false" />
			</bean>
		</property>
		<property name="initialEnableBackground" value="0" />
		<property name="initialEnableFlatField" value="0" />
		<property name="initialEnableOffsetScale" value="0" />
		<property name="initialEnableHighClip" value="0" />
		<property name="initialEnableLowClip" value="0" />
		<property name="initialEnableFilter" value="0" />
	</bean>

	<bean id="camFloat2.proc2" class="gda.device.detector.areadetector.v17.impl.NDProcessImpl">
		<property name="basePVName" value="BL12I-DI-PHDGN-07:PROC2:" />
		<property name="pluginBase">
			<bean class="gda.device.detector.areadetector.v17.impl.NDPluginBaseImpl">
				<property name="basePVName" value="BL12I-DI-PHDGN-07:PROC2:" />
				<property name="initialArrayPort" value="camFloat2.roi2" />
				<property name="initialArrayAddress" value="0" />
				<property name="initialEnableCallbacks" value="0" />
				<property name="initialBlockingCallbacks" value="false" />
			</bean>
		</property>
		<property name="initialEnableBackground" value="0" />
		<property name="initialEnableFlatField" value="0" />
		<property name="initialEnableOffsetScale" value="0" />
		<property name="initialEnableHighClip" value="0" />
		<property name="initialEnableLowClip" value="0" />
		<property name="initialEnableFilter" value="0" />
	</bean>

	<bean id="camFloat2.stat" class="gda.device.detector.areadetector.v17.impl.NDStatsImpl">
		<property name="basePVName" value="BL12I-DI-PHDGN-07:STAT:" />
		<property name="pluginBase">
			<bean class="gda.device.detector.areadetector.v17.impl.NDPluginBaseImpl">
				<property name="basePVName" value="BL12I-DI-PHDGN-07:STAT:" />
				<property name="initialArrayPort" value="camFloat2.roi1" />
				<property name="initialArrayAddress" value="0" />
				<property name="initialEnableCallbacks" value="0" />
				<property name="initialBlockingCallbacks" value="false" />
			</bean>
		</property>
	</bean>

	<bean id="camFloat2.draw" class="gda.device.detector.areadetector.v17.impl.NDOverlayImpl">
		<property name="basePVName" value="BL12I-DI-PHDGN-07:DRAW:" />
		<property name="pluginBase">
			<bean class="gda.device.detector.areadetector.v17.impl.NDPluginBaseImpl">
				<property name="basePVName" value="BL12I-DI-PHDGN-07:DRAW:" />
				<property name="initialArrayPort" value="camFloat2.roi1" />
				<property name="initialArrayAddress" value="0" />
				<property name="initialEnableCallbacks" value="1" />
				<property name="initialBlockingCallbacks" value="false" />
			</bean>
		</property>
	</bean>

	<bean id="camFloat2.mjpeg1"
		class="gda.device.detector.areadetector.v17.impl.FfmpegStreamImpl">
		<property name="basePVName" value="BL12I-DI-PHDGN-07:MPEG1:" />
		<property name="pluginBase">
			<bean class="gda.device.detector.areadetector.v17.impl.NDPluginBaseImpl">
				<property name="basePVName" value="BL12I-DI-PHDGN-07:MPEG1:" />
				<property name="initialArrayPort" value="camFloat2.draw" />
				<property name="initialArrayAddress" value="0" />
				<property name="initialEnableCallbacks" value="1" />
				<property name="initialBlockingCallbacks" value="false" />
			</bean>
		</property>
		<property name="initialQuality" value="100" />
		<property name="initialFalseColor" value="0" />
		<property name="initialAlwaysOn" value="0" />
	</bean>

	<bean id="camFloat2.mjpeg2"
		class="gda.device.detector.areadetector.v17.impl.FfmpegStreamImpl">
		<property name="basePVName" value="BL12I-DI-PHDGN-07:MPEG2:" />
		<property name="pluginBase">
			<bean class="gda.device.detector.areadetector.v17.impl.NDPluginBaseImpl">
				<property name="basePVName" value="BL12I-DI-PHDGN-07:MPEG2:" />
				<property name="initialArrayPort" value="camFloat2.draw" />
				<property name="initialArrayAddress" value="0" />
				<property name="initialEnableCallbacks" value="0" />
				<property name="initialBlockingCallbacks" value="false" />
			</bean>
		</property>
		<property name="initialQuality" value="100" />
		<property name="initialFalseColor" value="0" />
		<property name="initialAlwaysOn" value="0" />
	</bean>

	<bean id="camFloat2.tif" class="gda.device.detector.areadetector.v17.impl.NDFileImpl">
		<property name="basePVName" value="BL12I-DI-PHDGN-07:TIFF:" />
		<property name="pluginBase">
			<bean class="gda.device.detector.areadetector.v17.impl.NDPluginBaseImpl">
				<property name="basePVName" value="BL12I-DI-PHDGN-07:TIFF:" />
				<property name="initialArrayPort" value="camFloat2.draw" />
				<property name="initialArrayAddress" value="0" />
				<property name="initialEnableCallbacks" value="0" />
				<property name="initialBlockingCallbacks" value="false" />
			</bean>
		</property>
		<property name="initialWriteMode" value="2" />
		<property name="initialNumCapture" value="1" />
		<property name="initialFileName" value="p" />
		<property name="initialFileTemplate" value="%s/%s_%05d.tif" />
	</bean>

	<bean id="camFloat2.hdf"
		class="gda.device.detector.areadetector.v17.impl.NDFileHDF5Impl">
		<property name="basePVName" value="BL12I-DI-PHDGN-07:HDF:" />
		<property name="file">
			<bean class="gda.device.detector.areadetector.v17.impl.NDFileImpl">
				<property name="basePVName" value="BL12I-DI-PHDGN-07:HDF:" />
				<property name="pluginBase">
					<bean class="gda.device.detector.areadetector.v17.impl.NDPluginBaseImpl">
						<property name="basePVName" value="BL12I-DI-PHDGN-07:HDF:" />
						<property name="initialArrayPort" value="camFloat2.draw" />
						<property name="initialArrayAddress" value="0" />
						<property name="initialEnableCallbacks" value="0" />
						<property name="initialBlockingCallbacks" value="false" />
					</bean>
				</property>
				<property name="initialWriteMode" value="2" />
				<property name="initialNumCapture" value="1" />
				<property name="initialFileName" value="projections" />
				<property name="initialFileTemplate" value="%s/%s.h5" />
			</bean>
		</property>
		<property name="initialNumExtraDims" value="0" />
		<property name="initialExtraDimSizeN" value="1" />
		<property name="initialExtraDimSizeX" value="0" />
		<property name="initialExtraDimSizeY" value="0" />
		<property name="initialNumRowChunks" value="2672" />
		<property name="initialCompression" value="zlib" />
		<property name="initialNumBitOffset" value="0" />
		<property name="initialNumBitPrecision" value="8" />
		<property name="initialSzipNumPixels" value="16" />
		<property name="initialZCompressLevel" value="6" />
	</bean>

	<bean id="camFloat2.arr" class="gda.device.detector.areadetector.v17.impl.NDArrayImpl">
		<property name="basePVName" value="BL12I-DI-PHDGN-07:ARR:" />
		<property name="pluginBase">
			<bean class="gda.device.detector.areadetector.v17.impl.NDPluginBaseImpl">
				<property name="basePVName" value="BL12I-DI-PHDGN-07:ARR:" />
				<property name="initialArrayPort" value="camFloat2.cam" />
				<property name="initialArrayAddress" value="0" />
				<property name="initialEnableCallbacks" value="1" />
				<property name="initialBlockingCallbacks" value="false" />
			</bean>
		</property>
	</bean>

</beans>