<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!--  Define all the motors -->
	<bean id="t4_xcoarse_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL12I-MO-TAB-04:X" />
	</bean>

	<bean id="t4_x_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL12I-MO-TAB-04:BENCH:X" />
	</bean>

	<bean id="t4_y_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL12I-MO-TAB-04:BENCH:Y" />
	</bean>

</beans>

