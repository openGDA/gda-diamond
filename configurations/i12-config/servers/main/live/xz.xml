<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<!-- Define all the motors -->
	<bean id="xz_x_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL12I-MO-TAB-09:X" />
		<property name="local" value="true" />
	</bean>

	<bean id="xz_z_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL12I-MO-TAB-09:Z" />
		<property name="local" value="true" />
	</bean>

</beans>
