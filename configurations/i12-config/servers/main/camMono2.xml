<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
	<!-- Set up the Area Detector base class which will manage the pix.cam -->
	<bean id="mono2.cam" class="gda.device.detector.areadetector.v17.impl.ADBaseImpl">
		<!-- <property name="deviceName" value="pco.cam_ADBase" /> -->
		<property name="basePVName" value="BL12I-OP-DCM-01:MONO2:CAM:" />
		<property name="initialDataType" value="UInt16" />
		<property name="initialMinX" value="1" />
		<property name="initialMinY" value="1" />
		<property name="initialSizeX" value="1292" />
		<property name="initialSizeY" value="964" />
		<property name="initialBinX" value="1" />
		<property name="initialBinY" value="1" />
	</bean>

	<bean id="mono2.proc1" class="gda.device.detector.areadetector.v17.impl.NDProcessImpl">
		<property name="basePVName" value="BL12I-OP-DCM-01:MONO2:PROC1:" />
		<property name="pluginBase">
			<bean class="gda.device.detector.areadetector.v17.impl.NDPluginBaseImpl">
				<!-- <property name="deviceName" value="pco.proc_NDPluginBase" /> -->
				<property name="basePVName" value="BL12I-OP-DCM-01:MONO2:PROC1:" />
				<property name="initialArrayPort" value="mono2.cam" />
				<property name="initialArrayAddress" value="0" />
				<property name="initialEnableCallbacks" value="1" />
				<property name="initialBlockingCallbacks" value="false" />
			</bean>
		</property>
		<property name="initialEnableBackground" value="0" />
		<property name="initialEnableFlatField" value="0" />
		<property name="initialEnableOffsetScale" value="0" />
		<property name="initialEnableHighClip" value="0" />
		<property name="initialEnableLowClip" value="0" />
		<property name="initialEnableFilter" value="0" />
	</bean>

	<bean id="mono2.proc2" class="gda.device.detector.areadetector.v17.impl.NDProcessImpl">
		<property name="basePVName" value="BL12I-OP-DCM-01:MONO2:PROC2:" />
		<property name="pluginBase">
			<bean class="gda.device.detector.areadetector.v17.impl.NDPluginBaseImpl">
				<!-- <property name="deviceName" value="pco.proc_NDPluginBase" /> -->
				<property name="basePVName" value="BL12I-OP-DCM-01:MONO2:PROC2:" />
				<property name="initialArrayPort" value="mono2.cam" />
				<property name="initialArrayAddress" value="0" />
				<property name="initialEnableCallbacks" value="1" />
				<property name="initialBlockingCallbacks" value="false" />
			</bean>
		</property>
		<property name="initialEnableBackground" value="0" />
		<property name="initialEnableFlatField" value="0" />
		<property name="initialEnableOffsetScale" value="0" />
		<property name="initialEnableHighClip" value="0" />
		<property name="initialEnableLowClip" value="0" />
		<property name="initialEnableFilter" value="0" />
	</bean>

	<bean id="mono2.roi1" class="gda.device.detector.areadetector.v17.impl.NDROIImpl">
		<!-- <property name="deviceName" value="mono2.roi1_NDROI" /> -->
		<property name="basePVName" value="BL12I-OP-DCM-01:MONO2:ROI1:" />
		<property name="pluginBase">
			<bean class="gda.device.detector.areadetector.v17.impl.NDPluginBaseImpl">
				<!-- <property name="deviceName" value="mono2.roi1_NDPluginBase" /> -->
				<property name="basePVName" value="BL12I-OP-DCM-01:MONO2:ROI1:" />
				<property name="initialArrayPort" value="mono2.cam" />
				<property name="initialArrayAddress" value="0" />
				<property name="initialEnableCallbacks" value="1" />
				<property name="initialBlockingCallbacks" value="false" />
			</bean>
		</property>
		<property name="initialMinX" value="1" />
		<property name="initialMinY" value="1" />
		<property name="initialBinX" value="1" />
		<property name="initialBinY" value="1" />
	</bean>

	<bean id="mono2.roi2" class="gda.device.detector.areadetector.v17.impl.NDROIImpl">
		<!-- <property name="deviceName" value="mono2.roi1_NDROI" /> -->
		<property name="basePVName" value="BL12I-OP-DCM-01:MONO2:ROI2:" />
		<property name="pluginBase">
			<bean class="gda.device.detector.areadetector.v17.impl.NDPluginBaseImpl">
				<!-- <property name="deviceName" value="mono2.roi1_NDPluginBase" /> -->
				<property name="basePVName" value="BL12I-OP-DCM-01:MONO2:ROI2:" />
				<property name="initialArrayPort" value="mono2.cam" />
				<property name="initialArrayAddress" value="0" />
				<property name="initialEnableCallbacks" value="1" />
				<property name="initialBlockingCallbacks" value="false" />
			</bean>
		</property>
		<property name="initialMinX" value="1" />
		<property name="initialMinY" value="1" />
		<property name="initialBinX" value="1" />
		<property name="initialBinY" value="1" />
	</bean>

	<bean id="mono2.arr" class="gda.device.detector.areadetector.v17.impl.NDArrayImpl">
		<!-- <property name="deviceName" value="mono2.arr_NDStdArrays" /> -->
		<property name="basePVName" value="BL12I-OP-DCM-02:MONO2:ARR:" />
		<property name="pluginBase">
			<bean class="gda.device.detector.areadetector.v17.impl.NDPluginBaseImpl">
				<!-- <property name="deviceName" value="mono2.arr_NDPluginBase" /> -->
				<property name="basePVName" value="BL12I-OP-DCM-02:MONO2:ARR:" />
				<property name="initialArrayPort" value="mono2.prc1" />
				<property name="initialArrayAddress" value="0" />
				<property name="initialEnableCallbacks" value="1" />
				<property name="initialBlockingCallbacks" value="false" />
			</bean>
		</property>
	</bean>

	<bean id="mono2.stat" class="gda.device.detector.areadetector.v17.impl.NDStatsImpl">
		<!-- <property name="deviceName" value="mono2.stat_NDStats" /> -->
		<property name="basePVName" value="BL12I-OP-DCM-01:MONO2:STAT:" />
		<property name="pluginBase">
			<bean class="gda.device.detector.areadetector.v17.impl.NDPluginBaseImpl">
				<property name="basePVName" value="BL12I-OP-DCM-01:MONO2:STAT:" />
				<!-- <property name="deviceName" value="mono2.stat_NDPluginBase" /> -->
				<property name="initialArrayPort" value="mono2.roi" />
				<property name="initialArrayAddress" value="0" />
				<property name="initialEnableCallbacks" value="1" />
				<property name="initialBlockingCallbacks" value="false" />
			</bean>
		</property>
	</bean>

	<bean id="mono2.draw" class="gda.device.detector.areadetector.v17.impl.NDOverlayImpl">
		<!-- <property name="deviceName" value="mono2.draw_NDOverlay" /> -->
		<property name="basePVName" value="BL12I-OP-DCM-01:MONO2:DRAW:" />
		<property name="pluginBase">
			<bean class="gda.device.detector.areadetector.v17.impl.NDPluginBaseImpl">
				<property name="basePVName" value="BL12I-OP-DCM-01:MONO2:DRAW:" />
				<!-- <property name="deviceName" value="mono2.draw_NDPluginBase" /> -->
				<!-- <property name="initialArrayPort" value="mono2.cam" /> -->
				<property name="initialArrayPort" value="mono2.cam" />
				<property name="initialArrayAddress" value="0" />
				<property name="initialEnableCallbacks" value="1" />
				<property name="initialBlockingCallbacks" value="false" />
			</bean>
		</property>
	</bean>

	<bean id="mono2.tif" class="gda.device.detector.areadetector.v17.impl.NDFileImpl">
		<!-- <property name="deviceName" value="mono2.tif_NDFile" /> -->
		<property name="basePVName" value="BL12I-OP-DCM-01:MONO2:TIFF:" />
		<property name="pluginBase">
			<bean class="gda.device.detector.areadetector.v17.impl.NDPluginBaseImpl">
				<property name="basePVName" value="BL12I-OP-DCM-01:MONO2:TIFF:" />
				<!-- <property name="deviceName" value="mono2.tif_NDPluginBase" /> -->
				<property name="initialArrayPort" value="mono2.cam" />
				<property name="initialArrayAddress" value="0" />
				<property name="initialEnableCallbacks" value="1" />
				<property name="initialBlockingCallbacks" value="false" />
			</bean>
		</property>
		<property name="initialAutoIncrement" value="Yes" />
		<property name="initialAutoSave" value="Yes" />
		<property name="initialWriteMode" value="2" />
		<property name="initialNumCapture" value="1" />
		<property name="initialFileName" value="p" />
		<property name="initialFileTemplate" value="%s/%s_%05d.tif" />
	</bean>

	<bean id="mono2.hdf"
		class="gda.device.detector.areadetector.v17.impl.NDFileHDF5Impl">
		<!-- <property name="deviceName" value="mono2.hdf_NDFileHDF5" /> -->
		<property name="basePVName" value="BL12I-OP-DCM-01:MONO2:HDF:" />
		<property name="file">
			<bean class="gda.device.detector.areadetector.v17.impl.NDFileImpl">
				<!-- <property name="deviceName" value="mono2.hdf_NDFile" /> -->
				<property name="basePVName" value="BL12I-OP-DCM-01:MONO2:HDF:" />
				<property name="pluginBase">
					<bean class="gda.device.detector.areadetector.v17.impl.NDPluginBaseImpl">
						<!-- <property name="deviceName" value="mono2.hdf_NDPluginBase" /> -->
						<property name="basePVName" value="BL12I-OP-DCM-01:MONO2:HDF:" />
						<property name="initialArrayPort" value="mono2.cam" />
						<property name="initialArrayAddress" value="0" />
						<property name="initialEnableCallbacks" value="1" />
						<property name="initialBlockingCallbacks" value="false" />
					</bean>
				</property>
				<property name="initialAutoSave" value="Yes" />
				<property name="initialAutoIncrement" value="Yes" />
				<property name="initialWriteMode" value="2" />
				<property name="initialNumCapture" value="1" />
				<property name="initialFileName" value="projections" />
				<property name="initialFileTemplate" value="%s/%s.h5" />
			</bean>
		</property>
		<property name="initialNumExtraDims" value="0" />
		<property name="initialExtraDimSizeN" value="1" />
		<property name="initialExtraDimSizeX" value="0" />
		<property name="initialExtraDimSizeY" value="0" />
		<property name="initialNumRowChunks" value="2672" />
		<property name="initialCompression" value="None" />
		<property name="initialNumBitOffset" value="0" />
		<property name="initialNumBitPrecision" value="8" />
		<property name="initialSzipNumPixels" value="16" />
		<property name="initialZCompressLevel" value="6" />
	</bean>

	<bean id="mono2.mjpeg1"
		class="gda.device.detector.areadetector.v17.impl.FfmpegStreamImpl">
		<!-- <property name="deviceName" value="mono2.mpeg_ffmpegStream" /> -->
		<property name="basePVName" value="BL12I-OP-DCM-01:MONO2:MPEG1:" />
		<property name="pluginBase">
			<bean class="gda.device.detector.areadetector.v17.impl.NDPluginBaseImpl">
				<property name="basePVName" value="BL12I-OP-DCM-01:MONO2:MPEG1:" />
				<!-- <property name="deviceName" value="mono2.mpeg_NDPluginBase" /> -->
				<property name="initialArrayPort" value="mono2.roi1" />
				<property name="initialArrayAddress" value="0" />
				<property name="initialEnableCallbacks" value="1" />
				<property name="initialBlockingCallbacks" value="false" />
			</bean>
		</property>
		<property name="initialQuality" value="100" />
		<property name="initialFalseColor" value="0" />
		<property name="initialAlwaysOn" value="0" />
	</bean>

	<bean id="mono2.mjpeg2"
		class="gda.device.detector.areadetector.v17.impl.FfmpegStreamImpl">
		<!-- <property name="deviceName" value="mono2.mpeg_ffmpegStream" /> -->
		<property name="basePVName" value="BL12I-OP-DCM-01:MONO2:MPEG2:" />
		<property name="pluginBase">
			<bean class="gda.device.detector.areadetector.v17.impl.NDPluginBaseImpl">
				<property name="basePVName" value="BL12I-OP-DCM-01:MONO2:MPEG2:" />
				<!-- <property name="deviceName" value="mono2.mpeg_NDPluginBase" /> -->
				<property name="initialArrayPort" value="mono2.roi1" />
				<property name="initialArrayAddress" value="0" />
				<property name="initialEnableCallbacks" value="1" />
				<property name="initialBlockingCallbacks" value="false" />
			</bean>
		</property>
		<property name="initialQuality" value="100" />
		<property name="initialFalseColor" value="0" />
		<property name="initialAlwaysOn" value="0" />
	</bean>

	<bean id="camMono2" class="gda.device.detector.AdDetectorExtRoiDraw">
		<property name="adBase" ref="mono2.cam" />
		<property name="ndStats" ref="mono2.stat" />
		<property name="ndArray" ref="mono2.arr" />
		<property name="ndFile" ref="mono2.tif" />
		<property name="ndRoi1" ref="mono2.roi1" />
		<property name="ndRoi2" ref="mono2.roi2" />
		<property name="ndDraw" ref="mono2.draw" />
		<property name="mjpeg1" ref="mono2.mjpeg1" />
		<property name="mjpeg2" ref="mono2.mjpeg2" />
		<property name="ndProc1" ref="mono2.proc1" />
		<property name="ndProc2" ref="mono2.proc2" />
		<property name="computeCentroid" value="true" />
		<property name="computeStats" value="true" />
	</bean>
</beans>
