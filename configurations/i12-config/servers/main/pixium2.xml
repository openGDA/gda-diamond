		<property name="pluginBase">
			<bean class="gda.device.detector.areadetector.v17.impl.NDPluginBaseImpl">
				<property name="pvProvider" ref="pixium_proc1_pvProvider" />
			</bean>
		</property>
	</bean>


	<bean id="pixium_multipleExposurePco"
		class="gda.device.detector.addetector.triggering.MultipleExposureSoftwareTriggerAutoMode">
		<constructor-arg ref="pixium_cam_base" />
		<constructor-arg value="10.0" />
		<property name="ndProcess" ref="pixium_proc1" />
		<property name="exposureTime" value="0.5" />
		<property name="readAcquireTimeFromHardware" value="true" />
		<property name="ndFile" ref="pixium_ndfiletif" />
		<property name="procDataTypeOut" value="6"/><!-- float32 -->
	</bean>
	<bean id="pixiumSingleImageFileWriter"
		class="gda.device.detector.addetector.filewriter.SingleImagePerFileWriter">
		<!--<constructor-arg value="pixium"/> -->
		<property name="fileTemplate" value="%s%s%05d.tif" />
		<property name="filePathTemplate" value="$datadir$/$scan$/" />
		<property name="fileNameTemplate" value="pixi_" />
		<property name="fileNumberAtScanStart" value="1" />
		<property name="ndFile" ref="pixium_ndfiletif" />
		<property name="ndArrayPortVal" value="pix.proc" />
		<property name="fileWriteMode" value="STREAM" />
	</bean>

	<bean id="pixium_tif" class="gda.device.detector.addetector.ADDetector">
		<property name="adBase" ref="pixium_cam_base" />
		<property name="ndStats" ref="pixium_stat" />
		<property name="ndArray" ref="pixium_arr" />
		<property name="ndFile" ref="pixium_ndfiletif" />
		<property name="collectionStrategy" ref="pixium_multipleExposurePco" />
		<property name="fileWriter" ref="pixiumSingleImageFileWriter"/>

		<property name="readAcquisitionTime" value="true" />
		<property name="readFilepath" value="true" />
		<property name="readAcquisitionPeriod" value="false" />
		<property name="computeStats" value="false" />
		<property name="computeCentroid" value="false" />
		<property name="readArray" value="false" />
		<property name="checkFileExists" value="true" />
		<property name="usePipeline" value="true" />
	</bean>
	
	<bean id="pixium_tif_nx" class ="gda.device.detector.NXDetector">
	<property name="name" value="pixium_tif_nx"/>
	<property name="collectionStrategy" ref="pixium_multipleExposurePco"/>
	<property name="additionalPluginList">
	<list>
		<ref bean="pixiumSingleImageFileWriter"/>
	</list>
	</property>
	</bean>


