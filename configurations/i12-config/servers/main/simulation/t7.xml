<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<!-- Define all the motors -->
	<bean id="t7_x_motor" class="gda.spring.EpicsMotorFactoryBean">
		<property name="deviceName" value="t7.x" />
	</bean>

	<bean id="t7_y_motor" class="gda.spring.EpicsMotorFactoryBean">
		<property name="deviceName" value="t7.y" />
	</bean>

	<bean id="t7_m1z_motor" class="gda.spring.EpicsMotorFactoryBean">
		<property name="deviceName" value="t7.m1z" />
	</bean>
	<bean id="t7_m2x_motor" class="gda.spring.EpicsMotorFactoryBean">
		<property name="deviceName" value="t7.m2x" />
	</bean>

	<bean id="t7_m2y_motor" class="gda.spring.EpicsMotorFactoryBean">
		<property name="deviceName" value="t7.m2y" />
	</bean>

	<bean id="t7_m2ry_motor" class="gda.spring.EpicsMotorFactoryBean">
		<property name="deviceName" value="t7.m2ry" />
	</bean>
	<bean id="t7_m2rx_motor" class="gda.spring.EpicsMotorFactoryBean">
		<property name="deviceName" value="t7.m2rx" />
	</bean>

	<bean id="t7_m2z_motor" class="gda.spring.EpicsMotorFactoryBean">
		<property name="deviceName" value="t7.m2z" />
	</bean>

	<bean id="t7_m3z_motor" class="gda.spring.EpicsMotorFactoryBean">
		<property name="deviceName" value="t7.m3z" />
	</bean>
	<bean id="t7_m3rz_motor" class="gda.spring.EpicsMotorFactoryBean">
		<property name="deviceName" value="t7.m3rz" />
	</bean>

	<bean id="t7_m3rx_motor" class="gda.spring.EpicsMotorFactoryBean">
		<property name="deviceName" value="t7.m3rx" />
	</bean>


</beans>
