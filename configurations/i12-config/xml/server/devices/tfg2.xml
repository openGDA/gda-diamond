<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="DAServerI" class="gda.device.detector.DAServer">
		<property name="name" value="DAServerI" />
		<property name="local" value="true" />
		<property name="host" value="i12-tfg2-10ge" />
		<property name="port" value="1972" />
		<property name="startupCommands">
			<list>
				<value>tfg setup-cc-mode scaler64</value>
				<value>tfg setup-cc-chan -1 edge</value>
			</list>
		</property>
		<property name="dataPort" value="20030" />
	</bean>


	<bean id="tfgi" class="gda.device.timer.Etfg">
		<property name="name" value="tfgi" />
		<property name="daServerName" value="DAServerI" />
	</bean>

</beans>