<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
	
	<import resource="devices/scaler1.xml"/>
	<import resource="live/Ioeh2.xml"/>
	<bean id="I0eh2" class="gda.device.detector.etldetector.ETLDetector">
		<property name="scaler" ref="scaler1"/>
		<property name="detector" ref="Ioeh2"/>
		<property name="scalerChannelIndex" value="18"/>
	</bean>

</beans>
