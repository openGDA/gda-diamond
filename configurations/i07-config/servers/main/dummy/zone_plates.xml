	</bean>

	<bean id="OSA_Y" class="gda.device.scannable.DummyScannable">
		<property name="name" value="OSA_Y" />
		<property name="useNameAsInputName" value="True" />
	</bean>

	<bean id="OSA_Z" class="gda.device.scannable.DummyScannable">
