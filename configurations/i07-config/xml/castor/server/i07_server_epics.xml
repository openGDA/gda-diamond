    <!--  ============================== Epics Eurotherm Devices ============== -->
    <!-- Controller 1 -->
	<EpicsEurotherm2kController>
		<name>etcontroller1</name>
		<local>true</local>
		<configureAtStartup>true</configureAtStartup>
		<deviceName>EURO1</deviceName>
		<maxRampRate>10</maxRampRate>
		<minRampRate>0.01</minRampRate>
		<maxOutput>100.0</maxOutput>
	</EpicsEurotherm2kController>

	<Eurotherm2K>
		<name>temp1</name>
		<configureAtStartup>true</configureAtStartup>
		<accuracy>5</accuracy>
		<polltime>1000</polltime>
		<fileSuffix>temp</fileSuffix>
		<upperTemp>1400</upperTemp>
		<lowerTemp>0</lowerTemp>
<!--		<EpicsEurotherm2kControllerName>etcontroller1</EpicsEurotherm2kControllerName>-->
	    <controllerName>etcontroller1</controllerName>
	</Eurotherm2K>

	<!-- Controller 2 -->
	<EpicsEurotherm2kController>
		<name>etcontroller2</name>
		<local>true</local>
		<configureAtStartup>true</configureAtStartup>
		<deviceName>EURO2</deviceName>
		<maxRampRate>10</maxRampRate>
		<minRampRate>0.01</minRampRate>
		<maxOutput>100.0</maxOutput>
	</EpicsEurotherm2kController>

	<Eurotherm2K>
		<name>temp2</name>
		<configureAtStartup>true</configureAtStartup>
		<accuracy>5</accuracy>
		<polltime>1000</polltime>
		<fileSuffix>temp</fileSuffix>
		<upperTemp>1400</upperTemp>
		<lowerTemp>0</lowerTemp>
<!--		<EpicsEurotherm2kControllerName>etcontroller2</EpicsEurotherm2kControllerName>-->
