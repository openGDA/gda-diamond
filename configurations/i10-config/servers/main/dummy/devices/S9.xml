<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<!-- Tertiary Slits / S9 Optical Element -->

	<bean id="s9xring_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-2.5" />
		<property name="maxPosition" value="18.0" />
		<property name="speed" value="1.0" />
		<property name="unitString" value="mm"/>
	</bean>
	<bean id="s9xhall_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-17.0" />
		<property name="maxPosition" value="1.0" />
		<property name="speed" value="1.0" />
		<property name="unitString" value="mm"/>
	</bean>
	<bean id="s9yplus_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-16.0" />
		<property name="maxPosition" value="2.5" />
		<property name="speed" value="1.0" />
		<property name="unitString" value="mm"/>
	</bean>
	<bean id="s9yminus_motor" class="gda.device.motor.DummyMotor">
		<property name="minPosition" value="-2.5" />
		<property name="maxPosition" value="17.5" />
		<property name="speed" value="1.0" />
		<property name="unitString" value="mm"/>
	</bean>
</beans>