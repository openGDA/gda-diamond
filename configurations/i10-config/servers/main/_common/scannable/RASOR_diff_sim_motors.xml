<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">     
 	
 	<!--Rasor-->
	<bean id="simtth" class="gda.device.scannable.ScannableMotor">
		<property name="name" value="simtth" />
		<property name="initialUserUnits" value="deg" />
		<property name="motor" ref="diff_sim_twotheta_motor" />
		<property name="hardwareUnitString" value="deg" />
	</bean>

	<bean id="simth" class="gda.device.scannable.ScannableMotor">
		<property name="name" value="simth" />
		<property name="initialUserUnits" value="deg" />
		<property name="motor" ref="diff_sim_theta_motor" />
		<property name="hardwareUnitString" value="deg" />
	</bean>
	
	<bean id="simchi" class="gda.device.scannable.ScannableMotor">
		<property name="name" value="simchi" />
		<property name="initialUserUnits" value="deg" />
		<property name="motor" ref="diff_sim_chi_motor" />
		<property name="hardwareUnitString" value="deg" />
	</bean>
	
	<bean id="simalpha" class="gda.device.scannable.ScannableMotor">
		<property name="name" value="simalpha" />
		<property name="initialUserUnits" value="deg" />
		<property name="motor" ref="diff_sim_alpha_motor" />
		<property name="hardwareUnitString" value="deg" />
	</bean>
</beans>
