	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- CA3 Epics Monitor -->
	
	<bean id="CA3" class="gda.device.scannable.scannablegroup.ScannableGroup">
		<property name="groupMembers">
			<list>
				<ref bean="ca31" />
				<ref bean="ca32" />
				<ref bean="ca33" />
				<ref bean="ca34" />
			</list>
		</property>
	</bean>
</beans>