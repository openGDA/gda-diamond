<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">     
	
	<!-- DJ1 (D9) -->
	
	<bean id="dj1_scrn" class="gda.device.scannable.ScannableMotor">
		<property name="initialUserUnits" value="mm" />
		<property name="motor" ref="dj1d9_scrn_motor" />
		<property name="hardwareUnitString" value="mm" />
		<!-- 
		<property name="lowerGdaLimits" value="-100" />
		<property name="upperGdaLimits" value="100" />
		 -->
	</bean>
 
 	<bean id="dj1_det" class="gda.device.scannable.ScannableMotor">
		<property name="initialUserUnits" value="mm" />
		<property name="motor" ref="dj1d9_det_motor" />
		<property name="hardwareUnitString" value="mm" />
		<!-- 
		<property name="lowerGdaLimits" value="-100" />
		<property name="upperGdaLimits" value="100" />
		 -->
	</bean>
	
	<bean id="DJ1" class="gda.device.scannable.scannablegroup.ScannableGroup">
		<property name="groupMembers">
			<list>
				<ref bean="dj1_scrn" />
				<ref bean="dj1_det" />
			</list>
		</property>
		<property name="local" value="true"/>
	</bean>
</beans>
