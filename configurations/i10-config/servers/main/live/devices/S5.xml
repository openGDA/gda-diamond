<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- Tertiary Slits / S5 Optical Element -->

	<bean id="s5xring_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL10I-AL-SLITS-05:XRING" />
	</bean>

	<bean id="s5xhall_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL10I-AL-SLITS-05:XHALL" />
	</bean>

	<bean id="s5yplus_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL10I-AL-SLITS-05:YPLUS" />
	</bean>

	<bean id="s5yminus_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL10I-AL-SLITS-05:YMINUS" />
	</bean>

	<bean id="s5xsize_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL10I-AL-SLITS-05:XSIZE" />
	</bean>

	<bean id="s5xcentre_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL10I-AL-SLITS-05:XCENTRE" />
	</bean>

	<bean id="s5ysize_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL10I-AL-SLITS-05:YSIZE" />
	</bean>

	<bean id="s5ycentre_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL10I-AL-SLITS-05:YCENTRE" />
	</bean>
</beans>