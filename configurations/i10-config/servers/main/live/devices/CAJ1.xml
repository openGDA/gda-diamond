<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<!-- CAJ1 Epics Monitor -->
	
	<bean id="caj11" class="gda.device.monitor.EpicsMonitor">
		<property name="pvName" value="BL10J-DI-IAMP-01:SIG1" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="caj12" class="gda.device.monitor.EpicsMonitor">
		<property name="pvName" value="BL10J-DI-IAMP-01:SIG2" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="caj13" class="gda.device.monitor.EpicsMonitor">
		<property name="pvName" value="BL10J-DI-IAMP-01:SIG3" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="caj14" class="gda.device.monitor.EpicsMonitor">
		<property name="pvName" value="BL10J-DI-IAMP-01:SIG4" />
		<property name="local" value="true"/>
	</bean>

</beans>