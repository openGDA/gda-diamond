<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<!-- Tertiary Slits / S9 Optical Element -->
	
	<bean id="s9xring_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="SLITS9.XRING" />
		<property name="epicsConfiguration" ref="epicsConfig" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="s9xhall_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="SLITS9.XHALL" />
		<property name="epicsConfiguration" ref="epicsConfig" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="s9yplus_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="SLITS9.YPLUS" />
		<property name="epicsConfiguration" ref="epicsConfig" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="s9yminus_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="SLITS9.YMINUS" />
		<property name="epicsConfiguration" ref="epicsConfig" />
		<property name="local" value="true"/>
	</bean>
</beans>