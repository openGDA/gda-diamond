<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<!-- Tertiary Slits / S5 Optical Element -->

  	<bean id="s5xring_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="SLITS5.XRING" />
		<property name="epicsConfiguration" ref="epicsConfig" />
		<property name="local" value="true"/>
	</bean>
  	
  	<bean id="s5xhall_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="SLITS5.XHALL" />
		<property name="epicsConfiguration" ref="epicsConfig" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="s5yplus_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="SLITS5.YPLUS" />
		<property name="epicsConfiguration" ref="epicsConfig" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="s5yminus_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="SLITS5.YMINUS" />
		<property name="epicsConfiguration" ref="epicsConfig" />
		<property name="local" value="true"/>
	</bean>

	<bean id="s5xsize_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="SLITS5.XGAPSIZE" />
		<property name="epicsConfiguration" ref="epicsConfig" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="s5xcentre_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="SLITS5.XGAPCENTRE" />
		<property name="epicsConfiguration" ref="epicsConfig" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="s5ysize_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="SLITS5.YGAPSIZE" />
		<property name="epicsConfiguration" ref="epicsConfig" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="s5ycentre_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="SLITS5.YGAPCENTRE" />
		<property name="epicsConfiguration" ref="epicsConfig" />
		<property name="local" value="true"/>
	</bean>
</beans>