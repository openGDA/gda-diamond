<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<!-- UMJ1 (U2) User Motors -->

	<bean id="user2_axis1_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="UJ1.motor1" />
		<property name="epicsConfiguration" ref="epicsConfig" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="user2_axis2_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="UJ1.motor2" />
		<property name="epicsConfiguration" ref="epicsConfig" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="user2_axis3_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="UJ1.motor3" />
		<property name="epicsConfiguration" ref="epicsConfig" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="user2_axis4_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="UJ1.motor4" />
		<property name="epicsConfiguration" ref="epicsConfig" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="user2_axis5_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="UJ1.motor5" />
		<property name="epicsConfiguration" ref="epicsConfig" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="user2_axis6_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="UJ1.motor6" />
		<property name="epicsConfiguration" ref="epicsConfig" />
		<property name="local" value="true"/>
	</bean>
</beans>