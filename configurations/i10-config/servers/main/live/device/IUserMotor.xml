<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<!-- UMI1 (U1) User Motors --> 

    <bean id="user1_axis1_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="UI1.motor1" />
		<property name="epicsConfiguration" ref="epicsConfig" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="user1_axis2_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="UI1.motor2" />
		<property name="epicsConfiguration" ref="epicsConfig" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="user1_axis3_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="UI1.motor3" />
		<property name="epicsConfiguration" ref="epicsConfig" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="user1_axis4_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="UI1.motor4" />
		<property name="epicsConfiguration" ref="epicsConfig" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="user1_axis5_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="UI1.motor5" />
		<property name="epicsConfiguration" ref="epicsConfig" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="user1_axis6_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="UI1.motor6" />
		<property name="epicsConfiguration" ref="epicsConfig" />
		<property name="local" value="true"/>
	</bean>
</beans>