<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<!-- ======= Access control for Scattering/Absorption branches ======= -->
	<!-- I10 Scattering (SHTRI) -->
	<bean id="branchena" class="gda.epics.AccessControl">
		<property name="accessControlPvName" value="BL10I-OP-BEAM-01:CONTROLLER" />
		<property name="enableValue" value="0" />
		<property name="disableValue" value="1" />
	</bean>
</beans>