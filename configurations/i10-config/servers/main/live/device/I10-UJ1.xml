<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">     

	<!-- UJ1 User Input/Output --> 
	
	<bean id="scalerj1" class="gda.device.detector.EpicsScaler">
		<property name="deviceName" value="SCALERJ1" />
		<property name="frequency" value="50000000"/>
	</bean>

	<bean id="ij1ai1" class="gda.device.monitor.EpicsMonitor">
		<property name="deviceName" value="UJ1.AI1" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="ij1ai2" class="gda.device.monitor.EpicsMonitor">
		<property name="deviceName" value="UJ1.AI2" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="ij1ai3" class="gda.device.monitor.EpicsMonitor">
		<property name="deviceName" value="UJ1.AI3" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="ij1ai4" class="gda.device.monitor.EpicsMonitor">
		<property name="deviceName" value="UJ1.AI4" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="ij1ai5" class="gda.device.monitor.EpicsMonitor">
		<property name="deviceName" value="UJ1.AI5" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="ij1ai6" class="gda.device.monitor.EpicsMonitor">
		<property name="deviceName" value="UJ1.AI6" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="ij1ai7" class="gda.device.monitor.EpicsMonitor">
		<property name="deviceName" value="UJ1.AI7" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="ij1ai8" class="gda.device.monitor.EpicsMonitor">
		<property name="deviceName" value="UJ1.AI8" />
		<property name="local" value="true"/>
	</bean>

	<bean id="ij1ao1" class="gda.device.controlpoint.EpicsControlPoint">
		<property name="deviceName" value="UJ1.AO1" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="ij1ao2" class="gda.device.controlpoint.EpicsControlPoint">
		<property name="deviceName" value="UJ1.AO2" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="ij1ao3" class="gda.device.controlpoint.EpicsControlPoint">
		<property name="deviceName" value="UJ1.AO3" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="ij1ao4" class="gda.device.controlpoint.EpicsControlPoint">
		<property name="deviceName" value="UJ1.AO4" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="ij1ao5" class="gda.device.controlpoint.EpicsControlPoint">
		<property name="deviceName" value="UJ1.AO5" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="ij1ao6" class="gda.device.controlpoint.EpicsControlPoint">
		<property name="deviceName" value="UJ1.AO6" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="ij1ao7" class="gda.device.controlpoint.EpicsControlPoint">
		<property name="deviceName" value="UJ1.AO7" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="ij1ao8" class="gda.device.controlpoint.EpicsControlPoint">
		<property name="deviceName" value="UJ1.AO8" />
		<property name="local" value="true"/>
	</bean>
 </beans>
