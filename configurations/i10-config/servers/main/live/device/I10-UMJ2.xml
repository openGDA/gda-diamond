<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<!-- UMJ2 (U3) User Motors -->

	<bean id="user3_axis1_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="UJ2.motor1" />
		<property name="epicsConfiguration" ref="epicsConfig" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="user3_axis2_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="UJ2.motor2" />
		<property name="epicsConfiguration" ref="epicsConfig" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="user3_axis3_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="UJ2.motor3" />
		<property name="epicsConfiguration" ref="epicsConfig" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="user3_axis4_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="UJ2.motor4" />
		<property name="epicsConfiguration" ref="epicsConfig" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="user3_axis5_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="UJ2.motor5" />
		<property name="epicsConfiguration" ref="epicsConfig" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="user3_axis6_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="UJ2.motor6" />
		<property name="epicsConfiguration" ref="epicsConfig" />
		<property name="local" value="true"/>
	</bean>
</beans>