<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<!-- CA4 Epics Monitor -->
	
	<bean id="ca41" class="gda.device.monitor.EpicsMonitor">
		<property name="deviceName" value="CA4.I1" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="ca42" class="gda.device.monitor.EpicsMonitor">
		<property name="deviceName" value="CA4.I2" />
		<property name="local" value="true"/>
	</bean>

	<bean id="ca43" class="gda.device.monitor.EpicsMonitor">
		<property name="deviceName" value="CA4.I3" />
		<property name="local" value="true"/>
	</bean>
	
	<bean id="ca44" class="gda.device.monitor.EpicsMonitor">
		<property name="deviceName" value="CA4.I4" />
		<property name="local" value="true"/>
	</bean>

	<!-- <bean id="ca4_ctrl" class="gda.device.enumpositioner.EpicsQuadraCurrentAmplifier">
		<property name="deviceName" value="CA4.CTRL"/>
	</bean> -->
</beans>