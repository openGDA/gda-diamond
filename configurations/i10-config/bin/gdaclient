#!/bin/bash
export GTK_OVERLAY_SCROLLING=0
if [ -z $GDA_LOG ] ; then
	LOGPANEL=$(ps -ef | grep gdalogpanel | grep -v grep)
	if [ "X$LOGPANEL" == "X" ] ; then
		nohup gdalog 2>/dev/null &
	else
		wmctrl -R "GDA Log Panel"
	fi
fi

export SPRING_PROFILES=ascattering,windows7,bsorption,em,hfm
gda --start client --springprofiles $SPRING_PROFILES "$@"