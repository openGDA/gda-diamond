			<property name="scannableName" value="thp" />
			<property name="increment" value="1" />
		</bean>
	
		<bean id="ttp_control" class="uk.ac.gda.client.livecontrol.ScannablePositionerControl">
			<property name="displayName" value="ttp" />
			<property name="group" value="RASOR POLAN" />
			<property name="scannableName" value="ttp" />
			<property name="increment" value="1" />
		</bean>
		
		<bean id="py_control" class="uk.ac.gda.client.livecontrol.ScannablePositionerControl">
			<property name="displayName" value="py" />
