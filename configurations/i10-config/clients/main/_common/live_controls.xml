			<property name="scannableName" value="dsd" />
			<property name="increment" value="1" />
		</bean>
	
		<bean id="dsu_control" class="uk.ac.gda.client.livecontrol.ScannablePositionerControl">
			<property name="displayName" value="dsu" />
			<property name="group" value="Detector Aperture" />
			<property name="scannableName" value="dsu" />
			<property name="increment" value="1" />
		</bean>
	
		<bean name="controlSet" class="uk.ac.gda.client.livecontrol.ControlSet">
			<property name="controls">
				<list>
					<ref bean="leg1_control" />
					<ref bean="leg2_control" />
					<ref bean="leg3_control" />
					<ref bean="sx_control" />
					<ref bean="sy_control" />
					<ref bean="sz_control" />
					<ref bean="tth_control" />
					<ref bean="th_control" />
					<ref bean="chi_control" />
					<ref bean="alpha_control" />
					<ref bean="difx_control" />
					<ref bean="pinhx_control" />
					<ref bean="pinhy_control" />
					<ref bean="emecy1_control" />
					<ref bean="emecy2_control" />
					<ref bean="emecpitch_control" />
					<ref bean="eta_control" />
					<ref bean="thp_control" />
					<ref bean="ttp_control" />
					<ref bean="py_control" />
					<ref bean="pz_control" />
					<ref bean="dsd_control" />
					<ref bean="dsu_control" />
				</list>
			</property>
		</bean>

		<!-- PIMTE Controls -->
		<bean id="pimte_expsure_time_control" class="uk.ac.gda.client.livecontrol.ScannableValueControl">
			<property name="displayName" value="Exposure" />
			<property name="group" value="PIMTE Controls" />
			<property name="scannableName" value="pimteExposureTime" />
			<property name="textWidth" value="35"/>
			<property name="userUnits" value="s" />
		</bean>
		<bean id="pimte_acquire_period_control" class="uk.ac.gda.client.livecontrol.ScannableValueControl">
			<property name="displayName" value="Period" />
			<property name="group" value="PIMTE Controls" />
			<property name="scannableName" value="pimteAcquirePeriod" />
			<property name="textWidth" value="35" />
		</bean>
		<bean id="pimte_proc_num_filter_control" class="uk.ac.gda.client.livecontrol.ScannableValueControl">
			<property name="displayName" value="PROC NumFilter" />
			<property name="group" value="PIMTE PROC" />
			<property name="scannableName" value="pimteProcNumFilter" />
			<property name="textWidth" value="30" />
			<property name="userUnits" value=""/>
		</bean>
		<bean id="pimte_cover_control" class="uk.ac.gda.client.livecontrol.ScannablePositionerControl">
			<property name="displayName" value="Cover" />
			<property name="scannableName" value="pimtecover" />
			<property name="showStop" value="False" />
			<property name="group" value="PIMTE Controls" />
		</bean>
		<bean id="pimteCameraState" class="uk.ac.gda.client.live.stream.controls.camera.state.EpicsCameraState">
			<property name="cameraConfig" ref="pimte_cam"/>
			<property name="stateVariableName" value="i10.pimte.live.stream.active"/>
		</bean>
		<bean id="pimteControlWidgetFactory" class="uk.ac.gda.client.live.stream.view.LiveStreamViewWithHistogramFactory">
			<property name="customWidget">
				<bean class="uk.ac.gda.client.live.stream.controls.custom.widgets.CameraControlWidget">
					<property name="liveControls">
						<list>
							<ref bean="pimte_expsure_time_control" />
							<ref bean="pimte_acquire_period_control"/>
							<ref bean="pimte_cover_control"/>
							<ref bean="pimte_proc_num_filter_control" />
						</list>
					</property>
					<property name="cameraState" ref="pimteCameraState"/>
				</bean>
			</property>
		</bean>
		<!-- PIXIS Controls -->
		<bean id="pixis_expsure_time_control" class="uk.ac.gda.client.livecontrol.ScannableValueControl">
