		<bean id="pixisCameraState" class="uk.ac.gda.client.live.stream.controls.camera.state.EpicsCameraState">
			<property name="cameraConfig" ref="pixis_cam"/>
			<property name="stateVariableName" value="i10.pixis.live.stream.active"/>
		</bean>
		<bean id="pixisControlWidgetFactory" class="uk.ac.gda.client.live.stream.view.LiveStreamViewWithHistogramFactory">
			<property name="customWidget">
				<bean class="uk.ac.gda.client.live.stream.controls.custom.widgets.CameraControlWidget">
					<property name="liveControls">
						<list>
							<ref bean="pixis_expsure_time_control" />
							<ref bean="pixis_acquire_period_control"/>
							<ref bean="pixis_proc_num_filter_control" />
						</list>
