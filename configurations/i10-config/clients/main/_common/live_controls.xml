		</bean>
	
		<bean id="sz_control" class="uk.ac.gda.client.livecontrol.ScannablePositionerControl">
			<property name="displayName" value="sz" />
			<property name="group" value="RASOR CryoStat" />
			<property name="scannableName" value="sz" />
			<property name="increment" value="1" />
		</bean>
	
		<!-- RASOR Diffractometer -->
		<bean id="tth_control" class="uk.ac.gda.client.livecontrol.ScannablePositionerControl">
