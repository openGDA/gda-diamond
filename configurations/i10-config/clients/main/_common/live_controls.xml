		<bean id="chi_control" class="uk.ac.gda.client.livecontrol.ScannablePositionerControl">
			<property name="displayName" value="chi" />
			<property name="group" value="RASOR DIFF" />
			<property name="scannableName" value="chi" />
			<property name="increment" value="1" />
		</bean>
		
		<bean id="alpha_control" class="uk.ac.gda.client.livecontrol.ScannablePositionerControl">
			<property name="displayName" value="alpha" />
			<property name="group" value="RASOR DIFF" />
