	</bean>

	<!-- PIMTE detector control -->
	<bean id="pimteStart" class="uk.ac.gda.client.livecontrol.JythonCommandControl">
		<property name="jythonCommand" value="pimte_addetector.adBase.setImageModeWait(gda.device.detector.areadetector.v17.ImageMode.CONTINUOUS); pimte_addetector.collectData()" />
		<property name="buttonText" value="Start" />
		<property name="group" value="PIMTE" />
	</bean>

	<bean id="pimteStop" class="uk.ac.gda.client.livecontrol.JythonCommandControl">
		<property name="jythonCommand" value="pimte_addetector.adBase.stopAcquiring()" />
		<property name="buttonText" value="Stop" />
		<property name="group" value="PIMTE" />
	</bean>
	
	<!-- PIXIS detector control -->
	<bean id="pixisStart" class="uk.ac.gda.client.livecontrol.JythonCommandControl">
		<property name="jythonCommand" value="pixis_addetector.adBase.setImageModeWait(gda.device.detector.areadetector.v17.ImageMode.CONTINUOUS); pixis_addetector.collectData()" />
		<property name="buttonText" value="Start" />
		<property name="group" value="PIXIS" />
	</bean>

	<bean id="pixisStop" class="uk.ac.gda.client.livecontrol.JythonCommandControl">
		<property name="jythonCommand" value="pixis_addetector.adBase.stopAcquiring()" />
		<property name="buttonText" value="Stop" />
		<property name="group" value="PIXIS" />
	</bean>
	
