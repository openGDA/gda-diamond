	<bean name="controlSet" class="uk.ac.gda.client.livecontrol.ControlSet">
		<property name="controls">
			<list>
				<ref bean="leg1_control" />
				<ref bean="leg2_control" />
				<ref bean="leg3_control" />
				<ref bean="sx_control" />
				<ref bean="sy_control" />
				<ref bean="sz_control" />
				<ref bean="tth_control" />
				<ref bean="th_control" />
				<ref bean="chi_control" />
				<ref bean="alpha_control" />
				<ref bean="difx_control" />
				<ref bean="pinhx_control" />
				<ref bean="pinhy_control" />
				<ref bean="emecy1_control" />
				<ref bean="emecy2_control" />
				<ref bean="emecpitch_control" />
				<ref bean="eta_control" />
				<ref bean="thp_control" />
				<ref bean="ttp_control" />
				<ref bean="py_control" />
				<ref bean="pz_control" />
				<ref bean="dsd_control" />
