			<property name="displayName" value="pinhx" />
			<property name="group" value="RASOR Pin Hole" />
			<property name="scannableName" value="pinhx" />
			<property name="increment" value="1" />
		</bean>
	
		<bean id="pinhy_control" class="uk.ac.gda.client.livecontrol.ScannablePositionerControl">
			<property name="displayName" value="pinhy" />
			<property name="group" value="RASOR Pin Hole" />
			<property name="scannableName" value="pinhy" />
			<property name="increment" value="1" />
		</bean>
	
		<!-- RASOR REMEC -->
		<bean id="emecy1_control" class="uk.ac.gda.client.livecontrol.ScannablePositionerControl">
			<property name="displayName" value="emecy1" />
			<property name="group" value="RASOR REMEC" />
