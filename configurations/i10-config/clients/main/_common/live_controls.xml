			<property name="increment" value="1" />
		</bean>
	
		<bean id="emecpitch_control" class="uk.ac.gda.client.livecontrol.ScannablePositionerControl">
			<property name="displayName" value="emecpitch" />
			<property name="group" value="RASOR REMEC" />
			<property name="scannableName" value="emecpitch" />
			<property name="increment" value="1" />
		</bean>
	
		<!-- RASOR POLAN -->
		<bean id="eta_control" class="uk.ac.gda.client.livecontrol.ScannablePositionerControl">
