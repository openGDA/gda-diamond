<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="gda.rcp.util.OSGIServiceRegister">
		<property name="properties">
			<bean class="uk.ac.gda.util.dictionary.MapBasedDictionary">
				<property name="map">
					<map>
						<entry key="SERVICE_NAME" value="pimteSuffixService" />
					</map>
				</property>
			</bean>
		</property>
		<!-- Using pv//pimte//BL10I-EA-PIXIS-01@ in plugin.xml:
		 -->
		<property name="class" value="uk.ac.gda.epics.adviewer.ADPVSuffixes" />
		<!-- Note concrete class in service property vvv, but Interface ^^^ in class property -->
		<property name="service">
			<bean class="uk.ac.gda.epics.adviewer.SimpleADPVSuffixes">
				<property name="arraySuffix" value="IMAGE1:" />
				<property name="arrayROISuffix" value="ROI1:" />
				<property name="mpgSuffix" value="MJPG1:" />
				<property name="mpgProcSuffix" value="PROC1:" />
				<!-- 
				<property name="statSuffix" value="????:" />
				 -->
			</bean>
		</property>
	</bean>
</beans>
