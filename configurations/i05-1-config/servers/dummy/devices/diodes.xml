<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="dj7y_motor" class="gda.device.motor.DummyMotor">
		<property name="position" value="0.0"/>
	</bean>

	<bean id="dj7y_positioner" class="gda.device.enumpositioner.DummyEnumPositioner">
		<property name="positions">
			<list>
				<value>Diode Out</value>
				<value>Diode In</value>
			</list>
		</property>
	</bean>

	<bean id="dj9y_motor" class="gda.device.motor.DummyMotor">
		<property name="position" value="0.0"/>
	</bean>

	<bean id="dj9y_positioner" class="gda.device.enumpositioner.DummyEnumPositioner">
		<property name="positions">
			<list>
				<value>Diode Out</value>
				<value>Diode In</value>
			</list>
		</property>
	</bean>

</beans>