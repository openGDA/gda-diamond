<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="sj6_xcentre_motor" class="gda.device.motor.DummyMotor"/>

	<bean id="sj6_ycentre_motor" class="gda.device.motor.DummyMotor"/>

	<bean id="sj6_xsize_motor" class="gda.device.motor.DummyMotor"/>

	<bean id="sj6_ysize_motor" class="gda.device.motor.DummyMotor">
		<property name="position" value="0.4"/>
	</bean>

	<bean id="sj6_z_motor" class="gda.device.motor.DummyMotor"/>

</beans>
