<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="zpx_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL05J-EA-ZP-01:X" />
	</bean>

	<bean id="zpy_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL05J-EA-ZP-01:Y" />
	</bean>

	<bean id="zpz_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL05J-EA-ZP-01:Z" />
	</bean>

	<bean id="zpx_lut_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL05J-EA-ZP-01:LUT:X" />
	</bean>

	<bean id="zpy_lut_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL05J-EA-ZP-01:LUT:Y" />
	</bean>

	<bean id="zpz_lut_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL05J-EA-ZP-01:LUT:Z" />
	</bean>

</beans>