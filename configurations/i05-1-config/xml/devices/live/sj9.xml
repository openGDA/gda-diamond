<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="sj9_xcentre_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL05J-AL-SLITS-02:XCENTRE" />
		<property name="local" value="true" />
	</bean>

	<bean id="sj9_xgap_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL05J-AL-SLITS-02:XSIZE" />
		<property name="local" value="true" />
	</bean>

	<bean id="sj9_ycentre_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL05J-AL-SLITS-02:YCENTRE" />
		<property name="local" value="true" />
	</bean>

	<bean id="sj9_ygap_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL05J-AL-SLITS-02:YSIZE" />
		<property name="local" value="true" />
	</bean>

	<bean id="sj9_xring_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL05J-AL-SLITS-02:XRING" />
		<property name="local" value="true" />
	</bean>

	<bean id="sj9_xhall_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL05J-AL-SLITS-02:XHALL" />
		<property name="local" value="true" />
	</bean>

	<bean id="sj9_ytop_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL05J-AL-SLITS-02:YPLUS" />
		<property name="local" value="true" />
	</bean>

	<bean id="sj9_ybottom_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL05J-AL-SLITS-02:YMINUS" />
		<property name="local" value="true" />
	</bean>

</beans>
