<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="smx_motor" class="gda.device.motor.DummyMotor">
		<property name="local" value="true" />
	</bean>

	<bean id="smy_motor" class="gda.device.motor.DummyMotor">
		<property name="local" value="true" />
	</bean>

	<bean id="smz_motor" class="gda.device.motor.DummyMotor">
		<property name="local" value="true" />
	</bean>

	<bean id="smpolar_motor" class="gda.device.motor.DummyMotor">
		<property name="local" value="true" />
	</bean>

	<bean id="smazimuth_motor" class="gda.device.motor.DummyMotor">
		<property name="local" value="true" />
	</bean>
	
	<!-- The sample stage rotation angles -->
	<bean id="smalpha" class="gda.device.scannable.DummyUnitsScannable">
		<property name="userUnits" value="deg"/>
	</bean>
	
	<bean id="smbeta" class="gda.device.scannable.DummyUnitsScannable">
		<property name="userUnits" value="deg"/>
	</bean>
	
	<bean id="smgamma" class="gda.device.scannable.DummyUnitsScannable">
		<property name="userUnits" value="deg"/>
	</bean>

</beans>