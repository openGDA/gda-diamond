<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- add scannables name that will populate dashboard table-->
	<bean id="dashboard_motors" class="uk.ac.gda.rcp.dashboard.configuration.DashboardScannables">
		<property name="dashboardScannableNames">
			<list>
				<value>ring_current</value>
				<value>id</value>
				<value>energy</value>
				<value>exit_slit</value>
				<value>analyser_slit</value>
				<value>cryostat</value>
			</list>
		</property>
	</bean>
</beans>