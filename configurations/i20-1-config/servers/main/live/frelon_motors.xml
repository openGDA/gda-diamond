<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans   http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="frelon_focus_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL20J-EA-DET-02:FOCUS" />
	</bean>
	<bean id="frelon_iris_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL20J-EA-DET-02:IRIS" />
	</bean>
	<bean id="frelon_rot_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL20J-EA-DET-02:ROT" />
	</bean>

</beans>