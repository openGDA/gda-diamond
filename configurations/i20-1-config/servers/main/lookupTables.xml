<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans   http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">


	<bean id="equip1LookupTable" class="gda.util.converters.LookupTableConverterHolder">
		<constructor-arg value="equip1LookupTable" />
		<constructor-arg value="${gda.config}/lookupTables/energyToHardwareTable" />
		<constructor-arg value="1" />
		<constructor-arg value="0" />
		<constructor-arg value="Both" />
	</bean>

	<bean id="equip2LookupTable" class="gda.util.converters.LookupTableConverterHolder">
		<constructor-arg value="equip2LookupTable" />
		<constructor-arg value="${gda.config}/lookupTables/energyToHardwareTable" />
		<constructor-arg value="2" />
		<constructor-arg value="0" />
		<constructor-arg value="Both" />
	</bean>

	<bean id="energyTable" class="uk.ac.gda.beamline.i20_1.utils.LookupTablesBean">
		<property name="initialUserUnitString" value="eV" />
		<property name="lookupTables">
			<map>
				<entry key="equip1LookupTable" value-ref="equip1LookupTable" />
				<entry key="equip2LookupTable" value-ref="equip2LookupTable" />
			</map>
		</property>
	</bean>

</beans>