<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans   http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="um1" class="gda.device.scannable.ScannableMotor">
		<property name="protectionLevel" value="3" />
		<property name="initialUserUnits" value="mm" />
		<property name="motor" ref="um1_motor" />
		<property name="hardwareUnitString" value="mm" />
	</bean>
	<bean id="um2" class="gda.device.scannable.ScannableMotor">
		<property name="protectionLevel" value="3" />
		<property name="initialUserUnits" value="mm" />
		<property name="motor" ref="um2_motor" />
		<property name="hardwareUnitString" value="mm" />
	</bean>
	<bean id="um3" class="gda.device.scannable.ScannableMotor">
		<property name="protectionLevel" value="3" />
		<property name="initialUserUnits" value="mm" />
		<property name="motor" ref="um3_motor" />
		<property name="hardwareUnitString" value="mm" />
	</bean>
	<bean id="um6" class="gda.device.scannable.ScannableMotor">
		<property name="protectionLevel" value="3" />
		<property name="initialUserUnits" value="mm" />
		<property name="motor" ref="um6_motor" />
		<property name="hardwareUnitString" value="mm" />
	</bean>
	<bean id="um7" class="gda.device.scannable.ScannableMotor">
		<property name="protectionLevel" value="3" />
		<property name="initialUserUnits" value="mm" />
		<property name="motor" ref="um7_motor" />
		<property name="hardwareUnitString" value="mm" />
	</bean>
	<bean id="um8" class="gda.device.scannable.ScannableMotor">
		<property name="protectionLevel" value="3" />
		<property name="initialUserUnits" value="mm" />
		<property name="motor" ref="um8_motor" />
		<property name="hardwareUnitString" value="mm" />
	</bean>

	<bean id="um9" class="gda.device.scannable.ScannableMotor">
		<property name="protectionLevel" value="3" />
		<property name="initialUserUnits" value="mm" />
		<property name="motor" ref="um9_motor" />
		<property name="hardwareUnitString" value="mm" />
	</bean>
	<bean id="um10" class="gda.device.scannable.ScannableMotor">
		<property name="protectionLevel" value="3" />
		<property name="initialUserUnits" value="mm" />
		<property name="motor" ref="um10_motor" />
		<property name="hardwareUnitString" value="mm" />
	</bean>

	<bean id="um" class="gda.device.scannable.scannablegroup.ScannableGroup">
		<property name="groupMembers">
			<list>
				<ref bean="um1" />
				<ref bean="um2" />
				<ref bean="um3" />
				<ref bean="um6" />
				<ref bean="um7" />
				<ref bean="um8" />
				<ref bean="um9" />
				<ref bean="um10" />
			</list>
		</property>
	</bean>

	<bean id="frelonfocus" class="gda.device.scannable.ScannableMotor">
		<property name="protectionLevel" value="3" />
		<property name="initialUserUnits" value="mm" />
		<property name="motor" ref="frelon_focus_motor" />
		<property name="hardwareUnitString" value="mm" />
	</bean>

	<bean id="freloniris" class="gda.device.scannable.ScannableMotor">
		<property name="protectionLevel" value="3" />
		<property name="initialUserUnits" value="deg" />
		<property name="motor" ref="frelon_iris_motor" />
		<property name="hardwareUnitString" value="deg" />
	</bean>

	<bean id="frelonrot" class="gda.device.scannable.ScannableMotor">
		<property name="protectionLevel" value="3" />
		<property name="initialUserUnits" value="deg" />
		<property name="motor" ref="frelon_rot_motor" />
		<property name="hardwareUnitString" value="deg" />
	</bean>

</beans>
	