			</list>
		</property>
	</bean>
	
	<!-- rmi exports -->

	<bean class="uk.ac.gda.remoting.server.GdaRmiServiceExporter">
		<property name="serviceName" value="gda/s2_hgap" />
		<property name="service" ref="s2_hgap" />
		<property name="serviceInterface" value="gda.device.ScannableMotionUnits" />
	</bean>  
	<bean class="uk.ac.gda.remoting.server.GdaRmiServiceExporter">
		<property name="serviceName" value="gda/s2_vgap" />
		<property name="service" ref="s2_vgap" />
		<property name="serviceInterface" value="gda.device.ScannableMotionUnits" />
	</bean>  
	<bean class="uk.ac.gda.remoting.server.GdaRmiServiceExporter">
