<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="test_motor" class="gda.device.motor.DummyMotor">
		<property name="speed" value="1000" />
	</bean>

	<bean id="test" class="gda.device.scannable.ScannableMotor">
		<property name="name" value="test" />
		<property name="motor" ref="test_motor" />
	</bean>

	<bean id="topup" class="gda.device.monitor.DummyMonitor">
		<property name="constantValue" value="600.0" />
	</bean>

	<import resource="file:${gda.config}/servers/main/scannables-common.xml" />

</beans>
