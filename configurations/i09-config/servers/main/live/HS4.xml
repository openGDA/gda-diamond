<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="hs4inboard_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="HS4.INBOARD" />
	</bean>
	<bean id="hs4outboard_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="HS4.OUTBOARD" />
	</bean>
	<bean id="hs4top_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="HS4.TOP" />
	</bean>
	<bean id="hs4bottom_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="HS4.BOTTOM" />
	</bean>
	<bean id="hs4xcentre_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="HS4.XCENTRE" />
	</bean>
	<bean id="hs4xgap_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="HS4.XGAP" />
	</bean>
	<bean id="hs4ycentre_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="HS4.YCENTRE" />
	</bean>
	<bean id="hs4ygap_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="HS4.YGAP" />
	</bean>
</beans>
