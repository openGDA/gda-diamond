<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="es2scaler" class="gda.device.epicsdevice.FindableEpicsDevice">
		<property name="recordPVs">
			<map>
				<entry key="RECORD" value="BL09L-VA-SCLR-01" />
			</map>
		</property>
	</bean>
	<bean id="scaler2" class="gda.device.detector.EpicsScalerSimple">
		<property name="epicsDevice" ref="es2scaler" />
		<property name="channelsToBeRead">
			<list>
				<value>2</value>
				<value>3</value>
				<value>4</value>
				<value>5</value>
			</list>
		</property>
		<property name="extraNames" value="hm3amp20,sm5amp8,smpmamp39,rfdamp10" />
		<property name="outputFormat" value="%s,%s,%s,%s" />
	</bean>
	<bean id="hm3amp20" class="gda.device.detector.EpicsScalerSimple">
		<property name="epicsDevice" ref="es2scaler" />
		<property name="channelsToBeRead">
			<list>
				<value>2</value>
			</list>
		</property>
		<property name="extraNames" value="hm3amp20" />
		<property name="outputFormat" value="%s" />
	</bean>
	<bean id="sm5amp8" class="gda.device.detector.EpicsScalerSimple">
		<property name="epicsDevice" ref="es2scaler" />
		<property name="channelsToBeRead">
			<list>
				<value>3</value>
			</list>
		</property>
		<property name="extraNames" value="sm5amp8" />
		<property name="outputFormat" value="%s" />
	</bean>
	<bean id="smpmamp39" class="gda.device.detector.EpicsScalerSimple">
		<property name="epicsDevice" ref="es2scaler" />
		<property name="channelsToBeRead">
			<list>
				<value>4</value>
			</list>
		</property>
		<property name="extraNames" value="smpmamp39" />
		<property name="outputFormat" value="%s" />
	</bean>
	<bean id="rfdamp10" class="gda.device.detector.EpicsScalerSimple">
		<property name="epicsDevice" ref="es2scaler" />
		<property name="channelsToBeRead">
			<list>
				<value>5</value>
			</list>
		</property>
		<property name="extraNames" value="rfdamp10" />
		<property name="outputFormat" value="%s" />
	</bean>
</beans>
