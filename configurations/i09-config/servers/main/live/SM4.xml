<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="sm4iamp16" class="gda.device.currentamplifier.EpicsCurrAmpSingle">
		<property name="deviceName" value="SM4.IAMP16" />
		<property name="poll" value="true"/>
	</bean>

	<bean id="sm4x_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="SM4.X" />
	</bean>
	<bean id="sm4y_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="SM4.Y" />
	</bean>
	<bean id="sm4z_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="SM4.Z" />
	</bean>
	<bean id="sm4pitch_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="SM4.PITCH" />
	</bean>
	<bean id="sm4roll_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="SM4.ROLL" />
	</bean>
	<bean id="sm4yaw_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="SM4.YAW" />
	</bean>

</beans>
