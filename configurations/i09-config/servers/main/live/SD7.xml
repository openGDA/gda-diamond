<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="sd7iamp17" class="gda.device.currentamplifier.EpicsCurrAmpSingle">
		<property name="deviceName" value="SD7.IAMP17"/>
		<property name="poll" value="true"/>
	</bean>
	<bean id="sd7x_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="SD7.X"/>
	</bean>
	<bean id="sd7aposn" class="gda.spring.EpicsPositionerFactoryBean">
		<property name="deviceName" value="SD7.APOSN" />
	</bean>

</beans>
