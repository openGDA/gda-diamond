<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="hs2top_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL09I-MO-HS-02:TOP" />
	</bean>
	<bean id="hs2bottom_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL09I-MO-HS-02:BOTTOM" />
	</bean>
	<bean id="hs2ycentre_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL09I-MO-HS-02:YCENTRE" />
	</bean>
	<bean id="hs2ygap_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL09I-MO-HS-02:YGAP" />
	</bean>
</beans>