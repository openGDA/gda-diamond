<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="sm5x_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="SM5.X" />
	</bean>
	<bean id="sm5y_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="SM5.Y" />
	</bean>
	<bean id="sm5pitch_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="SM5.PITCH" />
	</bean>
	<bean id="sm5bender1_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="SM5.BENDER1" />
	</bean>
	<bean id="sm5bender2_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="SM5.BENDER2" />
	</bean>
	<bean id="sm5iamp8" class="gda.device.currentamplifier.EpicsCurrAmpSingle">
		<property name="deviceName" value="SM5.IAMP8" />
		<property name="poll" value="false"/>
	</bean>

</beans>
