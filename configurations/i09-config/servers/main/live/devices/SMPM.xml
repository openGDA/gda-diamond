<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="smpmiamp39" class="gda.device.currentamplifier.EpicsCurrAmpSingle">
		<property name="poll" value="true" />
		<property name="pvName" value="BL09I-MO-SMPM-01:IAMP39" />
	</bean>

	<bean id="smpmx_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL09I-MO-SMPM-01:X" />
	</bean>
	<bean id="smpmy_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL09I-MO-SMPM-01:Y" />
	</bean>
	<bean id="smpmz_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL09I-MO-SMPM-01:Z" />
	</bean>
	<bean id="smpmpolar_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL09I-MO-SMPM-01:POLAR" />
	</bean>
	<bean id="smpmazimuth_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL09I-MO-SMPM-01:AZIMUTH" />
	</bean>
</beans>