<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="sm5x_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL09J-MO-SM-05:X" />
	</bean>
	<bean id="sm5y_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL09J-MO-SM-05:Y" />
	</bean>
	<bean id="sm5pitch_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL09J-MO-SM-05:PITCH" />
	</bean>
	<bean id="sm5bender1_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL09J-MO-SM-05:BENDER1" />
	</bean>
	<bean id="sm5bender2_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL09J-MO-SM-05:BENDER2" />
	</bean>
	
	<bean id="sm5strain1" class="gda.device.monitor.EpicsMonitor">
		<property name="pvName" value="BL09J-MO-SM-05:STRAIN1"/>
		<property name="unit" value="N"/>
	</bean>
	
	<bean id="sm5strain2" class="gda.device.monitor.EpicsMonitor">
		<property name="pvName" value="BL09J-MO-SM-05:STRAIN2"/>
		<property name="unit" value="N"/>
	</bean>

	<import resource="file://${gda.config.shared}/servers/soft/live/devices/SM5.xml"/>
</beans>