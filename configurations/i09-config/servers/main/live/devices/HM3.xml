<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="hm3iamp20" class="gda.device.currentamplifier.EpicsCurrAmpSingle">
		<property name="poll" value="false" />
		<property name="pvName" value="BL09I-MO-HM-03:IAMP20" />
	</bean>
	<bean id="hm3y_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL09I-MO-HM-03:Y" />
	</bean>
	<bean id="hm3x_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL09I-MO-HM-03:X" />
	</bean>
	<bean id="hm3pitch_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL09I-MO-HM-03:PITCH" />
	</bean>
	<bean id="hm3upstreamx_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL09I-MO-HM-03:UPSTREAMX" />
	</bean>
	<bean id="hm3downstreamx_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL09I-MO-HM-03:DOWNSTREAMX" />
	</bean>
	<bean id="hm3mainbender_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL09I-MO-HM-03:BENDER1" />
	</bean>
	<bean id="hm3elipticalbender_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL09I-MO-HM-03:BENDER2" />
	</bean>
</beans>