<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="sm4iamp16" class="gda.device.currentamplifier.EpicsCurrAmpSingle">
		<property name="poll" value="true" />
		<property name="pvName" value="BL09J-MO-SM-04:IAMP16" />
	</bean>

	<bean id="sm4x_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL09J-MO-SM-04:X" />
	</bean>
	<bean id="sm4y_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL09J-MO-SM-04:Y" />
	</bean>
	<bean id="sm4z_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL09J-MO-SM-04:Z" />
	</bean>
	<bean id="sm4pitch_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL09J-MO-SM-04:PITCH" />
	</bean>
	<bean id="sm4roll_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL09J-MO-SM-04:ROLL" />
	</bean>
	<bean id="sm4yaw_motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL09J-MO-SM-04:YAW" />
	</bean>

</beans>