	<bean id="sm1fpitch_motor" class="gda.device.motor.DummyMotor">
		<property name="fastSpeed" value="400" />
		<property name="mediumSpeed" value="200" />
		<property name="slowSpeed" value="100" />
	</bean>
	
	<bean id="sm1fpitchfeedback" class="gda.device.enumpositioner.DummyEnumPositioner">
		<property name="positions">
			<list>
				<value>Enabled</value>
				<value>Disabled</value>
			</list>
		</property>
	</bean>
</beans>
