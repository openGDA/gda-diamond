		<property name="slowSpeed" value="100" />
	</bean>
	<bean id="sm1y_motor" class="gda.device.motor.DummyMotor">
		<property name="fastSpeed" value="400" />
		<property name="mediumSpeed" value="200" />
		<property name="slowSpeed" value="100" />
	</bean>
	<bean id="sm1z_motor" class="gda.device.motor.DummyMotor">
		<property name="fastSpeed" value="400" />
		<property name="mediumSpeed" value="200" />
		<property name="slowSpeed" value="100" />
	</bean>
	<bean id="sm1pitch_motor" class="gda.device.motor.DummyMotor">
		<property name="fastSpeed" value="400" />
		<property name="mediumSpeed" value="200" />
		<property name="slowSpeed" value="100" />
	</bean>
	<bean id="sm1roll_motor" class="gda.device.motor.DummyMotor">
		<property name="fastSpeed" value="400" />
