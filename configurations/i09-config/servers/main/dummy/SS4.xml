	</bean>
	<bean id="ss4y_motor" class="gda.device.motor.DummyMotor">
		<property name="fastSpeed" value="400" />
		<property name="mediumSpeed" value="200" />
		<property name="slowSpeed" value="100" />
	</bean>
	<bean id="ss4z_motor" class="gda.device.motor.DummyMotor">
