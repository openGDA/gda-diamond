<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
		
	<bean id="rs_fill" class="gda.device.scannable.DummyScannable">
	</bean>
	
	<bean id="rs_lifetime" class="gda.device.scannable.DummyScannable">
		<property name="outputFormat" value="%1.3f" />
	</bean>
	
	<bean id="rs_countdown" class="gda.device.scannable.DummyScannable">
		<property name="outputFormat" value="%1.3f" />
	</bean>
	
	<bean id="rs_message" class="gda.device.scannable.DummyScannable">
		<property name="outputFormat" value="%1.3f" />
	</bean>
</beans>