<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="femX_Motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL24B-MO-FEM-01:X" />
	</bean>
	<bean id="femXd_Motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL24B-MO-FEM-01:DOWNSTREAMX"/>
	</bean>
	<bean id="femXu_Motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL24B-MO-FEM-01:UPSTREAMX"/>
	</bean>
	
	<bean id="femY_Motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL24B-MO-FEM-01:Y" />
	</bean>
	<bean id="femYu_Motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL24B-MO-FEM-01:UPSTREAMY" />
	</bean>
	<bean id="femYd_Motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL24B-MO-FEM-01:DOWNSTREAMY" />
	</bean>
	<bean id="femYi_Motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL24B-MO-FEM-01:INBOARDY" />
	</bean>
	<bean id="femYo_Motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL24B-MO-FEM-01:OUTBOARDY" />
	</bean>
	
	<bean id="femYaw_Motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL24B-MO-FEM-01:YAW" />
	</bean>
	<bean id="femRoll_Motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL24B-MO-FEM-01:ROLL" />
	</bean>
	<bean id="femPitch_Motor" class="gda.device.motor.EpicsMotor">
		<property name="pvName" value="BL24B-MO-FEM-01:PITCH" />
	</bean>

</beans>
