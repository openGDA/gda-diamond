<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="regiondefinition" nsURI="http://www.opengda.org/regiondefinition"
    nsPrefix="">
  <eClassifiers xsi:type="ecore:EClass" name="DocumentRoot">
    <eStructuralFeatures xsi:type="ecore:EReference" name="sequence" eType="#//Sequence"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Sequence">
    <eStructuralFeatures xsi:type="ecore:EReference" name="region" upperBound="-1"
        eType="#//Region" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Region">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="lensmode" eType="#//LENS_MODE"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="passEnergy" eType="#//PASS_ENERGY"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="runMode" eType="#//RunMode"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="acquisitionMode" eType="#//ACQUIAITION_MODE"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="energyMode" eType="#//ENERGY_MODE"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="energy" eType="#//Energy"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="LENS_MODE">
    <eLiterals name="TRANSMISSION"/>
    <eLiterals name="Angular45" value="1"/>
    <eLiterals name="Angular60" value="2"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="RunMode">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="mode" eType="#//RUN_MODES"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="numIterations" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="repeatUnitilStopped" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="RUN_MODES">
    <eLiterals name="Normal"/>
    <eLiterals name="ADD_DIMENSION" value="1"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="PASS_ENERGY">
    <eLiterals name="5"/>
    <eLiterals name="10" value="1"/>
    <eLiterals name="20" value="2"/>
    <eLiterals name="50" value="3" literal=""/>
    <eLiterals name="70" value="4"/>
    <eLiterals name="100" value="5"/>
    <eLiterals name="200" value="6"/>
    <eLiterals name="500" value="7"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="ACQUIAITION_MODE">
    <eLiterals name="SWEPT"/>
    <eLiterals name="FIXED" value="1"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="ENERGY_MODE">
    <eLiterals name="KINETIC"/>
    <eLiterals name="BINDING" value="1"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Energy">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="low" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="high" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="center" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="width" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Step">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="frames" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="time" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="size" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="totalTime" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="totalSteps" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Detector">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="firstXChannel" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="lastXChannel" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="firstYChannel" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="lastYChannel" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="slices" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="detectorMode" eType="#//DETECTOR_MODE"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="DETECTOR_MODE">
    <eLiterals name="ADC"/>
    <eLiterals name="PULSE_COUNTING" value="1"/>
  </eClassifiers>
</ecore:EPackage>
