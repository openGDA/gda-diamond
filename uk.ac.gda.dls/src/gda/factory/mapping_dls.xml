	<class identity="name" name="gda.images.camera.DummyOpticalCamera">
		<map-to xml="DummyOpticalCamera" />
		<field name="name" type="string">
			<bind-xml name="name" node="element" />
		</field>
	</class>

	<class identity="name" name="gda.images.camera.FirewireCamera">
		<map-to xml="FirewireCamera" />
		<field name="name" type="string">
			<bind-xml name="name" node="element" />
		</field>
		<field name="imageFile" type="string">
			<bind-xml name="imageFile" node="element" />
		</field>
		<field name="serverPort" type="integer">
			<bind-xml name="serverPort" node="element" />
		</field>
		<field name="zoomMotorName" type="string">
			<bind-xml name="zoomMotorName" node="element" />
		</field>
		<field name="focusMotorName" type="string">
			<bind-xml name="focusMotorName" node="element" />
		</field>
		<field collection="collection" get-method="getZoomPositionList" name="zoomPositionList" set-method="addZoomPosition" type="string">
			<bind-xml name="zoomPosition" node="element" />
		</field>
		<field collection="collection" get-method="getFocusPositionList" name="focusPositionList" set-method="addFocusPosition" type="string">
			<bind-xml name="focusPosition" node="element" />
		</field>
	</class>


	<class extends="gda.gui.AcquisitionPanel" identity="name" name="gda.images.GUI.CmuCameraDisplayPanel">
		<map-to xml="CmuCameraDisplayPanel" />
	</class>
	<class extends="gda.device.scannable.ScannableBase" name="gda.device.scannable.keyence.Keyence">
		<map-to xml="Keyence" />
		<field collection="collection" get-method="getStartupCommands" name="startupCommands" set-method="setStartupCommands" type="string">
			<bind-xml name="startupCommand" node="element" />
		</field>
		<field name="host" type="string">
			<bind-xml name="host" node="element" />
		</field>
		<field name="port" type="integer">
			<bind-xml name="port" node="element" />
		</field>
	</class>
</mapping>