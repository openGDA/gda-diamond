<?xml version="1.0" ?>
<mapping>
	<!-- general objects -->
	<class name="gda.factory.ObjectFactory">
		<map-to xml="ObjectFactory"/>
		<field name="name" type="java.lang.String">
			<bind-xml name="name" node="element"/>
		</field>
		<field collection="collection" get-method="getFindables" name="findables" set-method="addFindable" type="gda.factory.Findable">
			<bind-xml name="findables" node="element"/>
		</field>
	</class>
	<class identity="name" name="gda.device.DeviceBase">
		<map-to xml="deviceBase"/>
		<field name="name" type="string">
			<bind-xml name="name" node="element"/>
		</field>
		<field name="protectionLevel" type="integer">
			<bind-xml name="protectionLevel" node="element"/>
		</field>
		<field name="local" type="boolean">
			<bind-xml name="local" node="element"/>
		</field>
		<field name="configureAtStartup" type="boolean">
			<bind-xml name="configureAtStartup" node="element"/>
		</field>
	</class>
	<class identity="name" name="gda.gui.AcquisitionPanel">
		<field name="name" type="string">
			<bind-xml name="name" node="element" />
		</field>
		<field name="InitMode" type="string">
			<bind-xml name="InitMode" node="element" />
		</field>
		<field name="VerticalScrollBarPolicy" type="string">
			<bind-xml name="VerticalScrollBarPolicy" node="element" />
		</field>
		<field name="HorizontalScrollBarPolicy" type="string">
			<bind-xml name="HorizontalScrollBarPolicy" node="element" />
		</field>
	</class>
	

	<class extends="gda.gui.AcquisitionPanel" identity="name"
		name="gda.gui.microfocus.camera.CameraDisplayPanel">
		<map-to xml="CameraDisplayPanel" />
		<field name="xConverterName" type="string">
			<bind-xml name="xConverterName" node="element" />
		</field>
		<field name="yConverterName" type="string">
			<bind-xml name="yConverterName" node="element" />
		</field>
		<field name="cameraOEName" type="string">
			<bind-xml name="cameraOEName" node="element" />
		</field>
		<field name="zoomMotorName" type="string">
			<bind-xml name="zoomMotorName" node="element" />
		</field>
		<field name="focusMotorName" type="string">
			<bind-xml name="focusMotorName" node="element" />
		</field>
		<field name="videoReceiverName" type="string">
			<bind-xml name="videoReceiverName" node="element" />
		</field>
		
	</class>
	<class identity="name" name="gda.gui.microfocus.camera.PixelToMMConverter">
		<map-to xml="PixelToMMConverter" />
		<field name="name" type="string">
			<bind-xml name="name" node="element" />
		</field>
	</class>
	<class identity="name" name="gda.images.camera.RTPStreamReceiver">
		<map-to xml="RTPStreamReceiver" />
		<field name="name" type="string">
			<bind-xml name="name" node="element" />
		</field>
		<field name="local" type="boolean">
			<bind-xml name="local" node="element" />
		</field>
		<field name="port" type="integer">
			<bind-xml name="port" node="element" />
		</field>
		<field name="host" type="string">
			<bind-xml name="host" node="element" />
		</field>
	</class>
	<class identity="name" name="gda.gui.polarimetry.PolarimeterMotorSetupPanel">
		<map-to xml="PolarimeterMotorSetupPanel"/>
