		<field name="rearPinholeMotorName" type="string">
			<bind-xml name="rearPinholeMotorName" node="element"/>
		</field>
		<field name="retarderTiltDofName" type="string">
			<bind-xml name="retarderTiltDofName" node="element"/>
		</field>
		<field name="retarderRotationDofName" type="string">
			<bind-xml name="retarderRotationDofName" node="element"/>
		</field>
		<field name="analyserTiltDofName" type="string">
			<bind-xml name="analyserTiltDofName" node="element"/>
		</field>
		<field name="analyserRotationDofName" type="string">
			<bind-xml name="analyserRotationDofName" node="element"/>
		</field>
		<field name="analyserTranslationDofName" type="string">
			<bind-xml name="analyserTranslationDofName" node="element"/>
		</field>
		<field name="analyserDetectorDofName" type="string">
			<bind-xml name="analyserDetectorDofName" node="element"/>
		</field>
	</class>
	<class extends="gda.gui.AcquisitionPanel" identity="name" name="gda.gui.generalscan.GeneralScanPanel">
		<map-to xml="GeneralScanPanel"/>
		<field name="beamMonitorName" type="string">
			<bind-xml name="beamMonitorName" node="element"/>
		</field>
		<field name="beamMonitorChannel" type="integer">
			<bind-xml name="beamMonitorChannel" node="element"/>
		</field>
		<field name="beamMonitorThreshold" type="double">
			<bind-xml name="beamMonitorThreshold" node="element"/>
		</field>
		<field name="beamMonitorWaitTime" type="integer">
			<bind-xml name="beamMonitorWaitTime" node="element"/>
		</field>
		<field name="beamMonitorCountTime" type="double">
			<bind-xml name="beamMonitorCountTime" node="element"/>
		</field>
		<field name="beamMonitorConsecutiveCountsAboveThreshold" type="integer">
			<bind-xml name="beamMonitorConsecutiveCountsAboveThreshold" node="element"/>
		</field>
	</class>
	<class extends="gda.gui.AcquisitionPanel" identity="name" name="gda.gui.generalscan.GeneralOptimizePanel">
		<map-to xml="GeneralOptimizePanel"/>
		<field collection="collection" get-method="getDisplayLineList" name="displayLineList" set-method="addDisplayLine" type="string">
			<bind-xml name="displayLine" node="element"/>
		</field>
		<field name="lineToMaximize" type="string">
			<bind-xml name="lineToMaximize" node="element"/>
		</field>
		<field name="oeName" type="string">
			<bind-xml name="oeName" node="element"/>
		</field>
		<field name="dofName" type="string">
			<bind-xml name="dofName" node="element"/>
		</field>
		<field name="increment" type="double">
			<bind-xml name="increment" node="element"/>
		</field>
		<field name="increments" type="integer">
			<bind-xml name="increments" node="element"/>
		</field>
		<field name="time" type="double">
			<bind-xml name="time" node="element"/>
		</field>
		<field name="units" type="string">
			<bind-xml name="units" node="element"/>
		</field>
		<field name="preScanSetupExecutorClass" type="string">
			<bind-xml name="preScanSetupExecutorClass" node="element"/>
		</field>
	</class>
	<class extends="gda.gui.AcquisitionPanel" identity="name" name="gda.gui.generalscan.ConfigurableExperimentPanel">
		<map-to xml="ConfigurableExperimentPanel"/>
		<field name="beamMonitorName" type="string">
			<bind-xml name="beamMonitorName" node="element"/>
		</field>
		<field name="beamMonitorChannel" type="integer">
			<bind-xml name="beamMonitorChannel" node="element"/>
		</field>
		<field name="beamMonitorThreshold" type="double">
			<bind-xml name="beamMonitorThreshold" node="element"/>
		</field>
		<field name="beamMonitorWaitTime" type="integer">
			<bind-xml name="beamMonitorWaitTime" node="element"/>
		</field>
		<field name="beamMonitorCountTime" type="double">
			<bind-xml name="beamMonitorCountTime" node="element"/>
		</field>
		<field name="beamMonitorConsecutiveCountsAboveThreshold" type="integer">
			<bind-xml name="beamMonitorConsecutiveCountsAboveThreshold" node="element"/>
		</field>
		<field collection="collection" get-method="getOeNames" name="oeNames" set-method="addOeName" type="string">
			<bind-xml name="oeNames" node="element"/>
		</field>
		<field collection="collection" get-method="getDofNames" name="dofNames" set-method="addDofName" type="string">
			<bind-xml name="dofNames" node="element"/>
		</field>
		<field name="addDimensionEnabled" type="boolean">
			<bind-xml name="addDimensionEnabled" node="element"/>
		</field>
		<field name="addRegionEnabled" type="boolean">
			<bind-xml name="addRegionEnabled" node="element"/>
		</field>
	</class>

	<class extends="gda.gui.generalscan.GeneralScanPanel" identity="name" name="gda.gui.polarimetry.PolarimeterScanPanel">
		<map-to table="panel_polarimeterscan" xml="PolarimeterScanPanel"/>
	</class>
	<class identity="name" name="gda.gui.polarimetry.NI6602CounterPanel">
		<map-to xml="NI6602CounterPanel" />
		<field name="name" type="string">
			<bind-xml name="name" node="element" />
