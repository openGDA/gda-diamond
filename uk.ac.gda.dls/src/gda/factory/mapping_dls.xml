		<field name="retarderName" type="string">
			<bind-xml name="retarderName" node="element"/>
		</field>
		<field name="analyserName" type="string">
			<bind-xml name="analyserName" node="element"/>
		</field>
		<field name="retarderTiltMotorName" type="string">
			<bind-xml name="retarderTiltMotorName" node="element"/>
		</field>
		<field name="retarderRotationMotorName" type="string">
			<bind-xml name="retarderRotationMotorName" node="element"/>
		</field>
		<field name="analyserTiltMotorName" type="string">
			<bind-xml name="analyserTiltMotorName" node="element"/>
		</field>
		<field name="analyserRotationMotorName" type="string">
			<bind-xml name="analyserRotationMotorName" node="element"/>
		</field>
		<field name="analyserTranslationMotorName" type="string">
			<bind-xml name="analyserTranslationMotorName" node="element"/>
		</field>
		<field name="analyserDetectorMotorName" type="string">
