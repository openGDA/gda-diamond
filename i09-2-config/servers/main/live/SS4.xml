<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="ss4xgap_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="SS4.XGAP" />
	</bean>
	<bean id="ss4ygap_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="SS4.YGAP" />
	</bean>

	<bean id="ss4z_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="SS4.Z" />
	</bean>

</beans>
