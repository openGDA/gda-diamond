<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="sm3x_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="SM3.X" />
	</bean>
	<bean id="sm3y_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="SM3.Y" />
	</bean>
	<bean id="sm3z_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="SM3.Z" />
	</bean>
	<bean id="sm3pitch_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="SM3.PITCH" />
	</bean>
	<bean id="sm3roll_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="SM3.ROLL" />
	</bean>
	<bean id="sm3yaw_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="SM3.YAW" />
	</bean>
	<bean id="sm3fpitch_motor" class="gda.device.motor.PiezoEpicsMotor">
		<property name="deviceName" value="SM3.FPITCH" />
	</bean>
</beans>
