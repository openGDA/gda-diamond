                  </equals>
               </with>
            </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="toolbar:uk.ac.diamond.daq.beamline.i06-2.camc2.live.stream.view.LiveStreamViewWithHistogram?before=uk.ac.gda.client.live.stream.view.reset ">
